#include <stdio.h>
#include <string.h>
int main() {
  char num[100];
  int cnt[10] = {0};
  int i;
  printf("Please enter the number:\n");
  scanf("%s", num);
  if (num[0] == '-')
    printf("%s: %d bits\n", num, strlen(num) - 1);
  else
    printf("%s: %d bits\n", num, strlen(num));
  for (i = 0; num[i]; i++)
    if (num[i] <= '9' && num[i] >= '0')
      cnt[num[i] - '0']++;

  for (i = 0; i < 10; i++)
    if (cnt[i])
      printf("%d: %d\n", i, cnt[i]);
}

