#include <stdio.h>
#define SIZE 10
int main() {
  int i, j, t, a[SIZE];
  printf("input  10  numbers: \n");
  for (i = 0; i < SIZE; i++)
    scanf("%d", &a[i]);
  printf("\n");
  for (i = 0; i < SIZE - 1; i++) {
    for (j = 0; j < (SIZE - 1 - i); j++)
      if (a[j + 1] < a[j]) {
        t = a[j + 1];
        a[j + 1] = a[j];
        a[j] = t;
      }
  }
  for (i = 0; i < SIZE; i++)
    printf("%d\n", a[i]);
  return 0;
}
