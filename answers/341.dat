#include <math.h>
#include <stdio.h>
#include <stdlib.h>
int main() {
  long int number;
  int n, m, i;
  printf("Please input number");
  scanf("%ld", &number);
  for (i = 1; i <= sqrt(number); i++) {
    if (number % i == 0)
      n = i;
  }
  m = number / n;
  printf("The max factor with 3 digits in %ld is %d.", number, n > m ? n : m);
  return 0;
}

