#include <math.h>
#include <stdio.h>
int main() {
  long int num;
  int i;
  printf("Please input number");
  scanf("%ld", &num);
  for (i = 999; i >= 100; i--) {
    if (num % i == 0) {
      printf("The max factor with 3 digits in %ld is %d.\n", num, i);
      break;
    }
  }
  return 0;
}

