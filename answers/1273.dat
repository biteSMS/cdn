#include <stdio.h>

void Cpy(char s[80], char c[80]);
int main() {
  char str1[80], str2[80];

  printf("输入一个字符串\n");
  gets(str1);

  Cpy(str1, str2);

  printf("输出一个字符串\n");
  puts(str2);

  return 0;
}
void Cpy(char s[80], char c[80]) {
  int i, j = 0;

  for (i = 0; s[i] != '\0'; i++) {
    if (s[i] == 'a' || s[i] == 'A' || s[i] == 'e' || s[i] == 'E' ||
        s[i] == 'i' || s[i] == 'I' || s[i] == 'o' || s[i] == 'O' ||
        s[i] == 'u' || s[i] == 'U') {
      c[j] = s[i];
      j++;
    }
  }
  c[j] = '\0';
}

