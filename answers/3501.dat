#include <math.h>
#include <stdio.h>
int main() {
  int num, i, find = 0;
  printf("请输入一个大于2的数:\n");
  scanf("%d", &num);
  for (i = 2; i <= sqrt(num); i++) {
    if (num % i == 0) {
      find = 1;
    }
  }
  if (find)
    printf("%d不是素数", num);
  else
    printf("%d是素数", num);
  return 0;
}

