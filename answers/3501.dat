#include <stdio.h>
#include <stdlib.h>

int susu(int x) {
  int susu = 1, i = 0;
  for (i = 2; i < x && susu; i++) {
    if (x % i == 0)
      susu = 0;
  }

  return susu;
}

int main() {
  int in = 0;
  printf("请输入一个大于2的数:\n");
  scanf("%d", &in);
  printf("%d%s", in, susu(in) ? "是素数" : "不是素数");
  return 0;
}

