main() {
  int a[M] = {1, 2, 3, 4, 5};
  int *p = a, *q = a + M - 1, t;
  while (p != q) {
    t = *p;
    *p = *q;
    *q = t;
    p++;
    q--;
  }
  for (t = 0; t < M; t++)
    printf("%d", a[t]);
}

