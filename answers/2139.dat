#include <stdio.h>

int Full(int n);

int main() {
  int i;
  for (i = 1; i <= 1000; i++) {
    if (Full(i))
      printf("%d\n", i);
  }
  return 0;
}

int Full(int n) {
  int i, sum = 0;
  for (i = 1; i < n; i++) {
    if (n % i == 0)
      sum += i;
  }
  if (sum == n)
    return 1;
  else
    return 0;
}

