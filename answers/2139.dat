#include <stdio.h>
#include <stdlib.h>

int main() {
  int i, j, sum, u = 0, k;
  int a[1000];
  for (i = 2; i <= 1000; i++) {
    sum = 0;
    for (j = 1; j < i; j++) {
      if (i % j == 0) {
        sum = sum + j;
      }
    }
    if (sum == i) {
      a[u] = i;
      u++;
    }
  }
  for (k = 0; k < u; k++) {
    printf("%d\n", a[k]);
  }
}

