#include <stdio.h>
main() {
  int i, sum = 0;
  for (i = 1; i <= 100; i++) {
    if (i % 7 == 0 && i % 3 != 0 && i % 2 == 0) {
      sum += i;
      printf("%5d", i);
    }
  }
  printf("\nsum=%d\n", sum);
}

