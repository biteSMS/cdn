#include <stdio.h>
main() {
  int a = 14, b = 28, c = 56, d = 70, e = 98;
  int i;
  int Sum = 0;
  for (i = 1; i <= 100 / 14; i++) {
    if ((i * 14) % 3 != 0) {
      Sum += (i * 14);
    }
  }
  printf("%5d", a);
  printf("%5d", b);
  printf("%5d", c);
  printf("%5d", d);
  printf("%5d", e);
  printf("\nsum=%d\n", Sum);
}

