#include <stdio.h>
int f(int a, int b, int n) {
  b += a * a;
  if (b >= n)
    return a - 1;
  else
    return f(++a, b, n);
}
int main() {
  int n = 1000;
  int b = 0;
  int a = 0;
  printf("n=%d\n", f(a, b, n));
  return 0;
}

