#include <stdio.h>
void main() {
  int a[10], n, max, maxpos;
  printf("请输入10个元素的值：\n");
  for (n = 0; n < 10; n++) {
    scanf("%d", &a[n]);
  }
  max = a[0];
  maxpos = 0;
  for (n = 0; n < 10; n++) {
    if (a[n] > max) {
      max = a[n];
      maxpos = n;
    }
  }
  printf("值最大的元素值为：%d  下标为%d\n", max, maxpos);
}

