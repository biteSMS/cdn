#include <stdio.h>
int main() {
  int num[10], i, k = 0;
  printf("请输入10个元素的值：\n");
  for (i = 0; i < 10; i++) {
    scanf("%d", &num[i]);
  }

  for (i = 0; i < 10; i++) {
    if (num[k] < num[i])
      k = i;
  }
  printf("值最大的元素值为：%d  下标为%d\n", num[k], k);

  return 0;
}

