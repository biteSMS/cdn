#include <stdio.h>
int main() {
  float x, y;
  printf("input x:\n");
  scanf("%f", &x);
  if (x >= -7 && x <= 10) {
    printf("y=%.3f", (5 * x * x) - (4 * x) + 6);
  } else {
    printf("y=%.3f", (float)1 / 3 * x + 32);
  }
  return 0;
}

