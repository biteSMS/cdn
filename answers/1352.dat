#include <stdio.h>
int check(int year, int month, int day) {
  int m[][12] = {{31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31},
                 {31, 29, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31}};
  int flag;
  flag = year % 4 == 0 && year % 100 != 0 || year % 400 == 0;
  if (flag) {
    if (day > m[1][month - 1])
      return 0;
  } else {
    if (day > m[0][month - 1])
      return 0;
  }
  return 1;
}
int main() {
  int n, age, flag;
  char s;
  int year, month, day;
  printf("Please input your busiest day.\n");
  scanf("%d", &n);
  if (n < 1 || n > 7) {
    printf("Invalid input.\n");
    return 0;
  }
  n *= 2;
  printf("Please input your gender.\n");
  scanf(" %c", &s);
  if (s == 'm' || s == 'f') {
    if (s == 'm')
      n += 4;
    else
      n += 8;
  } else {
    printf("Invalid input.\n");
    return 0;
  }
  n *= 50;
  printf("Please input your birthday.\n");
  scanf("%4d-%2d-%2d", &year, &month, &day);
  flag = check(year, month, day);
  if (!flag) {
    printf("Invalid input.\n");
    return 0;
  }
  if (month < 7)
    n += 2002;
  else
    n += 3002;
  n -= year;
  age = n % 100;
  printf("Your age is:%d\n", age + 10);
  return 0;
}


