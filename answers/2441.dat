#include <stdlib.h>

int range_test(int val, int low, int high);
int main() {
  int val, low, high;
  int temp;

  printf("请输入数值和上下界：\n");
  scanf("%d%d%d", &val, &low, &high);

  if (low > high) {
    temp = low;
    low = high;
    high = temp;
  }
  printf("函数测试输出为%d！\n", range_test(val, low, high));

  return 0;
}
int range_test(int val, int low, int high) {
  int flag = 0;

  if (low <= val && val <= high) {
    flag = 1;
  }

  return flag;
}

