#include <stdio.h>
#include <string.h>
void sort(char c[][20], int n);
int main(void) {
  int n, i;
  char a[10][20];
  printf("请输入n(n<10):");

  scanf("%d", &n);
  printf("请输入%d个字符串", n);
  getchar();
  for (i = 0; i < n; i++) {

    gets(a[i]);
  }
  sort(a, n);
  for (i = 0; i < n; i++) {
    puts(a[i]);
  }
  return 0;
}
void sort(char c[][20], int n) {
  char temp[20];
  int i, j, k;
  for (i = 0; i < n - 1; i++) {
    k = i; //开始一趟选择排序，假定第i个元素是后面n - i +
           //1个未排序的元素中最小的元素
    for (j = i + 1; j < n; j++)
      if (strcmp(c[k], c[j]) >
          0) //如果发现比当前最小元素还小的元素，则更新记录最小元素的下标d
        k = j;

    if (k !=
        i) //如果最小元素的下标不是后面n - i +
           //1的未排序序列的第一个元素，则需要交换第i个元素和后面找到的最小元素的位置
    {
      strcpy(temp, c[k]);
      strcpy(c[k], c[i]);
      strcpy(c[i], temp);
    }
  }
}


