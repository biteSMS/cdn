#include <stdio.h>
#include <stdlib.h>

int main() {
  int a, b;

  for (a = 32; a < 100; a++) {
    b = a * a;
    if (b / 1000 == b / 100 % 10 && b % 10 == b / 10 % 10 &&
        b / 1000 != b % 10) {
      printf("k=%d, m=%d\n", b, a);
    }
  }
  return 0;
}

