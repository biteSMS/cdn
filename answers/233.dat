#include <stdio.h>

main() {
  int a, b, n, i; // a是前两位的数字，b是后两位的数字
  for (a = 0; a <= 9; ++a) {
    for (b = 0; b <= 9; ++b) {
      n = a * 1100 + b * 11; //因为前两位和后两位是相同的
      for (i = 1; i <= 100; ++i) {
        if (n == i * i) //如果这4位车牌是i的平方，则满足条件
        {
          printf("k=%d, m=%d\n", n, i); //输出
        }
      }
    }
  }
}

