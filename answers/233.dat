#include <stdio.h>
#include <stdlib.h>
int main() {
  int m, n, a, b, c, d;
  for (m = 1000; m <= 9999; m++) {
    n = sqrt(m);
    a = m / 1000;
    b = m / 100 - a * 10;
    c = m % 100 / 10;
    d = m % 10;
    if (m == n * n && a == b && c == d && b != c)
      printf("k=%d, m=%d\n", m, n);
  }
}

