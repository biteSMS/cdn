#include <stdio.h>
#define N 10
int main() {
  int a[N] = {2, 17, 8, 3, 24, 53, 82, 1, 29, 101};
  int i, j, t;
  for (i = 1; i < 10; i++)
    for (j = 0; j < 10 - i; j++) {
      if (a[j] < a[j + 1]) {
        t = a[j];
        a[j] = a[j + 1];
        a[j + 1] = t;
      }
    }
  for (i = 0; i < 10; i++)
    printf("%d ", a[i]);
  printf("\n");
  return 0;
}


