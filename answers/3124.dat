#include <stdio.h>
int main() {
  int x;
  float y, z, price = 0;
  scanf("%d,%f", &x, &y);
  z = (int)y;
  if (x == 0) {
    if (z - y == 0)
      price = 10 + (z - 1) * 3;
    else
      price = 10 + z * 3;
  } else if (x == 1) {
    if (z - y == 0)
      price = 10 + (z - 1) * 4;
    else
      price = 10 + z * 4;
  } else if (x == 2) {
    if (z - y == 0)
      price = 15 + (z - 1) * 5;
    else
      price = 15 + z * 5;
  } else if (x == 3) {
    if (z - y == 0)
      price = 15 + (z - 1) * 6.5;
    else
      price = 15 + z * 6.5;
  } else if (x == 4) {
    if (z - y == 0)
      price = 15 + (z - 1) * 10;
    else
      price = 15 + z * 10;
  } else {
    printf("Error in Area\n");
  }
  printf("Price: %5.2f\n", price);
}


