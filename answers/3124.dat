#include <stdlib.h>
int main() {
  int area;
  float i, price;
  scanf("%d,%f", &area, &i);
  if (area == 0) {
    price = 10 + (int)i * 3;
    printf("Price: %5.2f\n", price);
  } else if (area == 1) {
    price = 10 + (int)i * 4;
    printf("Price: %5.2f\n", price);
  } else if (area == 2) {
    price = 15 + (int)i * 5;
    printf("Price: %5.2f\n", price);
  } else if (area == 3) {
    price = 15 + (int)i * 6.5;
    printf("Price: %5.2f\n", price);
  } else if (area == 4) {
    price = 15 + (int)i * 10;
    printf("Price: %5.2f\n", price);
  } else
    printf("Error in Area\n");
