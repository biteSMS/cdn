#include <stdio.h>
int main() {
  int area;
  float kg;
  scanf("%d,%f", &area, &kg);
  float price;
  if (kg != (int)kg) {
    kg = ((int)kg);
  }
  if (area > 4 || kg < 0) {
    printf("Error in Area\n");
    price = 0;
  } else {
    if (area == 0) {
      price = 10 + 3 * kg;
    } else if (area == 1) {
      price = 10 + 4 * kg;
    } else if (area == 2) {
      price = 15 + 5 * kg;
    } else if (area == 4) {
      price = 15 + 10 * kg;
    } else {
      price = 15 + 6.5 * kg;
    }
  }
  printf("Price: %5.2f\n", price);
  return 0;
}

