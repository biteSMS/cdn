#include "stdio.h"
int main() {
  int ar;
  double we, mo, sjwe;
  mo = 0;
  double jc[5] = {10, 10, 15, 15, 15}, xz[5] = {3, 4, 5, 6.5, 10};
  scanf("%d,%lf", &ar, &we);
  if (ar < 0 || ar > 4) {
    printf("Error in Area\nPrice:  0.00\n");
    return 0;
  }
  sjwe = (int)we;
  if (sjwe < we)
    sjwe++;
  mo = jc[ar];
  if (we > 1)
    mo += (sjwe - 1) * xz[ar];
  printf("Price: %5.2lf\n", mo);
}

