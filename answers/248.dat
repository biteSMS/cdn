#include <stdio.h>
int main() {
  int a[12], total = 0, i;
  for (i = 0; i < 12; i++) {
    a[i] = Fun(i + 1);
    printf("%4d", a[i]);
  }
  printf("\nTotal = %d\n", a[i - 1]);
}
int Fun(int n) {
  int i;
  for (i = 1; i <= n; i++) {
    if (n == 1) {
      return 1;
    } else if (n == 2) {
      return 2;
    } else
      return Fun(n - 1) + Fun(n - 2);
  }
}

