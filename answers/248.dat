#include <stdio.h>
#include <stdlib.h>

int fib(int m) {
  if (m == 0)
    return 1;
  if (m == 1)
    return 1;
  else
    return fib(m - 1) + fib(m - 2);
}
int main() {
  int i;
  for (i = 1; i < 13; i++) {
    printf("%4d", fib(i));
  }
  printf("\nTotal = %d\n", fib(12));

  return 0;
}

