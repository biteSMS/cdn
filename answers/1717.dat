#include <ctype.h>
#include <stdio.h>
int main() {
  printf("Press a key and then press Enter:");
  char ch = '\0';
  scanf("%c", &ch);
  int i = 0;
  if (i = isalpha(ch)) {
    switch (i) {
    case 1:
      printf("%c, %d\n", tolower(ch), tolower(ch));
      break;
    case 2:
      printf("%c, %d\n", toupper(ch), toupper(ch));
      break;
    }
  } else {
    printf("Input error!");
  }

  return 0;
}

