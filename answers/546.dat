#include <stdio.h>
#include <string.h>
int main() {
  char str[150][50], s[20];
  int i, j, k, n;
  printf("How many countries?");
  scanf("%d", &n);
  printf("Input their names:\n");
  for (i = 0; i < n; i++) {
    scanf("%s", &str[i]);
  }
  for (i = 0; i < n - 1; i++) {
    k = i;
    for (j = i; j < n; j++) {
      if ((strcmp(*(str + k), *(str + j))) > 0) {
        k = j;
      }
    }
    if (k != i) {
      strcpy(s, *(str + k));
      strcpy(*(str + k), *(str + i));
      strcpy(*(str + i), s);
    }
  }
  printf("Sorted results:\n");
  for (i = 0; i < n; i++) {
    printf("%s\n", *(str + i));
  }
  return 0;
}


