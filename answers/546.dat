#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
  int i, j, n, k;
  char a[150][50], temp[50];
  printf("How many countries?");
  scanf("%d", &n);
  printf("Input their names:\n");
  for (i = 0; i < n; i++) {
    scanf("%s", a[i]);
  }
  for (i = 0; i < n - 1; i++) {
    k = i;
    for (j = i; j < n; j++) {
      if (strcmp(a[k], a[j]) > 0) {
        k = j;
      }
    }
    if (k != i) {
      strcpy(temp, a[k]);
      strcpy(a[k], a[i]);
      strcpy(a[i], temp);
    }
  }
  printf("Sorted results:\n");
  for (i = 0; i < n; i++) {
    printf("%s\n", a[i]);
  }
  return 0;
}

