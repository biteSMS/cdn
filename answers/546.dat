#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void sort(char *a[], int n) {
  int i, j;
  char *temp = NULL;
  for (i = 0; i < n - 1; i++) {
    for (j = i + 1; j < n; j++) {
      if (strcmp(a[i], a[j]) >= 0) {
        temp = a[i];
        a[i] = a[j];
        a[j] = temp;
      }
    }
  }
}
int main() {
  char a[150][20], *b[150];
  int n, i;
  printf("How many countries?");
  scanf("%d", &n);
  getchar();

  printf("Input their names:\n");
  for (i = 0; i < n; i++) {
    b[i] = a[i];
    gets(a[i]);
  }

  sort(b, n);
  printf("Sorted results:\n");

  for (i = 0; i < n; i++) {
    puts(b[i]);
  }

  return 0;
}

