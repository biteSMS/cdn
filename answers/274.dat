main() {
  int i, b, j;
  for (i = 1; i <= 9; i++)
    for (j = 1; j <= i; j++) {
      b = i * j;
      if (j < i)
        printf("%4d", b);
      else {
        printf("%4d\n", b);
      }
    }
}

