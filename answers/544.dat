#include <stdio.h>
#include <stdlib.h>
#define N 40
#define MAX_len 15

int main() {
  long student[N][MAX_len];
  int student_score[N][3];
  int i, j, n, stu_scoresum[N] = {0}, course_scoresum[N] = {0};
  double stu_scoreaver[N] = {0}, course_scoreaver[N] = {0};
  printf("Input the total number of the students(n<40):");
  scanf("%d", &n);
  printf("Input student¡¯s ID and score as: MT  EN  PH:\n");
  for (i = 0; i < n; i++) {
    scanf("%ld", &student[i]);
    for (j = 0; j < 3; j++) {
      scanf("%d", &student_score[i][j]);
      stu_scoresum[i] += student_score[i][j];
    }
    stu_scoreaver[i] = (double)stu_scoresum[i] / 3;
  }
  for (i = 0; i < 3; i++) {
    for (j = 0; j < n; j++) {
      course_scoresum[i] += student_score[j][i];
    }
    course_scoreaver[i] = (double)course_scoresum[i] / n;
  }
  printf("Counting Result:\n");
  printf("Student¡¯s ID\t  MT \t  EN \t  PH \t SUM \t AVER\n");
  for (i = 0; i < n; i++) {
    printf("%12ld\t", *student[i]);
    for (j = 0; j < 3; j++) {
      printf("%4d\t", student_score[i][j]);
    }
    printf("%4d\t%5.1f\n", stu_scoresum[i], stu_scoreaver[i]);
  }
  printf("SumofCourse \t");
  for (i = 0; i < 3; i++) {
    printf("%4d\t", course_scoresum[i]);
  }
  printf("\nAverofCourse\t");
  for (i = 0; i < 3; i++) {
    printf("%4.1f\t", course_scoreaver[i]);
  }
  return 0;
}

