  printf("Input student¡¯s ID and score as: MT  EN  PH:\n");
  for (i = 0; i < n; i++) {
    s[i].sum = 0;
    scanf("%ld", &s[i].id);
    for (j = 0; j < 3; j++) {
      scanf("%d", &s[i].score[j]);
      s[i].sum += s[i].score[j];
    }
    s[i].aver = s[i].sum / 3.0;
  }
  printf("Counting Result:\n");
  printf("Student¡¯s ID\t  MT \t  EN \t  PH \t SUM \t AVER\n");
  for (i = 0; i < n; i++) {
    printf("%12ld\t", s[i].id);
    for (j = 0; j < 3; j++) {
      printf("%4d\t", s[i].score[j]);
    }
    printf("%4d\t%5.1f\n", s[i].sum, s[i].aver);
  }
  for (i = 0; i < n; i++) {
    sum1 += s[i].score[0];
    sum2 += s[i].score[1];
    sum3 += s[i].score[2];
  }
  printf("SumofCourse \t");
  printf("%4d\t", sum1);
  printf("%4d\t", sum2);
  printf("%4d\t", sum3);
  printf("\nAverofCourse\t");
  printf("%4.1f\t%4.1f\t%4.1f\t", 1.0 * sum1 / n, 1.0 * sum2 / n,
         1.0 * sum3 / n);
}

