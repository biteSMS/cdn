#include <stdio.h>
typedef struct student {
  long id;
  int score[3];
  float aver;
  int sum;
} student;
void input(student *ss, int n);
void count(student *ss, int n, int *sum);
void output(student *ss, int *sum, int n);
int main() {
  student ss[40];
  int n, sum[3] = {0};
  printf("Input the total number of the students(n<40):");
  scanf("%d", &n);
  printf("Input student¡¯s ID and score as: MT  EN  PH:\n");
  input(ss, n);
  count(ss, n, sum);
  printf("Counting Result:\n");
  output(ss, sum, n);
}
void input(student *ss, int n) {
  int i, j;
  for (i = 0; i < n; i++) {
    scanf("%ld", &ss[i].id);
    ss[i].sum = 0;
    for (j = 0; j < 3; j++)
      scanf("%d", &ss[i].score[j]);
  }
}
void count(student *ss, int n, int *sum) {
  int i, j;
  for (i = 0; i < 3; i++)
    for (i = 0; i < n; i++) {
      for (j = 0; j < 3; j++) {
        ss[i].sum += ss[i].score[j];
      }
      ss[i].aver = ss[i].sum / 3.0;
    }
  for (i = 0; i < 3; i++) {
    for (j = 0; j < n; j++) {
      sum[i] += ss[j].score[i];
    }
  }
}
void output(student *ss, int *sum, int n) {
  int i, j;
  printf("Student¡¯s ID\t  MT \t  EN \t  PH \t SUM \t AVER\n");
  for (i = 0; i < n; i++) {
    printf("%12ld\t", ss[i].id);
    for (j = 0; j < 3; j++) {
      printf("%4d\t", ss[i].score[j]);
    }
    printf("%4d\t%5.1f\n", ss[i].sum, ss[i].aver);
  }
  printf("SumofCourse \t");
  for (i = 0; i < 3; i++) {
    printf("%4d\t", sum[i]);
  }
  printf("\nAverofCourse\t");
  for (i = 0; i < 3; i++)
    printf("%4.1f\t", sum[i] / n * 1.0);
}

