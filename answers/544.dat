#include <stdio.h>
#include <stdlib.h>
struct student {
  int mt;
  int en;
  int ph;
  int sum;
  long int no;
  float v;
};

typedef struct student stu;

int main() {
  stu a[40];
  int n;
  int SumofCourse1 = 0, SumofCourse2 = 0, SumofCourse3 = 0;
  float AverofCourse1, AverofCourse2, AverofCourse3;
  printf("Input the total number of the students(n<40):");
  scanf("%d", &n);
  printf("Input student¡¯s ID and score as: MT  EN  PH:\n");
  int i;
  for (i = 0; i < n; i++) {
    scanf("%ld%d%d%d", &a[i].no, &a[i].mt, &a[i].en, &a[i].ph);
  }
  for (i = 0; i < n; i++) {
    a[i].sum = 0;
    a[i].sum = a[i].mt + a[i].en + a[i].ph;
    a[i].v = a[i].sum / 3.0;
    SumofCourse1 += a[i].mt;
    SumofCourse2 += a[i].en;
    SumofCourse3 += a[i].ph;
  }
  AverofCourse1 = (float)SumofCourse1 / n;
  AverofCourse2 = (float)SumofCourse2 / n;
  AverofCourse3 = (float)SumofCourse3 / n;
  printf("Counting Result:\n");
  printf("Student¡¯s ID\t  MT \t  EN \t  PH \t SUM \t AVER\n");
  for (i = 0; i < n; i++) {

    printf("%12ld\t%4d\t%4d\t%4d\t%4d\t%5.1f\n", a[i].no, a[i].mt, a[i].en,
           a[i].ph, a[i].sum, a[i].v);
  }
  printf("SumofCourse \t%4d\t%4d\t%4d\t", SumofCourse1, SumofCourse2,
         SumofCourse3);
  printf("\nAverofCourse\t%4.1f\t%4.1f\t%4.1f\t", AverofCourse1, AverofCourse2,
         AverofCourse3);

  return 0;
}

