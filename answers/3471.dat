#include <stdio.h>

int main() {
  int n;
  printf("输入一个整数:\n");
  scanf("%d", &n);
  if (n % 2 == 0) {
    printf("%d 是偶数！\n", n);
  } else
    printf("%d 不是偶数！", n);
}

