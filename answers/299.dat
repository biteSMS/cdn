#include <stdio.h>
#include <string.h>
void Input(char a[][80], int n) {
  int i;

  for (i = 0; i < n; i++) {
    gets(a[i]);
  }
}
void Sort(char a[][80], int n) {
  int i, j, k;
  char temp[80];
  for (i = 0; i < n - 1; i++) {

    for (j = i + 1; j < n; j++) {

      if (strcmp(a[i], a[j]) > 0) {
        strcpy(temp, a[i]);
        strcpy(a[i], a[j]);
        strcpy(a[j], temp);
      }
    }
  }
}
void Print(char a[][80], int n) {
  int i;
  for (i = 0; i < n; i++) {
    puts(a[i]);
  }
}
int main() {
  char str[10][80];
  Input(str, 10);
  Sort(str, 10);
  Print(str, 10);
  return 0;
}

