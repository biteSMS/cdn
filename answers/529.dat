int main() {
  double pi = 1.0;
  double i, j;
  int count = 0;
  for (i = 1.0, j = 2.0; count < 100;) {
    pi = pi * (j / i);
    count++;
    if (j > i) {
      i = i + 2.0;
    } else if (j < i) {
      j = j + 2.0;
    }
  }
  pi = pi * 2;
  printf("pi = %f\n", pi);
  return 0;
}

