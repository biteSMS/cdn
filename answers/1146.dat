#include <stdio.h>
#include <stdlib.h>
#define END -1
int Factorial(int x);
int main() {
  int x, y;
  while (1) {
    printf("input x:");
    scanf("%d", &x);
    if (x <= END)
      break;
    else {
      y = Factorial(x);
      printf("%d! = %d\n", x, y);
    }
  }
  return 0;
}
int Factorial(int x) {
  int i;
  int result = 1;
  for (i = 1; i <= x; i++) {
    result *= i;
  }
  return result;
}

