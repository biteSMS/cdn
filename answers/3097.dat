#include <stdio.h>

double fac(int n) {
  double result = 1;
  int i;

  for (i = 1; i <= n; i++) {
    result *= i;
  }

  return result;
}

double C(int n, int k) { return fac(n) / (fac(k) * fac(n - k)); }

void printYH(int line) {
  int i, j;
  for (i = 0; i < line; i++) {
    for (j = 0; j <= i; j++) {
      printf("%4g", C(i, j));
    }
    printf("\n");
  }
}

int main() {
  int line;

  printf("input n (n<=10):\n");
  scanf("%d", &line);

  printYH(line);
  return 0;
}

