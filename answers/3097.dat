#include <stdio.h>
#define N 10
int main() {
  int n, a[N][N], b, c, d = 1, e = 0;
  printf("input n (n<=10):\n");
  scanf("%d", &n);
  for (b = 0; b < n; ++b) {
    for (c = 0; c < n; ++c) {
      if (b == c || c == 0) {
        a[b][c] = 1;
      }
    }
  }
  for (b = 1; b < n; ++b) {
    for (c = 1; c < d; ++c) {
      a[b][c] = a[b - 1][c - 1] + a[b - 1][c];
    }
    d++;
  }
  for (b = 0; b < n; ++b) {
    for (c = 0; c <= e; ++c) {
      printf("%4d", a[b][c]);
    }
    e++;
    printf("\n");
  }
  return 0;
}

