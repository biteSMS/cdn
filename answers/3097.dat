#include <stdio.h>
#include <string.h>
void main() {
  int a[11][11], i, j, n;
  printf("input n (n<=10):\n");
  scanf("%d", &n);
  for (i = 0; i < n; i++) {
    a[i][0] = 1;
    a[i][i] = 1;
  }
  for (i = 2; i < n; i++) {
    for (j = 1; j < i; j++) {
      a[i][j] = a[i - 1][j] + a[i - 1][j - 1];
    }
  }
  for (i = 0; i < n; i++) {
    for (j = 0; j < i + 1; j++)
      printf("%4d", a[i][j]);
    putchar('\n');
  }
}

