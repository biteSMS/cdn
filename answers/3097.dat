#include <stdio.h>
#include <stdlib.h>
#define N 15
void CaculateYH(int (*p)[N], int n) {
  int i, j;
  for (i = 1; i <= n; i++) //控制行数
  {
    for (j = 0; j < i; j++) //控制每行数值个数
    {
      if (j == 0 || j == i - 1) //控制边缘值
      {
        p[i - 1][j] = 1;
      } else {
        p[i - 1][j] = p[i - 2][j - 1] + p[i - 2][j]; //由杨辉三规律获得中间值
      }
    }
  }
}
void PrintYH(int (*p)[N], int n) //用嵌套循环进行遍历
{
  int i, j;
  for (i = 0; i < N; i++) {
    for (j = 0; j < N; j++) {
      if (p[i][j] != 0)
        printf("%4d", p[i][j]);
    }
    printf("\n");
  }
}
int main() {
  int Yh[N][N] = {0, 0}, n;
  printf("input n (n<=10):\n");
  scanf("%d", &n);   //获得杨辉三角行数
  CaculateYH(Yh, n); //用缓冲器获得杨辉三角赋值情况
  PrintYH(Yh, n);    //打印杨辉三角
  return 0;
}

