int main() {
  double a;
  printf("请输入月用电量：\n");
  scanf("%lf", &a);
  if (a <= 50)
    printf("应支付电费=%.2f\n", 0.53 * a);
  else
    printf("应支付电费=%.2f\n", 0.53 * 50 + (a - 50) * 0.58);
  return 0;
}

