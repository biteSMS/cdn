void ReadData(int a[], int n);
void PrintData(int a[], int n);
void MaxMinExchang(int a[], int n);

int main() {
  const MAX = 10;
  int n, a[MAX];

  printf("Input n(n<=10):\n");
  scanf("%d", &n);
  printf("Input %d numbers:\n", n);

  ReadData(a, n);
  MaxMinExchang(a, n);
  PrintData(a, n);

  return 0;
}
void ReadData(int a[], int n) {
  int i;

  for (i = 0; i < n; i++) {
    scanf("%d", &a[i]);
  }
}
void PrintData(int a[], int n) {
  int i;
  printf("Exchange results:");
  for (i = 0; i < n; i++) {
    printf("%5d", a[i]);
  }
}
void MaxMinExchang(int a[], int n) {
  int max, min, i, pmax = 0, pmin = 0, temp;
  max = a[pmax];
  min = a[pmin];
  for (i = 1; i < n; i++) {
    if (a[i] > max) {
      max = a[i];
      pmax = i;
    } else if (a[i] < min) {
      min = a[i];
      pmin = i;
    }
  }
  temp = a[pmax];
  a[pmax] = a[pmin];
  a[pmin] = temp;
}

