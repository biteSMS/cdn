#include <stdio.h>
int calc(int n) {
  if (n == 1)
    return 1;
  else
    return calc(n - 1) * n;
}
int fun(int m, int n) { return calc(m) / (calc(n) * calc(m - n)); }
int main() {
  int m, n;
  printf("请输入m,n的值( m>n )：\n");
  scanf("%d %d", &m, &n);
  printf("n项之和为：%lf\n", fun(m, n) * 1.0);
  return 0;
}

