#include <stdio.h>
int main() {
  int m, n;
  int i, a = 1, b = 1, c = 1;
  double P;
  printf("请输入m,n的值( m>n )：\n");
  scanf("%d %d", &m, &n);
  for (i = 1; i <= m; i++)
    a *= i;
  for (i = 1; i <= n; i++)
    b *= i;
  for (i = 1; i <= m - n; i++)
    c *= i;
  P = a / (b * c);
  printf("n项之和为：%lf\n", P);
  return 0;
}

