#include <stdio.h>
long fact(int n) {
  int i;
  long res = 1;
  for (i = 1; i <= n; i++) {
    res = i * res;
  }
  return res;
}
double fuc(int m, int n) {
  double p;
  p = (fact(m) / (fact(n) * fact(m - n)));
  return p;
}
int main() {
  int i, j;
  printf("请输入m,n的值( m>n )：\n");
  scanf("%d %d", &i, &j);
  printf("n项之和为：%lf\n", fuc(i, j));
  return 0;
}


