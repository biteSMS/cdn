#include <stdio.h>
#include <stdlib.h>

int main() {
  int m[5] = {};
  int x = 0, i = 0, flag = 0;

  printf("Enter 5 integers: ");
  for (i = 0; i < 5; i++)
    scanf("%d", m + i);
  printf("Enter x: ");
  scanf("%d", &x);

  for (i = 0; i < 5; i++) {
    if (m[i] == x) {
      flag = i;
      break;
    }
  }
  if (flag)
    printf("Index is %d\n", flag);
  if (!flag)
    printf("Not Found");

  return 0;
}

