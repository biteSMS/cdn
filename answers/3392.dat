#include <stdio.h>
main() {
  int i, j, num[10], temp = 0;
  printf("请输入10个整数:");
  for (i = 0; i < 10; i++) {
    scanf("%d", &num[i]);
  }
  for (i = 0; i < 10; i++) {
    for (j = 0; j < 10; j++) {
      if (num[i] < num[j]) {
        temp = num[i];
        num[i] = num[j];
        num[j] = temp;
      }
    }
  }
  for (i = 0; i < 10; i++) {
    printf("%d\t", num[i]);
  }
}


