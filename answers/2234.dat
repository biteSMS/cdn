#include <stdio.h>
int main() {
  int i;
  for (i = 2; i < 10; i++) {
    if (i == i * i * i)
      printf("There are following Armstrong number smaller than 1000:%d\n", i);
  }
  for (i = 10; i < 100; i++) {
    if (i == (i % 10) * (i % 10) * (i % 10) + (i / 10) * (i / 10) * (i / 10))
      printf("There are following Armstrong number smaller than 1000:%d\n", i);
  }
  printf("There are following Armstrong number smaller than 1000:\n");
  for (i = 100; i < 1000; i++) {
    if (i ==
        (i % 10) * (i % 10) * (i % 10) + (i / 100) * (i / 100) * (i / 100) +
            ((i / 10) % 10) * ((i / 10) % 10) * ((i / 10) % 10)) {
      printf("%d ", i);
    }
  }
  return 0;
}

