#include <stdio.h>
int main() {
  int i;
  int a, b, c;
  int sum;
  printf("There are following Armstrong number smaller than 1000:\n");
  for (i = 100; i < 1000; i++) {
    a = i % 10;
    b = (i / 10) % 10;
    c = i / 100;
    sum = c * c * c + b * b * b + a * a * a;
    if (sum == i)
      printf("%d ", i);
  }
}

