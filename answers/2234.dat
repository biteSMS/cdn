int main() {
  int n;
  int a, b, c;
  printf("There are following Armstrong number smaller than 1000:\n");
  for (n = 100; n < 1000; n++) {
    a = n / 100; //百喂数
    b = n % 100 / 10;
    c = n % 10;
    if (n == a * a * a + b * b * b + c * c * c)
      printf("%d ", n);
  }
  return 0;
}

