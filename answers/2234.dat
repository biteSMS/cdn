#include <stdio.h>
main() {
  int m, i, j, k;
  printf("There are following Armstrong number smaller than 1000:\n");
  for (m = 2; m < 1000; m++) {
    i = m % 10;
    j = m / 100;
    k = (m - j * 100) / 10;
    if (m == i * i * i + j * j * j + k * k * k)
      printf("%d ", m);
  }
}


