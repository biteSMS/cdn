  scanf("%d", &data);
  for (j = 2; j < data; j++) {
    if (data % j == 0)
      break;
  }
  if (j == data)
    printf("Invalid input.\n");
  else {
    while (data > 1) {
      if (data % i == 0) {
        if (i == data) {
          printf("%d", i);
          data /= i;
        } else {
          printf("%d", i);
          printf(",");
          data /= i;
        }
      } else
        i++;
    }
  }
}


