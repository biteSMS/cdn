#include <stdio.h>
#include <stdlib.h>
int main() {
  int i = 1, n, sum = 0;
  printf("Input n:");
  scanf("%d", &n);
  do {
    sum += i;
    i++;
  } while (i != n + 1);
  printf("sum = %d\n", sum);
}

