#include <stdio.h>
#include <stdlib.h>
int mystrlen(char *p) {
  int size = 0;
  if (!p)
    return -1;
  while (*p) {
    size++;
    p++;
  }
  return size;
}
int main() {
  char str[1024];
  printf("Please input the string:\n");
  gets(str);
  printf("%d\n", mystrlen(str));
}

