#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define M 5
#define N 10
int main() {
  printf("Input five countries' names:\n");
  int i;
  char a[M][N], b[N];
  for (i = 0; i < M; i++) {
    gets(a[i]);
  }
  strcpy(b, a[0]);
  for (i = 0; i < M; i++) {
    if (strcmp(b, a[i]) > 0)
      strcpy(b, a[i]);
  }
  printf("The minimum is:%s\n", b);
  return 0;
}

