#include <stdio.h>
#include <string.h>
#define N 5
#define M 20
int main() {
  printf("Input five countries' names:\n");
  int i;
  char a[N][M], b[M];
  for (i = 0; i < 5; i++)
    gets(a[i]);
  for (i = 0; i < N; i++) {
    if (strcmp(a[i], a[i + 1]) < 0) {
      strcpy(b, a[i]);
      strcpy(a[i], a[i + 1]);
      strcpy(a[i + 1], b);
    }
  }
  printf("The minimum is:%s\n", a[4]);
  return 0;
}

