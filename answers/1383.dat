#include <stdio.h>
int main() {
  int a[10], n, max = 0, min = 0, maxPos, minPos;
  for (n = 0; n < 10; n++) {
    scanf("%d", &a[n]);
  }
  for (n = 1; n < 10; n++) {
    if (a[n] > max) {
      max = a[n];
      maxPos = n;
    } else if (a[n] <= min) {
      min = a[n];
      minPos = n;
    }
  }
  printf("max=%d, pos=%d\n", max, maxPos);
  printf("min=%d, pos=%d\n", min, minPos);
  return 0;
}

