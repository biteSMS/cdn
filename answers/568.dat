void sort(int a[], int n) {
  int i, j, k, t;
  for (i = 0; i < n - 1; i++) {
    k = i;
    for (j = i + 1; j < n; j++) {
      if (a[j] < a[k])
        k = j;
    }
    if (k != i) {
      t = a[k];
      a[k] = a[i];
      a[i] = t;
    }
  }
