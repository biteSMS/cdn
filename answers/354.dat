#include <stdio.h>
int main() {

  int a[5] = {10, 20, 30, 40, 50}, i;

  for (i = 0; i < 5; ++i) {
    scanf("%d", &a[i]);
  }

  for (i = 4; i >= 0; --i) {
    printf("\ta[%d]=%d,", i, a[i]);
  }
  printf("\n");
  return 0;
}

