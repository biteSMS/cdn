#include <stdio.h>
void inv(int x[], int n);
void main() {
  int i, a[5] = {10, 20, 30, 40, 50};

  inv(a, 5);
  for (i = 0; i < 5; i++)
    printf("\ta[%d]=%d,", (4 - i), a[i]);
  printf("\n");
}
void inv(int x[], int n) {
  int temp, i, j, m = (n - 1) / 2;
  for (i = 0; i <= m; i++) {
    j = n - 1 - i;
    temp = x[i];
    x[i] = x[j];
    x[j] = temp;
  }
}

