#include <stdio.h>
main() {
  int a[9][9];
  int i, j;
  for (i = 1; i <= 9; i++) {
    printf("%4d", i);
  }
  printf("\n-----------------------------\n");
  for (i = 0; i < 9; i++) {
    for (j = 0; j <= i; j++) {
      a[i][j] = (i + 1) * (j + 1);
    }
  }
  for (i = 0; i < 9; i++) {
    for (j = 0; j <= i; j++) {
      printf("%4d", a[i][j]);
    }
    printf("\n");
  }
}

