int main() {
  int i, j, n, k;

  for (i = 1; i <= 9; i++) {
    printf("%4d", i);
  }
  printf("\n-----------------------------\n");

  for (j = 1; j <= 9; j++) {
    for (k = j, n = 1; k <= j * j;) {

      printf("%4d", k);
      n++;
      k = j * n;
    }
    printf("\n");
  }
  return 0;
}

