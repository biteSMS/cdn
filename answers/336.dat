#include <stdio.h>
int main() {
  int i, j, n, m;
  for (m = 1; m < 10; m++) {
    printf("%4d", m);
  }
  printf("\n-----------------------------\n");
  for (n = 1; n < 10; n++) {
    for (j = 1; j < n + 1; j++) {
      printf("%4d", n * j);
    }
    printf("\n");
  }
  return 0;
}

