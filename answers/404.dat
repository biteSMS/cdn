#include <stdio.h>
#define N 5
void choiceSort(int a[N]);
void Print(int a[N]);
int main() {
  int a[N] = {0};
  int i;
  for (i = 0; i < N; i++) {
    printf("Enter No.%2d:", i + 1);
    scanf("%d", a + i);
  }
  choiceSort(a);
  Print(a);
  return 0;
}
void choiceSort(int a[N]) {
  int i, j, k, temp;
  for (i = 0; i < N; i++) {
    k = i;
    for (j = i; j < N; j++) {
      if (a[i] > a[j])
        k = j;
    }
    if (k != i) {
      temp = a[k];
      a[k] = a[i];
      a[i] = temp;
    }
  }
}
void Print(int a[N]) {
  int i = 0;
  for (i = 0; i < N; i++) {
    printf("%d", *(a + i));
  }
}

