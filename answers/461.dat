#include <stdio.h>
#include <string.h>
char back(char s[], int len) {
  if (len <= 0) {
    printf("%c", s[len]);
    return 0;
  } else {
    printf("%c", s[len]);
    return back(s, --len);
  }
}
int main() {
  char str[20];

  printf("input your string:\n");
  scanf("%s", str);

  int len;
  len = strlen(str) - 1;

  back(str, len);
  return 0;
}

