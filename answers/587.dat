int main() {
  int x, y, i, j, l;
  for (i = 1; i < 10; i++) {
    for (j = 1; j < 10; j++) {
      for (l = 0; l < 10; l++) {
        if (i != j && i != l && j != l &&
            (i * 100 + j * 10 + l + j * 100 + l * 10 + l == 532)) {
          printf("x=%d,y=%d,z=%d\n", i, j, l);
        }
      }
    }
  }
}

