#include <stdio.h>

int main() {
  int x, y, z;
  for (x = 0; x <= 9; x++) {
    for (y = 0; y <= 9; y++) {
      for (z = 0; z <= 9; z++) {
        if (100 * x + 110 * y + 12 * z == 532)
          printf("x=%d,y=%d,z=%d\n", x, y, z);
      }
    }
  }

  return 0;
}

