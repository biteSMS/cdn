main() {
  int x, y, i, a, b, c;
  printf("Input x:");
  scanf("%d", &x);
  if (x < 0) {
    i = -x;
    a = i / 100;
    b = (i - a * 100) / 10;
    c = i % 10;
    y = 100 * c + b * 10 + a;
  } else {
    i = x;
    a = i / 100;
    b = (i - a * 100) / 10;
    c = i % 10;
    y = 100 * c + b * 10 + a;
  }
  printf("y = %d\n", y);
}

