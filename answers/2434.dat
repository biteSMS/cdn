#include <stdio.h>
#include <string.h>
#define M 100
void Fun(char *name, char *resultName) {
  int i = 0, length = strlen(name);
  do {
    resultName[2 * i] = name[i];
    resultName[2 * i + 1] = ' ';
    i++;
  } while (name[i] != ' ');
  resultName[2 * i] = '\0';
}
int main() {
  char name[M], resultName[M];
  printf("请输入你的姓名：");
  scanf("%s", name);
  Fun(name, resultName);
  puts(resultName);
  return 0;
}

