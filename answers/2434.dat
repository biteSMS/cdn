#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main() {
  char a[20];
  char name[40];
  char *b = NULL;
  int i, n = 0;
  printf("请输入你的姓名：");
  scanf("%s", a);
  n = strlen(a);
  b = a;

  for (i = 0; *b != '\0'; b++, i += 2) {
    name[i] = *b;
    if (i < 2 * n)
      name[i + 1] = ' ';
  }
  name[i] = '\0';
  puts(name);
  return 0;
}

