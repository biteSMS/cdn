#include <stdio.h>
#include <string.h>
void addspace(char *s, char *b) {
  int l = strlen(s);
  int lb = 2 * l - 1;
  int i;
  for (i = 0; i < lb; i++)
    *(b + i) = i % 2 ? ' ' : *(s + i / 2);
  *(b + lb) = *(s + l);
}
int main() {
  int i, l;
  char s[150], b[300];
  printf("请输入你的姓名：");
  scanf("%s", s);
  addspace(s, b);
  printf("%s\n", b);
  return 0;
}

