#include <stdio.h>
int main() {
  int i, a, b, c;
  for (i = 101; i <= 999; i++) {
    a = i / 100;
    c = i % 10;
    b = (i % 100) / 10;
    if (i + a + b * 10 + c * 100 == 1333) {
      printf("a=%d,b=%d,c=%d\n", a, b, c);
    }
  }
  return 0;
}

