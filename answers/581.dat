#include <stdio.h>
#define n 3
int main() {
  int i, j;
  for (i = 0; i <= n; i++) {
    for (j = 1; j <= n - i; j++)
      printf(" ");
    for (j = 1; j <= 2 * i + 1; j++)
      printf("*");
    printf("\n");
  }
  for (i = n - 1; i >= 0; i--) {
    for (j = 1; j <= n - i; j++)
      printf(" ");
    for (j = 1; j <= 2 * i + 1; j++)
      printf("*");
    printf("\n");
  }
}

