void main() {
  int a[50][50], i, j, n, k, b[50][50];
  printf("please input n:");
  scanf("%d", &n);
  for (i = 0; i < n; i++) {
    for (j = 0; j < n; j++) {
      a[i][j] = n * i + j + 1;
    }
  }
  printf("转置前：\n");
  for (i = 0; i < n; i++) {
    for (j = 0; j < n; j++) {
      printf("%-4d", a[i][j]);
    }
    putchar('\n');
  }
  for (i = 0; i < n; i++) {
    for (j = 0; j < n; j++) {
      b[i][j] = a[j][i];
