#include <stdio.h>
#include <stdlib.h>

int main() {
  char f, s, t, F;
  printf("input your English name:\n");
  scanf("%c%c%c", &f, &s, &t);
  F = f - 32;
  printf("%c%c%c\n", F, s, t);
  printf("%c:%d\n", f, f - 'a' + 1);
  printf("%c:%d\n", s, s - 'a' + 1);
  printf("%c:%d\n", t, t - 'a' + 1);

  return 0;
}

