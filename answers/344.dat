#include <stdio.h>
main() {
  int x;                      // 1
  printf("Enter a integer:"); // 1
  scanf("%d", &x);            // 1
  if (x != 0)                 // 1
  {
    if (x > 0) // 1
    {
      x = x % 2; // 1
      if (x == 0)
        printf("The integer is a plus even number\n"); // 1
      else
        printf("This integer is a plus odd number\n"); // 1
    } else {
      x = x % 2;
      if (x == 0)
        printf("This integer is a minus even number\n");
      else
        printf("This integer is a minus odd number\n"); // 1
    }
  } else
    printf("This integer is zero.\n"); // 1
}
