    if (n > 0) //奇函数
    {
      printf("This integer is a plus odd number\n");
    } else {
      printf("This integer is a minus odd number\n");
    }
  } else {
    if (n > 0) {
      printf("The integer is a plus even number\n");
    } else {
      printf("This integer is a minus even number\n");
    }
  }
  return 0;
}

