#include <stdio.h>
#include <string.h>
int main() {
  int n = 0;
  printf("Enter a integer:");
  scanf("%d", &n);
  if (n != 0 && n > 0 && n % 2 == 0) {
    printf("The integer is a plus even number\n");
    return 0;
  }
  if (n != 0) {
    char ans[100];
    strcpy(ans, "This integer is a ");
    strcat(ans, n > 0 ? "plus " : "minus ");
    strcat(ans, n % 2 ? "odd number" : "even number");
    printf("%s\n", ans);
  } else {
    printf("This integer is zero.\n");
  }
  return 0;
}


