#include <stdio.h>
#define N 3
#define M 2
int main() {
  int matrix[N][M], i = 0, m = 0;
  for (i = 0; i < N; i++) {
    for (m = 0; m < M; m++)
      matrix[i][m] = i + m;
  }
  for (i = 0; i < N; i++) {
    for (m = 0; m < M; m++)
      printf("%4d", matrix[i][m]);
    printf("\n");
  }
  return 0;
}

