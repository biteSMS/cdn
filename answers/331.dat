int main() {
  int a[10];
  int i, sum;
  for (i = 0, sum = 0; i < 10; i++) {
    printf("Enter the No.%d=", i + 1);
    scanf("%d", &a[i]);
    sum = sum + a[i];
  }
  printf("Total=%d\n", sum);
  return 0;
}

