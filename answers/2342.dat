#include <math.h>
#include <stdio.h>

void num(int m, int k, int a[]);

int main() {
  int i, m, n, a[1000];
  printf("Please enter two integers m and n:\n");
  scanf("%d,%d", &m, &n);
  num(m, n, a);
  for (i = 0; i < n; i++) {
    printf("%d ", a[i]);
  }
  return 0;
}

void num(int m, int k, int a[]) {
  int i = m + 1, j, count = 0;
  while (count < k) {
    int flag = 1;
    for (j = 2; j <= sqrt(i); j++) {
      if (i % j == 0)
        flag = 0;
    }
    if (flag) {
      a[count] = i;
      count++;
    }
    i++;
  }
}

