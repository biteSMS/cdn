#include <stdio.h>
#include <stdlib.h>

int susu(int x) {
  int susu = 1, i = 0;
  for (i = 2; i < x && susu; i++) {
    if (x % i == 0)
      susu = 0;
  }

  return susu;
}

int main() {
  int m = 0, n = 0, i = 0;
  printf("Please enter two integers m and n:\n");
  scanf("%d,%d", &m, &n);
  for (i = m + 1; n > 0; i++) {
    if (susu(i)) {
      printf("%d ", i);
      n--;
    }
  }
  return 0;
}


