#include <math.h>
#include <stdio.h>
#include <stdlib.h>
int fun(int n) {
  int i = 0, j, k = n;
  while (n != 0) {
    n /= 10;
    i++;
  }
  j = k / pow(10, i - 1);
  return j;
}
int main() {
  int n, m;
  printf("请输入一个整数：");
  scanf("%d", &n);
  m = fun(n);
  printf("该整数以%d打头！\n", m);
  return 0;
}

