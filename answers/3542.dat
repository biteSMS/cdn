#include <stdio.h>
void f(int a[], int *odd, int *even) {
  int i;
  for (i = 0; i < 20; i++) // 2
    if (a[i] % 2 != 0)     // 2
      (*odd)++;            // 1
    else                   // 1
      (*even)++;           // 1
  return;
}
int main() {
  int a[20], i, odd = 0, even = 0;
  printf("input 20 numbers:");
  for (i = 0; i < 20; i++) // 2
    scanf("%d", &a[i]);    // 1
  f(a, &odd, &even);       // 1
  printf("Odd numbered=%d,Even numbered=%d\n", odd, even);
  return 0;
}


