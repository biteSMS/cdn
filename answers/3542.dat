void fun(int a[], int *odd, int *even) {
  int i, j = 0, c = 0;

  for (i = 0; i < 20; i++) {
    if (a[i] % 2 == 0) {
      j = j + 1;
      *even = j;
    } else {
      c = c + 1;
      *odd = c;
    }
  }
}
int main() {
  int i;
  int a[20];
  int odd = 0;
  int even = 0;
  printf("input 20 numbers:");
  for (i = 0; i < 20; i++) {
    scanf("%d", &a[i]);
  }
  fun(a, &odd, &even);
  printf("Odd numbered=%d,Even numbered=%d\n", odd, even);
  return 0;
}
