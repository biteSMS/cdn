#include <stdio.h>
void f(int a[], int *odd, int *even) {
  int i;
  int as = 0, bs = 0;
  for (i = 0; i <= 19; i++) {
    if (a[i] % 2 == 0) {
      as++;
    } else {
      bs++;
    }
    *odd = bs;
    *even = as;
  }
}
void main() {
  int a[20], i, odd = 0, even = 0;
  printf("input 20 numbers:");
  for (i = 0; i <= 19; i++) {
    scanf("%d", &a[i]);
  }
  f(a, &odd, &even);
  printf("Odd numbered=%d,Even numbered=%d\n", odd, even);
  return 0;
}

