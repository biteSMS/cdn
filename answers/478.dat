  scanf("%lf", &r);
  printf("请输入要判断的点的坐标(x,y)：");
  scanf("%lf,%lf", &pos_x, &pos_y);

  double flag = pow(pos_x - centre_x, 2) + pow(pos_y - centre_y, 2) - pow(r, 2);
  if (flag > 0) {
    printf("该点不在圆内\n");
  } else {
    if (flag < 0) {
      printf("该点在圆内\n");
    } else {
      printf("该点在圆上\n");
    }
  }
  return 0;
}

