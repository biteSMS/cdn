#include <stdio.h>

int main() {
  float x, y, r, s, m, n;

  printf("请输入圆的圆心坐标：");
  scanf("%f,%f", &m, &n);
  printf("请输入圆的半径：");
  scanf("%f", &r);
  printf("请输入要判断的点的坐标(x,y)：");
  scanf("%f,%f", &x, &y);

  s = (x - m) * (x - m) + (y - n) * (y - n);

  if (s > r * r) {
    printf("该点不在圆内\n");
  }
  if (s < r * r) {
    printf("该点在圆内\n");
  }
  if (s == r * r) {
    printf("该点在圆上\n");
  }
}

