#include <stdio.h>

int main() {
  float ox, oy, r, x, y, D;
  printf("请输入圆的圆心坐标：");
  scanf("%f,%f", &ox, &oy);
  printf("请输入圆的半径：");
  scanf("%f", &r);
  printf("请输入要判断的点的坐标(x,y)：");
  scanf("%f,%f", &x, &y);
  D = (x - ox) * (x - ox) + (y - oy) * (y - oy) - r * r;
  if (D > 0)
    printf("该点在圆外\n");
  else if (D == 0)
    printf("该点在圆上\n");
  else
    printf("该点在圆内\n");
}

