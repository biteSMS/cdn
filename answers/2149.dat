int main() {
  float cj[10];
  float avage = 0;
  float sum = 0;
  int i;
  printf("Input 10 scores:\n");
  for (i = 0; i < 10; i++) {
    scanf("%f", &cj[i]);
    sum += cj[i];
  }
  avage = sum / 10;
  printf("Total = %.1f\nAverage = %.1f\n", sum, avage);
  return 0;
}

