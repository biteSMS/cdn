#include <stdio.h>
#include <stdlib.h>

int main() {
  char a, b, c;
  char n;

  for (n = 'x'; n <= 'z'; n++) {
    if (n != 'x' && n != 'z') {
      c = n;
    } else if (n != 'x' && n != c) {
      a = n;
    } else {
      b = n;
    }
  }

  printf("order is a--%c\tb--%c\tc--%c\n", a, b, c);
  return 0;
}

