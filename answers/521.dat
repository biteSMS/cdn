#include <stdio.h>
int main() {
  int n, m = 0, j, sum;
  do {
    printf("Input num:");
    scanf("%d", &n);
    if (n != 0) {
      m = 0;
      for (j = 0; j <= n; j++) {
        m = m + j;
      }
      printf("sum = %d\n", m);
    } else
      sum = m;
  } while (n);
  printf("sum = %d\n", sum);
  return 0;
}

