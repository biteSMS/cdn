#include <stdio.h>
int main() {
  char ch;
  printf("Press a key and then press Enter:");
  scanf("%c", &ch);
  if (ch >= 'A' && ch <= 'Z')
    ch = ch + 32;
  else if (ch >= 'a' && ch <= 'z')
    ch = ch - 32;
  printf("%c\n", ch);
  return 0;
}

