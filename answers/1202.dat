int main() {
  int a;
  printf("Input m:");
  scanf("%d", &a);
  if (a > 0 && a % 2 == 0)
    printf("%d is a positive even\n", a);
  else if (a > 0 && a % 2 != 0)
    printf("%d is a positive odd\n", a);
  else if (a < 0 && a % 2 == 0)
    printf("%d is a negative even\n", a);
  else if (a < 0 && a % 2 != 0)
    printf("%d is a negative odd\n", a);
  else
    printf("%d is zero. It is an even\n", a);
  return 0;
}

