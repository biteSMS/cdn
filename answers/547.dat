#include <stdio.h>
#include <stdlib.h>
#define N 50
void strcpy(char a[], char b[]);
int main() {
  char a[N], b[N];
  printf("Input a string:");
  gets(a);
  strcpy(a, b);
  printf("The copy is:%s", b);
}
void strcpy(char a[], char b[]) {
  int i = 0;
  do {
    b[i] = a[i];
    i++;
  } while (a[i] != '\0');
  b[i] = '\0';
}

