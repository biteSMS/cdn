#include <stdio.h>
#define N 5

void InputArray(int a[N], int b[N]);
void Swap(int a[N], int b[N]);
void Print(int a[N], int b[N]);
int main() {
  int a[N], b[N];

  InputArray(a, b);
  Swap(a, b);
  Print(a, b);

  return 0;
}
void InputArray(int a[N], int b[N]) {
  int i;

  for (i = 0; i < N; i++) {
    scanf("%d", a + i);
  }
  for (i = 0; i < N; i++) {
    scanf("%d", b + i);
  }
}
void Swap(int a[N], int b[N]) {
  int i, temp;

  for (i = 0; i < N; i++) {
    temp = a[i];
    a[i] = b[i];
    b[i] = temp;
  }
}
void Print(int a[N], int b[N]) {
  int i;

  for (i = 0; i < N; i++) {
    printf("%5d", *(a + i));
  }
  printf("\n");
  for (i = 0; i < N; i++) {
    printf("%5d", *(b + i));
  }
}

