#include <stdio.h>
#define N 5
void exchange(int a[], int b[]);
int main() {
  int a[N], b[N], i;
  for (i = 0; i < 5; i++) {
    scanf("%d", &a[i]);
  }
  for (i = 0; i < 5; i++) {
    scanf("%d", &b[i]);
  }
  exchange(a, b);
  for (i = 0; i < 5; i++) {
    printf("%5d", a[i]);
  }
  printf("\n");
  for (i = 0; i < 5; i++) {
    printf("%5d", b[i]);
  }
  return 0;
}
void exchange(int a[], int b[]) {
  int temp;
  int i;
  for (i = 0; i < 5; i++) {
    temp = a[i];
    a[i] = b[i];
    b[i] = temp;
  }
}

