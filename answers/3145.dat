#include <stdio.h>
#include <stdlib.h>
int main() {
  int i, j, n;
  printf("Input n:\n");
  scanf("%d", &n);
  for (i = 1; i <= n; i++) {
    printf("%4d", i);
  }
  printf("\n");
  for (i = 1; i <= n; i++) {
    printf("   -");
  }
  printf("\n");
  for (i = 1; i <= n; i++) {
    for (j = 1; j <= n; j++) {
      printf("%4d", i * j);
    }
    printf("\n");
  }
  return 0;
}

