#include <stdio.h>
int prime(int n) {

  int i;
  for (i = 3; i < n / 2; i = i + 2) {
    if (n % i == 0)
      return (0);
  }
  return 1;
}

main(void) {
  int a, i;
  a = 3;
  for (i = 3; i < 98;) {
    if (prime(i)) {
      if ((i - a) == 2)
        printf("%4d/%d", a, i);
      a = i;
    }
    i = i + 2;
  }
}

