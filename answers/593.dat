int sushu(int m);
int main() {
  int i;
  for (i = 3; i < 100; i++) {
    if (sushu(i) == 1) {
      if (sushu(i + 2) == 1)
        printf("%4d/%d", i, i + 2);
    }
  }
}
int sushu(int m) {
  int q, i, find = 0;
  q = sqrt(m);
  for (i = 2; i <= q && !find; i++) {
    if (m % i == 0)
      find = 1;
  }
  if (find == 1)
    return 0;
  else
    return 1;
}

