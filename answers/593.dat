#include <math.h>
#include <stdio.h>
int SS(int x) {
  int i = 2;
  for (i = 2; i <= sqrt(x); i++) {
    if (x % i == 0)
      return 0;
  }
  return 1;
}
int SSS(int x) {
  if (SS(x) == 1 && SS(x + 2) == 1)
    return 1;
}
int main() {
  int n = 3;
  for (n = 3; n < 100; n++) {
    if (SSS(n) == 1)
      printf("%4d/%d", n, n + 2);
  }
}

