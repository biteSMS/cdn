#include <stdio.h>
#include <string.h>
#define N 80
void Inverse(char str[], char ptr[]);
int main() {
  char a[N], b[N];
  printf("Please input the string:\n");
  gets(a);
  Inverse(a, b);
  printf("4");
  return 0;
}
void Inverse(char str[], char ptr[]) {
  int len, i, j;
  len = strlen(str);
  for (i = 0, j = len - 1; str[i] != '\0'; i++, j--) {
    ptr[j] = str[i];
  }
  ptr[i] = '\0';
}


