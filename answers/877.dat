#include <stdio.h>
void main() {
  int a[10] = {2, 17, 8, 3, 24, 53, 82, 1, 29, 101};
  int i, j, t;
  for (j = 0; j < 9; j++) {
    for (i = 0; i < 9 - j; i++)
      if (a[i] < a[i + 1]) {
        t = a[i];
        a[i] = a[i + 1];
        a[i + 1] = t;
      }
  }
  for (i = 0; i < 10; i++)
    printf("%d ", a[i]);
}

