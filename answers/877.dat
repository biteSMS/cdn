#include <stdio.h>
#include <stdlib.h>
#define SIZE 10

void sort(int *p, int size) {
  int i = 0, j = 0;
  for (i = 0; i < size - 1; i++) {
    for (j = 0; j < size - 1; j++) {
      if (p[j] < p[j + 1]) {
        int temp = p[j];
        p[j] = p[j + 1];
        p[j + 1] = temp;
      }
    }
  }
}

int main() {
  int a[SIZE] = {2, 17, 8, 3, 24, 53, 82, 1, 29, 101};
  sort(a, SIZE);

  int i = 0;
  for (i = 0; i < 10; i++)
    printf("%d ", a[i]);

  return 0;
}

