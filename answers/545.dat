#include <stdio.h>
void Exchange(int *a, int *b) {
  int t;
  t = *b;
  *b = *a;
  *a = t;
}
int main() {
  int a, b;
  printf("Please enter a,b:");
  scanf("%d,%d", &a, &b);
  printf("Before swap: a = %d, b = %d\n", a, b);
  Exchange(&a, &b);
  printf("After swap: a = %d, b = %d\n", a, b);
  return 0;
}

