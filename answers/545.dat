#include <stdio.h>
#include <stdlib.h>
void Swap(int *p1, int *p2) {
  int c;
  c = *p1;
  *p1 = *p2;
  *p2 = c;
}
int main() {
  int a, b;
  printf("Please enter a,b:");
  scanf("%d,%d", &a, &b);
  printf("Before swap: a = %d, b = %d\n", a, b);
  Swap(&a, &b);
  printf("After swap: a = %d, b = %d", a, b);
  return 0;
}

