int GetAboveAver(int score[], int n) {
  int i, sum = 0, x, count = 0;
  for (i = 0; i < n; i++) {

    sum += score[i];
  }

  x = sum / n;
  for (i = 0; i < n; i++) {
    if (x <= score[i]) {
      count++;
    }
  }

  printf("Average score is %d\n", x);
  return count;
}
