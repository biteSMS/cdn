#include <stdio.h>

int main() {
  int a[20], i, sum = 0;
  printf("input 20 numbers:");
  for (i = 0; i < 20; i++) {
    scanf("%d", &a[i]);
    if (a[i] > 0)
      sum += a[i];
  }
  printf("sum=%d\n", sum);

  return 0;
}

