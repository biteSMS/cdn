#include <stdio.h>

long getFact(int num);

void main() {
  int n, i;
  printf("Input n:");
  scanf("%d", &n);
  for (i = 1; i <= n; i++) {
    printf("%d! = %ld\n", i, getFact(i));
  }
}

long getFact(int num) {
  if (num == 0)
    return 1;
  else {
    int i;
    long fact = 1;
    for (i = 2; i <= num; i++) {
      fact *= i;
    }
    return fact;
  }
}

