int hw(int n) //判断回文数
{
  int t, r = 0;
  for (t = n; t; t /= 10) //把n从个位到最高位取反
    r = r * 10 + t % 10;
  return r == n;
}
int ss(int n) //判断素数
{
  int i;
  for (i = 2; i <= sqrt(n); i++) //从2到n的平方根判断能否被n整除
    if (n % i == 0)
      return 0;
  return 1;
}
