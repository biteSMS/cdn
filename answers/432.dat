#include <math.h>
#include <stdio.h>
int f1(int n);
int f2(int n);
int main() {
  int i;
  printf("Following are palindrome primes not greater than 1000:\n");
  for (i = 10; i < 1000; i++) {
    if (f1(i)) {
      if (f2(i))
        printf("%d\t", i);
    }
  }
}
int f1(int n) {
  int i, flag = 1;
  for (i = 2; i <= sqrt(n); i++) {
    if (n % i == 0) {
      flag = 0;
      break;
    }
  }
  if (flag)
    return 1;
  else
    return 0;
}
int f2(int n) {
  int a[3];
  int i = -1, j;
  while (n) {
    i++;
    a[i] = n % 10;
    n = n / 10;
  }
  for (j = 0; j <= i; j++, i--) {
    if (a[i] != a[j]) {
      return 0;
    }
  }
  return 1;
}

