#include "stdio.h"
int prime(int n) {
  int i;
  if (n > 2 && !(n & 1) || n < 2)
    return 0;
  for (i = 3; i * i <= n; i += 2)
    if (!(n % i))
      return 0;
  return 1;
}
int main(void) {
  int i;
  printf("Following are palindrome primes not greater than 1000:\n");
  for (i = 11; i < 1001; i += 2)
    if (prime(i) && (i < 10 || i < 100 && i / 10 == i % 10 ||
                     i < 1000 && i / 100 == i % 10))
      printf("%d\t", i);
  return 0;
}

