#include <stdio.h>

int sushu(int n) {
  int t = 0, i;
  for (i = 1; i <= n; i++)
    if (n % i == 0)
      t++;
  if (t == 2)
    return 1;
  else
    return 0;
}
int huiwen(int n) {
  if (n > 99)
    return n % 10 == n / 100;
  else
    return n % 10 == n / 10;
}
void main() {
  int i;
  printf("Following are palindrome primes not greater than 1000:\n");
  for (i = 11; i < 1000; i++)
    if (sushu(i) && huiwen(i))
      printf("%d\t", i);
}

