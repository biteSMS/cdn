#include <stdio.h>
#include <stdlib.h>
int fun() {
  int i, j, k, sum = 0;
  for (i = 1; i <= 3; i++) {
    for (j = 1; j <= 5; j++) {
      k = 8 - i - j;
      if ((k >= 1 && k <= 6) && (i != 0) && (j != 0) || (k == 0)) {
        sum++;
        printf("red:%4d white:%4d black:%4d\n", i, j, k);
      }
    }
  }
  return sum;
}
int main() {
  int sum = 0;
  printf("The result:\n");
  sum = fun();
  printf("sum=%4d\n", sum);
  return 0;
}

