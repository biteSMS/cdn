#include <stdio.h>
#define N 10
void insert(int px[]) {
  int i, j, t;
  for (i = 0; i < N - 1; i++) {
    for (j = i + 1; j < N; j++) {
      if (px[i] > px[j]) {
        t = px[i];
        px[i] = px[j];
        px[j] = t;
      }
    }
  }
}
int main() {
  int i, a[N];
  for (i = 0; i < N; i++)
    scanf("%d", &a[i]);
  insert(a);
  for (i = 0; i < N; i++)
    printf("%d ", a[i]);
  return 0;
}


