#include <stdio.h>
#include <stdlib.h>

void insert(int num[], int size) {
  int i, j, k, temp;
  for (i = 0; i < size - 1; i++) {
    k = i;
    for (j = i; j < size; j++) {
      if (num[k] > num[j]) {
        k = j;
      }
    }
    if (k != i) {
      temp = num[k];
      num[k] = num[i];
      num[i] = temp;
    }
  }
}

int main() {
  const int size = 10;
  int num[size], i;
  for (i = 0; i < size; i++) {
    scanf("%d", &num[i]);
  }
  insert(num, size);
  for (i = 0; i < size; i++) {
    printf("%d ", num[i]);
  }
  return 0;
}

