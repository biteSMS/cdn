#include <stdio.h>
main() {
  int num, yy, mm, dd;
  float p;
  printf("Enter item number:\n");
  scanf("%d", &num);
  printf("Enter unit price:\n");
  scanf("%f", &p);
  printf("Enter purchase date (yy mm dd):\n");
  scanf("%d%d%d", &yy, &mm, &dd);
  printf("Item      Unit     Purchase\n");
  printf("%-9d$%-9.2f%02d/%02d/%02d\n", num, p, mm, dd, yy);
}

