#include <stdio.h>
void main() {
  int m, w, c, n = 0;
  printf("\tMEN\tWOMEN\tCHILDREN\n");
  printf("-----------------------------------------\n");
  for (m = 0; m <= 10; m++) {
    for (w = 0; w <= 20; w++) {
      for (c = 10; c <= 20; c++)
        if (m * 3 + w * 2 + c * 1 == 50 && m + w + c == 30) {
          n++;
          printf("%2d:\t%d\t%d\t%d\n", n, m, w, c);
        }
    }
  }
}

