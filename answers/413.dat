  for (i = 0; i < 17; i++)
    for (j = 0; j < 25; j++) {
      if (i * 3 + j * 2 + (30 - i - j) == 50) {
        n++;
        printf("%2d:\t%d\t%d\t%d\n", n, i, j, 30 - i - j);
      }
