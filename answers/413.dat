#include <stdio.h>
int main() {
  int men, women, children, n = 0;
  printf("\tMEN\tWOMEN\tCHILDREN\n");
  printf("-----------------------------------------\n");
  for (men = 0; men <= 10; men++) {
    for (women = 0; women <= 25; women++) {
      children = 30 - men - women;
      if (3 * men + 2 * women + children == 50) {
        n++;
        printf("%2d:\t%d\t%d\t%d\n", n, men, women, children);
      }
    }
  }
}

