#include <stdio.h>
int main() {
  printf("请输入行数：");
  int a;
  scanf("%d", &a);
  printf("请输入列数：");
  int b;
  scanf("%d", &b);
  printf("请输入%d*%d个数据：\n", a, b);
  int num[100][100];
  int i, j;
  for (i = 0; i < a; i++) {
    for (j = 0; j < b; j++) {
      scanf("%d", &num[i][j]);
    }
  }
  int temp[100];
  int temp2;
  for (i = 0; i < a; i++)
    temp[i] = num[i][b - 1];
  for (j = 0; j < b; j++) {
    for (i = 0; i < a; i++) {
      temp2 = num[i][j];
      num[i][j] = temp[i];
      temp[i] = temp2;
    }
  }
  printf("结果是 ：\n");
  for (i = 0; i < a; i++) {
    for (j = 0; j < b; j++) {
      printf("%6d", num[i][j]);
      if (j == b - 1)
        printf("\n");
    }
  }
}

