#include <stdio.h>
int main() {
  int d[10][10] = {0};
  int i, j, a, b;
  printf("请输入行数：");
  scanf("%d", &a);
  printf("请输入列数：");
  scanf("%d", &b);
  printf("请输入%d*%d个数据：\n", a, b);
  for (i = 0; i < a; i++)
    for (j = 0; j < b; j++)
      scanf("%d", &d[i][j]);
  for (i = 0; i < a; i++) {
    for (j = b - 1; j >= 0; j--)
      d[i][j + 1] = d[i][j];
    d[i][0] = d[i][b];
  }
  printf("结果是 ：\n");
  for (i = 0; i < a; i++) {
    for (j = 0; j < b; j++)
      printf("%6d", d[i][j]);
    printf("\n");
  }
  return 0;
}

