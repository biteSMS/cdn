#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define M 10
#define N 10
void Input(int a[][N], int m, int n);
void Change(int a[][N], int m, int n);
void Printf(int a[][N], int m, int n);
int main() {
  int m, n, a[M][N];
  printf("请输入行数：");
  scanf("%d", &m);
  printf("请输入列数：");
  scanf("%d", &n);
  Input(*a, m, n); //输入数组的数据
  printf("结果是 ：\n");
  Change(a, m, n);
  Printf(a, m, n);
  return 0;
}
//函数功能：输入二维数组的数据
void Input(int a[][N], int m, int n) {
  int i, j;
  printf("请输入%d*%d个数据：\n", m, n);
  for (i = 0; i < m; i++) {
    for (j = 0; j < n; j++) {
      scanf("%d", &a[i][j]);
    }
  }
}
void Change(int a[][N], int m, int n) {
  int i, j, x;
  for (i = 0; i < m; i++) {
    x = a[i][n - 1];
    for (j = n - 1; j > 0; j--) {
      a[i][j] = a[i][j - 1];
    }
    a[i][0] = x;
  }
}
void Printf(int a[][N], int m, int n) {
  int i, j;
  for (i = 0; i < m; i++) {
    for (j = 0; j < n; j++) {
      printf("%6d", a[i][j]);
    }
    printf("\n");
  }
}


