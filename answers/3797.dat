#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main() {
  int a[10][10];
  int i, j, x, y, n;
  printf("请输入行数：");
  scanf("%d", &x);
  printf("请输入列数：");
  scanf("%d", &y);
  printf("请输入%d*%d个数据：\n", x, y);
  for (i = 0; i < x; i++) {
    for (j = 0; j < y; j++) {
      scanf("%d", &a[i][j]);
    }
  }
  for (i = 0; i < x; i++) {
    for (j = 0; j < y; j++) {
      n = a[i][j];
      a[i][j] = a[i][y - 1];
      a[i][y - 1] = n;
    }
  }
  printf("结果是 ：\n");
  for (i = 0; i < x; i++) {
    for (j = 0; j < y; j++) {
      printf("%6d", a[i][j]);
    }
    printf("\n");
  }
  return 0;
}

