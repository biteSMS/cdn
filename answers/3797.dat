#include <stdio.h>
#include <stdlib.h>

int main() {
  int a[10][10], i, j, temp[10], k, l;
  printf("请输入行数：");
  scanf("%d", &i);
  printf("请输入列数：");
  scanf("%d", &j);
  printf("请输入%d*%d个数据：\n", i, j);
  for (k = 0; k < i; k++) {
    for (l = 0; l < j; l++) {
      scanf("%d", &a[k][l]);
    }
  }
  for (k = 0; k < i; k++) {
    l = j - 1;
    temp[k] = a[k][l];
  }
  for (l = j; l > 0; l--) {
    for (k = 0; k < i; k++) {
      a[k][l] = a[k][l - 1];
    }
  }
  for (k = 0; k < i; k++) {
    a[k][0] = temp[k];
  }
  printf("结果是 ：\n");
  for (k = 0; k < i; k++) {
    for (l = 0; l < j; l++) {
      printf("%6d", a[k][l]);
    }
    printf("\n");
  }
  return 0;
}

