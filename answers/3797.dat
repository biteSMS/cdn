#include <stdio.h>
main() {
  int i, j, a, b, k;
  printf("请输入行数：");
  scanf("%d", &i);
  printf("请输入列数：");
  scanf("%d", &j);
  int str[i][j];
  int str1[i][j];
  printf("请输入%d*%d个数据：\n", i, j);
  for (a = 0; a < i; a++)
    for (b = 0; b < j; b++) {
      scanf("%d", &str[a][b]);
    }
  for (a = 0; a < i; a++) {
    k = 0;
    for (b = 0; b < j; b++) {
      k = b + 1;
      if (b + 1 == j) {
        k = 0;
      }
      str1[a][k] = str[a][b];
    }
  }
  a = 0, b = 0;
  printf("结果是 ：\n");
  for (a = 0; a < i; a++) {
    for (b = 0; b < j; b++)
      printf("%6d", str1[a][b]);
    printf("\n");
  }
}

