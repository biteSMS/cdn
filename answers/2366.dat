#include <stdio.h>
#define N 80
void strcat(char *p, char *q);
main() {
  char s[N], t[N];
  printf("Input a string:\n");
  gets(s);
  printf("Input another string:\n");
  gets(t);

  strcat(s, t);
  printf("Concatenate results:%s\n", s);
  return 0;
}
void strcat(char *p, char *q) {
  char *point = p;
  while (*point)
    point++;
  while (*q) {
    *point = *q;
    point++;
    q++;
  }
  *point = '\0';
}

