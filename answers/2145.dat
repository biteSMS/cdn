#include <stdio.h>
#include <stdlib.h>

float FindAver(float score[], int n);

int main() {
  float score[10], aver;
  int i;
  for (i = 0; i < 10; i++) {
    scanf("%f", &score[i]);
  }
  aver = FindAver(score, 10);
  printf("aver = %5.2f\n", aver);
  for (i = 0; i < 10; i++) {
    if (score[i] > aver)
      printf("score = %5.2f\n", score[i]);
  }

  return 0;
}

float FindAver(float score[], int n) {
  float sum = 0.0;
  int i;
  for (i = 0; i < n; i++) {
    sum += score[i];
  }
  return (float)sum / n;
}

