#include <math.h>
#include <stdio.h>

int main() {
  int i, a, b, c, d;
  printf("四位玫瑰花数有:");

  for (i = 1000; i <= 9999; i++) {
    a = i / 1000;
    b = i / 100 % 10;
    c = i / 10 % 10;
    d = i % 10;
    if (i == pow(a, 4) + pow(b, 4) + pow(c, 4) + pow(d, 4)) {
      printf("%d\t", i);
    }
  }

  return 0;
}

