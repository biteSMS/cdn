#include <stdio.h>
#define N 80
char Mystrcat(char *s, char *t);
int main() {
  char a[2 * N], b[N];
  printf("Input the first string:");
  gets(a);
  printf("Input the second string:");
  gets(b);
  Mystrcat(a, b);
  printf("The result is: %s", a);
  return 0;
}
char Mystrcat(char *s, char *t) {
  while (*s != '\0') {
    s++;
  }
  for (; *t != '\0'; s++, t++) {
    *s = *t;
  }
  *s = '\0';
}

