#include <stdio.h>
#include <string.h>
#define N 80

char *MyStrcat(char *str1, char *str2) {
  char *str = str1;
  while (*str1 != '\0') {
    str1++;
  }
  for (; *str2 != '\0'; str1++, str2++) {
    *str1 = *str2;
  }
  *str1 = '\0';
  return str;
}
int main() {
  char str1[N], str2[N];
  printf("Input the first string:");
  gets(str1);
  printf("Input the second string:");
  gets(str2);
  printf("The result is: %s", MyStrcat(str1, str2));
  return 0;
}

