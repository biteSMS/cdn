#include <stdio.h>
#include <stdlib.h>

unsigned long Fact(unsigned int n);
int main() {
  int m, k;
  do {
    printf("Input m,k (m>=k>0):");
    scanf("%d,%d", &m, &k);
  } while (m <= 0 || k < 0 || m < k);

  printf("The combination is %ld\n", Fact(m) / (Fact(k) * Fact(m - k)));

  return 0;
}
unsigned long Fact(unsigned int n) {
  int i, sum = 1;

  for (i = 1; i <= n; i++) {
    sum *= i;
  }

  return sum;
}

