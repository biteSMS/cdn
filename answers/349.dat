#include <stdio.h>
#include <stdlib.h>
unsigned long Fact(unsigned int n) {
  if (n >= 2)
    return n * Fact(n - 1);
  else
    return 1;
}
int main() {
  int m, k;
  unsigned long n;
  do {
    printf("Input m,k (m>=k>0):");
    scanf("%d,%d", &m, &k);
  } while (m < 0 || k < 0 || m <= k);
  n = Fact(m) / (Fact(k) * Fact(m - k));
  printf("The combination is %ld\n", n);
  return 0;
}

