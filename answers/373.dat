#include <stdio.h>
int a[9];
int ok(int t, int *s);
int main() {
  int i, count = 1;
  for (i = 123; i <= 333; i++) {
    if (ok(i, a) && ok(2 * i, a + 3) && ok(3 * i, a + 6))
      printf("No.%d: %d %d %d\n", count++, i, 2 * i, 3 * i);
  }

  return 0;
}
int ok(int t, int *s) {
  int *p, *q;
  for (p = s; p < s + 3; p++) {
    *p = t % 10;
    t = t / 10;
    for (q = a; q < p; q++) {
      if (*p == 0 || *q == *p)
        return 0;
    }
  }
}

