#include <stdio.h>
#include <stdlib.h>

int main() {
  int a, b, c;
  printf("请输入一个整数:\n");
  scanf("%d", &a);
  printf("请输入第一个倍数:\n");
  scanf("%d", &b);
  printf("请输入第二个倍数:\n");
  scanf("%d", &c);
  if (a % b == 0 && a % c == 0) {
    printf("yes\n");
  } else {
    printf("no\n");
  }
  return 0;
}

