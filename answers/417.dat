#include <stdio.h>
int sub(int n, int d) /*定义函数递归求鱼的总数*/
{
  if (d == 0)
    return 1;
  if (n % 5 == 1 && 5 < n)
    return sub(n / 5 * 4, d - 1);
  else
    return 0;
}
int main() {
  int i;
  for (i = 1; !sub(i, 5); i++)
    ; /*调用递归函数*/
  printf("Total number of fish catched=%d\n", i);
  return 0;
}

