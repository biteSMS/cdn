#include <stdio.h>
#include <stdlib.h>
#include <string.h>
void main() {
  char a[15], b[15], c[15];
  int i, j, max;
  scanf("%s%s", a, b);
  max = 0;
start:
  for (i = max; a[i] != '\0'; i++) {
    if (a[i] == b[0]) {
      max = i;
      for (j = 0; a[i] != '\0'; i++, j++)
        if (a[i] != b[j]) {
          max = i;
          goto start;
        }
    }
  }

  strcpy(c, a + max);

  printf("%s\n", c);
}

