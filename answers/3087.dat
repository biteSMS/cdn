#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main() {
  char str1[50], str2[50], keep[50];
  int i, j, count = 0, flag = 1;
  scanf("%s%s", str1, str2);
  for (i = 0; i < strlen(str1); i++) {
    if (str1[i] == str2[count]) {
      keep[count] = str1[i];
      if (str1[i + 1] == str2[count + 1]) {
        keep[count + 1] = str1[i + 1];
        for (j = count + 2, i = i + 2; i < strlen(str1); j++, i++) {
          if (str1[i] == str2[j])
            keep[j] = str1[i];
          else
            break;
        }
        keep[j] = '\0';
        break;
      } else {
        for (i = i + 2, j = count + 2; flag && i < strlen(str1); i++, j++) {
          if (str1[i] == str2[j])
            flag = 0;
        }
        if (flag)
          keep[count + 1] = '\0';
        break;
      }
    }
  }
  printf("%s\n", keep);
}

