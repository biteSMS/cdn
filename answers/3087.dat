#include <stdio.h>
#include <string.h>
#define N 81
int main() {
  char str1[N], str2[N];
  int i = 0, flag = 1;
  scanf("%s%s", str1, str2);
  for (i = 1; str1[i] != '\0' && str2[i] != '\0'; i++) {
    if (strncmp(str2, str1 + strlen(str1) - i - 1, i) == 0)
      flag++;
    else
      break;
  }
  for (i = 0; i < flag; i++)
    printf("%c", str2[i]);
  return 0;
}

