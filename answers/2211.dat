#include <stdio.h>

int main() {
  int A, B, C;
  int i, j, k;
  int count = 0;
  for (i = 1; i <= 5; i++) {
    for (j = 1; j <= 5; j++) {
      if (i == j) {
        continue;
      }
      for (k = 1; k <= 5; k++) {
        if (k == i || k == j) {
          continue;
        }

        count++;
        printf("%2d:%d,%d,%d\n", count, i, j, k);
      }
    }
  }

  return 0;
}

