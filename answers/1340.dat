#include <stdio.h>
int main() {
  int i, j;
  char cur_ch = 'A';  //当前字母
  char next_ch = 'A'; //下一个字母
  for (i = 0; i < 6; ++i, ++next_ch) {
    cur_ch = next_ch;
    for (j = 6; j > i; --j) {
      printf("%c", cur_ch++);
    }
    printf("\n");
  }
  return 0;
}

