#include <stdio.h>
#define N 10
#define M 5
int main() {
  int year, rate;
  int i, j;
  float a[N][M];
  printf("Enter interest rate:");
  scanf("%d", &rate);
  printf("Enter number of year:");
  scanf("%d", &year);
  for (i = 0; i < M; i++) {
    a[0][i] = 100 * (1 + (rate + i) / 100.0);
  }
  for (i = 1; i < year; i++) {
    for (j = 0; j < M; j++) {
      a[i][j] = a[i - 1][j] * (1 + (rate + j) / 100.0);
    }
  }
  printf("\nyears");
  for (i = 0; i < M; i++) {
    printf("%6d%%", rate + i);
  }
  printf("\n");
  for (i = 0; i < year; i++) {
    printf("%3d   ", i + 1);
    for (j = 0; j < M; j++) {
      printf("%7.2f", a[i][j]);
    }
    printf("\n");
  }
  return 0;
}

