#include <math.h>
#include <stdio.h>
int main() {
  int rate, year, money = 100, i, j;
  printf("Enter interest rate:");
  scanf("%d", &rate);
  printf("Enter number of year:");
  scanf("%d", &year);
  printf("\nyears");
  for (i = rate; i <= rate + 4; i++) {
    printf("%6d%%", i);
  }
  printf("\n");
  for (j = 0; j < year; j++) {
    printf("%3d   ", j + 1);
    for (i = rate;; i++) {
      printf("%7.2f", pow((1 + (double)i / 100), j + 1) * money);
      if (i == rate + 4) {
        printf("\n");
        break;
      }
    }
  }
}

