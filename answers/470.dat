#include <math.h>
#include <stdio.h>

void Output(int year, int rate);

main() {
  int rate;
  int year;
  printf("Enter interest rate:");
  scanf("%d", &rate);
  printf("Enter number of year:");
  scanf("%d", &year);
  Output(year, rate);
}

void Output(int year, int rate) {
  int i, j;
  float a[5];
  int rate1;

  rate1 = rate;

  for (i = 0; i < 5; i++) {
    a[i] = (float)rate / 100;
    rate++;
  }

  printf("\nyears");
  for (i = 1; i <= 5; i++) {
    printf("%6d%%", rate1);
    rate1++;
  }
  printf("\n");

  for (j = 1; j < year + 1; j++) {
    printf("%3d   ", j);
    for (i = 0; i < 5; i++) {
      printf("%7.2f", 100 * pow((1 + a[i]), j));
    }
    printf("\n");
  }
}

