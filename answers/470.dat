#include <math.h>
#include <stdio.h>
#include <stdlib.h>

int main() {
  int rate, year, i, j;
  printf("Enter interest rate:");
  scanf("%d", &rate);
  printf("Enter number of year:");
  scanf("%d", &year);
  printf("\nyears");
  for (i = 0; i < 5; i++)
    printf("%6d%%", rate + i);
  for (i = 0; i < year; i++) {
    printf("\n");
    printf("%3d   ", i + 1);
    for (j = 0; j < 5; j++)
      printf("%7.2f", 100 * pow(1 + (rate + j) / 100.0, i + 1));
  }
  return 0;
}


