#include <math.h>
#include <stdio.h>
int main() {
  int r, y;
  printf("Enter interest rate:");
  scanf("%d", &r);
  printf("Enter number of year:");
  scanf("%d", &y);
  printf("\nyears");
  printf("%6d%%%6d%%%6d%%%6d%%%6d%%\n", r, r + 1, r + 2, r + 3, r + 4);
  int i = 1, j = r;
  float r1 = r;
  float a, b;
  for (i = 1; i < y + 1; i++) {
    printf("%3d   ", i);
    for (j = r; j < r + 5; j++, r1++) {
      a = pow(1 + r1 / 100, i);
      b = a * 100;
      printf("%7.2f", b);
    }
    r1 = r;
    printf("\n");
  }
}

