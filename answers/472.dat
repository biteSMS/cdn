#include <stdio.h>
int main() {
  int a, b, sum;
  printf("请输入第一个整数：");
  scanf("%d", &a);
  printf("请输入第二个整数：");
  scanf("%d", &b);
  sum = a + b;
  printf("%d + %d = %d\n", a, b, sum);
  return 0;
}

