#include <stdio.h>
#include <stdlib.h>
int MyStrcmp(char s[], char t[]) {
  int i, k = 0;
  for (i = 0; s[i] != '\0' && t[i] != '\0'; i++) {
    if (s[i] > t[i]) {
      k = 1;
      break;
    }
    if (s[i] < t[i]) {
      k = -1;
      break;
    }
  }
  return k;
}
int main() {
  const int n = 100;
  char s[n], t[n];
  int k = 0;
  printf("Input s\n");
  gets(s);
  printf("Input t\n");
  gets(t);
  k = MyStrcmp(s, t);
  if (k > 0)
    printf("string s>string t.\n");
  if (k < 0)
    printf("string s<string t.\n");
  if (k == 0)
    printf("string s=string t.\n");
  return 0;
}

