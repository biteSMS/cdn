#include <stdio.h>
#include <string.h>
#define N 8
#define M 20
int main() {
  char a[N][M], b[M];
  int i, j;
  for (i = 0; i < N; i++)
    scanf("%s", &a[i]);
  for (i = 0; i < N - 1; i++) {
    for (j = i + 1; j < N; j++) {
      if (strcmp(a[i], a[j]) > 0) {
        strcpy(b, a[i]);
        strcpy(a[i], a[j]);
        strcpy(a[j], b);
      }
    }
  }
  for (i = 0; i < N; i++)
    puts(a[i]);
  return 0;
}

