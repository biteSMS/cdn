#include <stdio.h>
#include <string.h>
int main() {
  char str[8][20];
  char temp[20];
  int i, j;

  for (i = 0; i < 8; i++) {
    scanf("%s", &str[i]);
  }
  for (i = 0; i < 8; i++) {
    for (j = i + 1; j < 8; j++) {
      if (strcmp(str[i], str[j]) > 0) {
        strcpy(temp, str[i]);
        strcpy(str[i], str[j]);
        strcpy(str[j], temp);
      }
    }
  }
  for (i = 0; i < 8; i++) {
    printf("%s\n", str[i]);
  }
  return 0;
}

