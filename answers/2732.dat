#include <stdio.h>
void Squeeze(char a[], char c) {
  int i, j;
  for (i = 0; a[i] != '\0'; i++) {
    if (a[i] == c) {
      for (j = i; a[j] != '\0'; j++)
        a[j] = a[j + 1];
      i--;
    }
  }
  puts(a);
}
main() {
  char a[20], c;
  gets(a);
  c = getchar();
  Squeeze(a, c);
}

