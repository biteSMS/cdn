int main() {
  int i = 0;
  char a[20], c;
  gets(a);
  c = getchar();
  while (a[i] != '\0') {
    if (a[i] == c) {
      int j = i;
      while (a[j + 1] != '\0') {
        a[j] = a[j + 1];
        j++;
      }
      a[j] = a[j + 1];
      i--;
    }
    i++;
  }
  puts(a);
  return 0;
}


