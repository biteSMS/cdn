#include <stdio.h>
#include <stdlib.h>
#include <string.h>
void Squeeze(char s[], char c) {
  int i, j = 0;
  int n;
  char b[100];
  strcpy(b, s);
  n = strlen(s);
  for (i = 0; i < n; i++) {
    if (s[i] != c) {
      b[j] = s[i];
      j++;
    }
  }
  b[j] = '\0';
  strcpy(s, b);
}
int main() {
  int i, j;
  char c;
  char s[100];
  int n;
  gets(s);
  c = getchar();
  Squeeze(s, c);
  puts(s);
  return 0;
}

