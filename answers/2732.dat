void Squeeze(char *s, char c) {
  int i, j = 0;
  for (i = 0; *(s + i) != '\0'; i++) {
    if (*(s + i) != c) {
      *(s + j) = *(s + i);
      j++;
    }
  }
  *(s + j) = '\0';
}

