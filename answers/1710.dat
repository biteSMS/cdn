#include <stdio.h>

int main() {
  int n;
  printf("Please enter a number:");
  scanf("%d", &n);
  if (n <= 1) {
    printf("It is not a prime number.No divisor!\n");
    return 0;
  }
  int i, ma = 0;
  for (i = 2; i < n; i++) {
    if (n % i == 0) {
      printf("%d\n", i);
      ma = 1;
    }
  }
  if (ma == 0)
    printf("It is a prime number.No divisor!\n");
}

