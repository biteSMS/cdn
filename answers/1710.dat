int isPerfect(int n) {
  int i, k = 1;
  for (i = 2; i < n; i++) {
    if (n % i == 0) {
      k = 0;
      break;
    }
  }
  if (k == 1) {
    return 1;
  } else {
    return 0;
  }
}


