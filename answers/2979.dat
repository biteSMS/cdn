int main() {
  int n, count = 0;
  printf("Please enter the number:\n");
  scanf("%d", &n);
  printf("%d: ", n);
  if (n < 0)
    n = -n;
  while (n) {
    n /= 10;
    count++;
  }
  printf("%d bits\n", count);
  return 0;
}

