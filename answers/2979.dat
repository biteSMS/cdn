#include <stdio.h>
int main() {
  int n, x, w, i = 1;
  printf("Please enter the number:\n");
  scanf("%d", &n);
  x = fabs(n);
  for (w = 1;; w *= 10) {
    if (x / w < 10)
      break;
    else
      i++;
  }
  printf("%d: %d bits\n", n, i);
  return 0;
}

