#include <stdio.h>

double my_pow(int a, int b) {
  int i = 0;
  double ans = 1;
  for (i = 0; i < b; i++) {
    ans *= a;
  }

  return ans;
}

int main() {
  printf("Enter x and n\n");
  int a = 0, b = 0;
  scanf("%d%d", &a, &b);
  printf("mypow(%d,%d) = %.2f\n", a, b, my_pow(a, b));

  return 0;
}

