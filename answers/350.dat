#include <stdio.h>

double myPOW(int x, int n);

int main() {
  int x, n;
  printf("Enter x and n\n");
  scanf("%d%d", &x, &n);

  printf("mypow(%d,%d) = %.2f\n", x, n, myPOW(x, n));

  return 0;
}

double myPOW(int x, int n) {
  double sum = 1;
  int i = 1;
  for (; i <= n; i++) {
    sum *= x;
  }
  return sum;
}

