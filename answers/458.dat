  int i, n;
  printf("Enter n:");
  scanf("%d", &n);
  for (i = 7; i >= 0; i--) {
    a[i + 1] = a[i];
    if (a[i] < n) {
      a[i + 1] = n;
      break;
    } else if (i == 0 && a[i] > n) {
      a[0] = n;
    }
  }
  for (i = 0; a[i] != '\0'; i++) {
    printf("%d ", a[i]);
  }
