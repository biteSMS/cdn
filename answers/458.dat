#include <stdio.h>
#include <stdlib.h>

int main() {
  int a[20] = {2, 4, 6, 8, 10, 12, 14, 16}, b[20];
  int i, n, j = 0;
  printf("Enter n:");
  scanf("%d", &n);
  for (i = 0; a[i] != '\0'; i++) {
    if (a[i] <= n) {
      b[j++] = a[i];
    }
  }
  b[j++] = n;
  for (i = 0; a[i] != '\0'; i++) {
    if (a[i] >= n) {
      b[j++] = a[i];
    }
  }
  b[j] = '\0';
  for (i = 0; b[i] != '\0'; i++) {
    printf("%d ", b[i]);
  }
  return 0;
}

