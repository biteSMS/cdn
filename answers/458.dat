#include <stdio.h>
void order(int a[], int n, int b) {
  int i;
  i = n - 1;
  while (i >= 0 && a[i] > b) {
    a[i + 1] = a[i];
    i--;
  }
  a[i + 1] = b;
}
int main() {
  int a[20] = {2, 4, 6, 8, 10, 12, 14, 16};
  int b, n, i;
  printf("Enter n:");
  scanf("%d", &b);
  n = 8;
  order(a, n, b);
  n = n + 1;
  for (i = 0; i < n; i++)
    printf("%d ", a[i]);
  printf("\n");
  return 0;
}

