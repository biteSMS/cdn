#include "stdio.h"
#include "stdlib.h"
#include "string.h"
int main() {
  int a[20] = {2, 4, 6, 8, 10, 12, 14, 16}; // 0-7
  printf("Enter n:");
  int n, i, j;
  int flag = 1;
  scanf("%d", &n);
  for (i = 0; i < 7; i++) {
    if (n <= a[i]) {
      for (j = 8; j > i; j--) {
        a[j] = a[j - 1];
      }
      a[i] = n;
      flag = 0;
    }
    if (flag == 0) {
      break;
    }
  }
  if (flag == 1) {
    a[8] = n;
  }
  for (i = 0; i < 9; i++) {
    printf("%d ", a[i]);
  }
  return 0;
}

