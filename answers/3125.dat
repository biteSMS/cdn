#include <stdio.h>
main() {
  int n, f = 1;
  printf("Please enter the number:\n");
  f = scanf("%d", &n);
  if (!f)
    printf("error!\n");
  else {
    if (n > 999 && n < 10000)
      printf("%d: 1000-9999\n", n);
    else if (n > 99 && n < 10000)
      printf("%d: 100-999\n", n);
    else if (n > 9 && n < 10000)
      printf("%d: 10-99\n", n);
    else if (n >= 0 && n < 10000)
      printf("%d: 0-9\n", n);
    else
      printf("error!\n");
  }
}


