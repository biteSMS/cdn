#include <stdio.h>
void Swap(int *x, int *y) {
  int temp;
  temp = *x;
  *x = *y;
  *y = temp;
}
void Sort(char *p, int m) {
  int i;
  char temp, *p1, *p2;
  for (i = 0; i < m / 2; i++) {
    p1 = p + i;
    p2 = p + (m - 1 - i);
    temp = *p1;
    *p1 = *p2;
    *p2 = temp;
  }
}
int main() {
  int i;
  int num[6];
  for (i = 0; i < 6; i++)
    scanf("%d", &num[i]);
  for (i = 5; i >= 0; i--)
    printf("%d ", num[i]);
  return 0;
}

