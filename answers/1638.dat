  char first[160];
  char second[80];
  printf("Input a string:");
  gets(first);
  printf("Input another string:");
  gets(second);
  MyStrCat(first, second);
  printf("Concatenate results:%s\n", first);
}
void MyStrCat(char *dest, char *source) {
  int i = 0, j = 0;
  while (*(dest + i) != '\0') {
    i++;
  }
  for (; *(source + j) != '\0'; i++, j++) {
    *(dest + i) = *(source + j);
  }
  *(dest + i) = '\0';
}

