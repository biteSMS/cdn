#include <stdio.h>
void mystrcat(char a[], char b[]) {
  int i = 0, j = 0;
  while (a[i++] != '\0')
    ; //找到a的结束位置
  i--;
  while (b[j] != '\0') { //把b元素赋值到a中
    a[i++] = b[j++];
  }
  a[i] = '\0'; //加上结束标志
}
int main() {
  char a[100], b[100];
  printf("Input a string:");
  gets(a);
  printf("Input another string:");
  gets(b);
  mystrcat(a, b);
  printf("Concatenate results:%s\n", a);
  return 0;
}

