#include <math.h>
#include <stdio.h>
#include <stdlib.h>

int main() {
  int i, x, y;
  for (i = 0; i <= 5; i++) {
    x = pow(2, i);
    y = pow((-3), i);
    printf("2 power %d is %d, -3 power %d is %d\n", i, x, i, y);
  }

  return 0;
}

