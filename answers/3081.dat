#include <stdio.h>
#include <string.h>

int IsAllDigit(char p[]) {
  int flag = 1, i, len;

  len = strlen(p);

  for (i = 0; i < len; i++) {
    if (p[i] < '0' || p[i] > '9')
      flag = 0;
  }
  return flag;
}
int main() {
  char str[20];

  printf("Please input a string:\n");
  gets(str);

  if (IsAllDigit(str))
    printf("The string is a digit string.");
  else
    printf("The string is not a digit string.");

  return 0;
}

