void Inverse(char str[]);

int main() {
  char a[ARR_SIZE];

  printf("Please enter a string: ");
  gets(a);
  Inverse(a);
  printf("The inversed string is: ");
  puts(a);
  return 0;
}

void Inverse(char str[]) {
  int len, i;

  char a[ARR_SIZE];
  strcpy(a, str);
  len = strlen(str);
  for (i = 0; i <= len - 1; i++) {
    str[i] = a[len - 1 - i];
  }
}

