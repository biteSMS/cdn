#include <string.h>

// ·´Ðò×Ö·û´®x²¢·µ»Ø
char *reverse(char *x) {
  int len = strlen(x);
  int i, n = len / 2;
  char tem;
  for (i = 0; i <= n; i++) {
    tem = x[i];
    x[i] = x[len - 1 - i];
    x[len - 1 - i] = tem;
  }
  return x;
}

int main() {
  char str[100];
  printf("Please enter a string: ");
  gets(str);
  printf("The inversed string is: ");
  puts(reverse(str));
  return 0;
}

