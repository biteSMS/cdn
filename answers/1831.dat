#include <stdio.h>

int main() {
  float x;
  printf("Please input x:");
  scanf("%f", &x);
  if (x < 1)
    printf("y = %.2f\n", x);
  else if (x >= 1 && x < 10)
    printf("y = %.2f\n", 2 * x - 1);
  else
    printf("y = %.2f\n", 3 * x - 11);

  return 0;
}

