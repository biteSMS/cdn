#include <stdio.h>
#include <string.h>
int main() {
  int l, i;
  char s[100], a[100];
  gets(a);
  for (i = 0; i < 5; i++) {
    gets(s);
    if (strcmp(s, a) < 0)
      strcpy(a, s);
  }
  printf("min is %s\n", a);
  return 0;
}

