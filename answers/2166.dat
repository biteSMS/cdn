#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(void) {
  char str[80], min[80];
  int i;
  scanf("%s", &min);
  for (i = 1; i <= 4; i++) {
    scanf("%s", &str);
    if (strcmp(min, str) > 0)
      strcpy(min, str);
  }
  printf("min is %s\n", min);
}

