#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main() {
  char a[5][100];
  char *p[5];
  int i, k;

  for (i = 0; i < 5; i++) {
    p[i] = a[i];
    gets(p[i]);
  }
  k = 0;
  for (i = 1; i < 5; i++) {
    if (strcmp(p[k], p[i]) > 0) {
      k = i;
    }
  }
  printf("min is %s\n", p[k]);
  return 0;
}

