#include "math.h"
#include <stdio.h>
/*编写程序求出用数字0至9可以组成多少个各位上没有重复数字的三位偶数。
****要求输入提示信息为：无
****输出格式要求为：%d\n
*/
main() {
  int a = 0;
  for (a = 1; a <= 9999; a++) {
    if (a % 2 == 1 && a % 3 == 2 && a % 5 == 4 && a % 6 == 5 && a % 7 == 0) {
      printf("x = %d\n", a);
      break;
    }
  }
}

