#include <math.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main() {
  int x, find = 1;

  x = 0;
  do {
    x++;
    if ((x % 2 == 1) && (x % 3 == 2) && (x % 5 == 4) && (x % 6 == 5) &&
        (x % 7 == 0)) {
      find = x;
    }
  } while (find == 1);
  printf("x = %d\n", x);
}

