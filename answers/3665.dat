#include <stdio.h>

void GetScore(int num[], int n) {
  int i;
  for (i = 0; i < n; i++) {
    scanf("%d", &num[i]);
  }
}

float Aver(int score[], int n) {
  int i;
  float sum = 0;
  for (i = 0; i < n; i++) {
    sum += score[i];
  }
  return sum / n;
}

int OverAver(int score[], int n, float aver) {
  int i, count = 0;
  for (i = 0; i < n; i++) {
    if (score[i] - aver > 0)
      count++;
  }
  return count;
}

int main() {
  const int size = 40;
  int score[size], count, n;
  float aver;

  scanf("%d", &count);
  GetScore(score, count);

  aver = Aver(score, count);
  printf("Average score is %10.2f\n", aver);

  n = OverAver(score, count, aver);
  printf("The number of students in more than average %d\n", n);
  return 0;
}

