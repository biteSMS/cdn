#include <stdio.h>
#define N 40
float aver(int score[], int n) {
  int i;
  float total = 0;
  for (i = 0; i < n; i++) {
    total += score[i];
  }
  return total / n;
}
void main() {
  int score[N] = {0};
  float avg = 0;
  int i, n, count = 0;
  scanf("%d", &n);
  for (i = 0; i < n; i++) {
    scanf("%d", &score[i]);
  }
  avg = aver(score, n);
  for (i = 0; i < n; i++) {
    if (avg < (float)score[i])
      count++;
  }
  printf("Average score is %10.2f\n", avg);
  printf("The number of students in more than average %d\n", count);
}

