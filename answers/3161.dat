#include <stdio.h>

int main() {
  int flag = 1;
  float dat1, dat2, result;
  char op;

  scanf("%f%c%f", &dat1, &op, &dat2);

  if (op == '+')
    result = dat1 + dat2;
  else if (op == '-')
    result = dat1 - dat2;
  else if (op == '*')
    result = dat1 * dat2;
  else if (op == '/') {
    if (dat2 == 0) {
      printf("dat is 0!Error!\n");
      flag = 0;
    } else
      result = dat1 / dat2;

  } else {
    printf("Error!\n");
    flag = 0;
  }

  if (flag == 1)
    printf("%.2f%c%.2f=%.2f\n", dat1, op, dat2, result);

  return 0;
}

