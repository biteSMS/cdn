#include <stdio.h>
#include <stdlib.h>

int main() {
  struct player {
    int Player_NO;
    int Score;
    int Rank;
  };
  int i;
  typedef struct player player;
  player s[7] = {{1, 5, 3}, {2, 3, 1}, {3, 4, 2}, {4, 7, 5},
                 {5, 3, 1}, {6, 5, 3}, {7, 6, 4}};
  printf("  Player\\_NO  Score  Rank\n");
