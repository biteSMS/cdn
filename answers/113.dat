#include <stdio.h>
#include <stdlib.h>
#define a 40
int GetAboveAver(int score[], int n) {
  int i, b = 0, sum = 0;
  float aver;
  for (i = 0; i <= n; i++) {
    sum += score[i];
  }
  aver = (float)sum / n;
  for (i = 0; i <= n; i++) {
    if (score[i] > aver) {
      b++;
    }
  }
  return b;
}
int main() {
  int i = -1, b;
  int s[a];
  do {
    i++;
    scanf("%d", &s[i]);

  } while (s[i] >= 0);

  b = GetAboveAver(s, i);
  printf("Students of above average is %d\n", b);
}


