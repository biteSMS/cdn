#include <stdio.h>
#define N 80

char *MyStrcat(char *dstStr, char *srcStr);

main() {
  char s[N], t[N];
  printf("Input a string:\n");
  gets(s);
  printf("Input another string:\n");
  gets(t);
  printf("Concatenate results:%s\n", MyStrcat(s, t));
}
char *MyStrcat(char *dstStr, char *srcStr) {
  char *p = NULL;
  p = dstStr;
  while (*dstStr != '\0') {
    dstStr++;
  }
  for (; *srcStr != '\0'; srcStr++, dstStr++) {
    *dstStr = *srcStr;
  }

  *dstStr = '\0';

  return p;
}


