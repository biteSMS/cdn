#include <stdio.h>
#include <string.h>
int check(char s[]) {
  int i, len;
  int flag = 1;
  len = (int)strlen(s);
  if (len > 5) {
    printf("错误：字符串太长\n\n");
    return 0;
  } else {
    for (i = 0; s[i] != '\0' && flag; i++) {
      if (s[i] < '0' || s[i] > '7') {
        printf("错误:%c 不是一个八进制字符\n\n", s[i]);
        flag = 0;
      }
    }
    return flag;
  }
}
int turn_to_ten(char s[]) {
  int i;
  int num = 0;
  for (i = 0; s[i] != '\0'; i++) {
    num = num * 8 + (s[i] - '0');
  }
  return num;
}
int main() {
  char s[10];
  int num;
  printf("输入一个八进制字符串（5位）:");
  gets(s);
  if (check(s)) {
    num = turn_to_ten(s);
    printf("输入的八进制字符串为%s\n", s);
    printf("\n%s 转换成十进制整数为：%d\n", s, num);
  }
  return 0;
}

