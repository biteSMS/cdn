//用双重循环编程打印以下图形：
// ABCDEF
// BCDEF
// CDEF
// DEF
// EF
// F

#include <stdio.h>

int main() {
  const char str[] = "ABCDEF";
  const int len = 6;
  int i = 0, j = 0;
  for (i = 0; i < len; i++) {
    for (j = i; j < len; j++) {
      printf("%c", str[j]);
    }
    printf("\n");
  }

  return 0;
}

