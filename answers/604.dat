#include <stdio.h>

int main() {
  int i, j;
  char a[6] = {'F', 'E', 'D', 'C', 'B', 'A'};
  for (i = 1; i <= 6; i++) {
    for (j = 6 - i; j >= 0; j--) {
      printf("%c", a[j]);
    }
    printf("\n");
  }
  return 0;
}

