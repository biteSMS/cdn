#include <stdio.h>
#include <stdlib.h>

int main() {
  int a[10] = {};
  int i = 0, j = 0, max = 0;

  for (i = 0; i < 10; i++)
    scanf("%d", a + i);

  max = a[0];
  for (i = 0; i < 9; i++) {
    for (j = i + 1; j < 10; j++)
      if (a[j] > max) {
        max = a[j];
      }
  }

  printf("max=%d\n", max);
  return 0;
}


