//编程利用循环打印如下字符图形：
// 12345678987654321
// 234567898765432
//  3456789876543
//   45678987654
//    567898765
//     6789876
//      78987
//       898
//        9
#include <stdio.h>

int main() {
  const char str[] = "123456789";
  const int len = 9;
  int i = 0, j = 0;
  for (i = 0; i < len; i++) {
    for (j = 0; j < i; j++)
      printf(" ");
    for (j = i; j < len; j++) {
      printf("%c", str[j]);
    }
    for (j = len - 2; j >= i; j--) {
      printf("%c", str[j]);
    }
    printf("\n");
  }

  return 0;
}

