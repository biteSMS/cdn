#include <math.h>
#include <stdio.h>

int fun(int n) {
  int i, k = 1;
  if (n <= 1) {
    k = 0;
  }
  for (i = n - 1; i > 1; i--) {
    if (n % i == 0) {
      return 0;
    }
  }

  return k;
}

void main() {
  int m, k = 0;
  for (m = 1; m <= 100; m++) {
    if (fun(m) == 1) {
      printf("%4d", m);
      k++;
      if (k % 5 == 0) {
        printf("\n");
      }
    }
  }
}

