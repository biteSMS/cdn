#include <stdio.h>
int main() {
  int x, y, z, count = 0;
  for (x = 0; x <= 20; x++) {
    for (y = 0; y <= 33; y++) {
      for (z = 0; z <= 100; z++) {
        if (5 * x + 3 * y + (z / 3) == 100 && z % 3 == 0 && x + y + z == 100) {
          count++;
          printf("%2d:cock=%2d hen=%2d chicken=%2d\n", count, x, y, z);
        }
      }
    }
  }
}

