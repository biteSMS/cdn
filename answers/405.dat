#include <stdio.h>
int main() {
  int i = 1, x, y, z;
  for (x = 0; x <= 100; x++) {
    for (y = 0; y <= 100; y++) {
      z = (100 - 5 * x - 3 * y) * 3;
      if (x + y + (100 - 5 * x - 3 * y) * 3 == 100) {
        printf("%2d:cock=%2d hen=%2d chicken=%2d\n", i, x, y, z);
        i++;
      }
    }
  }
}

