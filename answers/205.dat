#include <stdio.h>
#include <stdlib.h>

int Search(int a[], int n, int x) {
  int i, flag = 0;
  for (i = 0; i < n; i++) {
    if (x == a[i]) {
      flag = 1;
      break;
    }
  }
  if (flag)
    return i;
  else
    return -1;
}

int main() {
  const int size = 10;
  int num[size], x, i, key;
  printf("Input 10 numbers:\n");
  for (i = 0; i < size; i++) {
    scanf("%d", &num[i]);
  }
  printf("Input x:\n");
  scanf("%d", &x);
  key = Search(num, size, x);
  if (-1 == key)
    printf("Not found!\n");
  else
    printf("Subscript of x is %d\n", key);
  return 0;
}
