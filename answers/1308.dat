[Answer 2:]

#include <math.h>
#include <stdio.h>
int main() {
  float x1, x0, f, f1;
  x1 = 1.5;
  do {
    x0 = x1;
    f = ((2 * x0 - 4) * x0 + 3) * x0 - 6;
    f1 = (6 * x0 - 8) * x0 + 3;
    x1 = x0 - f / f1;
  } while (fabs(x1 - x0) >= 1e-5);
  printf("方程的根=%6.2f\n", x1);
  return 0;
}

