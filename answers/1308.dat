#include <math.h>
#include <stdio.h>
int main() {
  int i = 0;
  double x1 = 1.5, x2 = 0; //迭代初值
  while (fabs(x2 - x1) >= 1e-5) {
    x1 = x1 -
         (2 * x1 * x1 * x1 - 4 * x1 * x1 + 3 * x1 - 6) /
             (6 * x1 * x1 - 8 * x1 + 3);
    x2 = x1 -
         (2 * x1 * x1 * x1 - 4 * x1 * x1 + 3 * x1 - 6) /
             (6 * x1 * x1 - 8 * x1 + 3);

    i++;
  }
  printf("方程的根=%6.2f\n", x2);
  return 0;
}

