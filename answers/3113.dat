#include <stdio.h>
#include <stdlib.h>

int CountDigit(int number, int digit);
int main() {
  int m, n;
  printf("Input m,n:\n");
  scanf("%d,%d", &m, &n);

  printf("%d\n", CountDigit(m, n));

  return 0;
}
int CountDigit(int number, int digit) {
  int sum = 0;
  int i;

  for (i = 1; i <= number; i *= 10) {
    if ((number / i) % 10 == digit)
      sum++;
  }

  return sum;
}

