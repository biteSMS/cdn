#include <stdio.h>
int CountDigit(int number, int digit) {
  int a = 0, temp_2 = number, temp_1 = 0;
  do {
    temp_1 = temp_2 % 10;
    if (temp_1 == digit) {
      a++;
    }
    temp_2 /= 10;
  } while (temp_2 != 0);
  return a;
}
int main() {
  int m, n;
  printf("Input m,n:\n");
  scanf("%d,%d", &m, &n);
  // printf("%d\t",n);
  printf("%d\n", CountDigit(m, n));
  return 0;
}

