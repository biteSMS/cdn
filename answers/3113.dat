#include <stdio.h>

int coutdigit(int integer, int digit) {
  char s[20];
  sprintf(s, "%d", integer); //将整数转换为字符串
  int i = 0;
  int current, numberOfDigit = 0;
  while (s[i]) {
    current = s[i] - '0';
    if (current == digit)
      numberOfDigit++;
    i++;
  }
  return numberOfDigit;
}

int main() {
  int integer, digit;
  printf("Input m,n:\n");
  scanf("%d,%d", &integer, &digit);
  int numberOfDigit = coutdigit(integer, digit);
  printf("%d\n", numberOfDigit);
  return 0;
}

