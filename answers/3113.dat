#include <math.h>
#include <stdio.h>
#include <stdlib.h>

int CountDigit(int number, int digit) {
  int i, k, a[100], l = 0;
  for (i = 0; number > 0; i++) {
    k = number % 10;
    a[i] = k;
    number = number / 10;
  }
  a[i] = '\0';
  for (i = 0; a[i] != '\0'; i++) {
    if (a[i] == digit) {
      l++;
    }
  }
  return l;
}
int main() {
  int m, n, l;
  printf("Input m,n:\n");
  scanf("%d,%d", &m, &n);
  l = CountDigit(m, n);
  printf("%d\n", l);
  return 0;
}

