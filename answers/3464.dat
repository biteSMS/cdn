#include <math.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
main() {
  int j = 1;
  long x, i;
  printf("请输入一个数表示范围:\n");
  scanf("%ld", &x);
  printf("结果是:");
  for (i = 0; i <= x; i++) {
    while (j <= i)
      j *= 10;
    if (i * i % j == i) {
      printf("%5ld", i);
    }
  }
  printf("\n");
}


