#include <stdio.h>
#include <stdlib.h>
int Fun(int m) {
  int i, flag = 1;
  if (m == 2)
    return 1;
  for (i = 2; i < sqrt(m) + 1; i++) {
    if (m % i == 0) {
      flag = 0;
      break;
    }
  }
  if (flag) {
    return 1;
  } else {
    return 0;
  }
}
int main() {
  int x, flag;
  printf("Please input a number:\n");
  do {
    scanf("%d", &x);
  } while (x < 2);
  flag = Fun(x);
  if (flag) {
    printf("%d is a prime number\n", x);
  } else {
    printf("%d is not a prime number\n", x);
  }
  return 0;
}

