#include <stdio.h>
int Fun(int num);

int main() {
  int num, result;

  printf("Please input a number:\n");

  do {
    scanf("%d", &num);
  } while (num <= 1);

  result = Fun(num);

  if (result == 1) {
    printf("%d is a prime number\n", num);
  } else {
    printf("%d is not a prime number\n", num);
  }

  return 0;
}

int Fun(int num) {
  int i = 1, n = 0;

  while (i <= num / 2) {
    if (num % i == 0)
      n++;
    i++;
  }
  if (n == 1)
    return 1;
  else
    return 0;
}

