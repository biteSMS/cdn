int MinCommonMultiple(int a, int b) {
  int i;
  if (a <= b)
    i = b;
  else
    i = a;
  for (; i <= a * b; i++) {
    if (i % a == 0 && i % b == 0) {
      return i;
      break;
    }
  }
}

