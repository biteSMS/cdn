#include <stdio.h>
main() {
  int n, i = 1;
  printf("This program prints a table of squares.\n"
         "Enter number of entries in table:\n");
  scanf("%d%d", &n);
  while (i > 0 && i <= n) {
    printf("%10d%10d\n", i, i * i);
    i++;
  }
}

