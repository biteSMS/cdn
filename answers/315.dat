#include <stdio.h>
int Search(int a[], int n, int x) {
  int i, flag = -1;
  for (i = 0; flag == -1 && i < n; i++) {
    if (a[i] == x) {
      flag = i;
    }
  }
  return flag;
}
int main() {
  int str[10];
  int x, i;
  printf("Input 10 numbers:\n");
  for (i = 0; i < 10; i++) {
    scanf("%d", &str[i]);
  }
  printf("Input x:\n");
  scanf("%d", &x);
  Search(str, 10, x) == -1
      ? printf("Not found!\n")
      : printf("Subscript of x is %d\n", Search(str, 10, x));
}

