#include <math.h>
#include <stdio.h>

void main() {
  char c[100] = {0};
  int i;
  do {
    printf("请输入一个字符串(长度为[3..5]个字符)：\n");
    gets(c);
    for (i = 0; i < 100; i++) {
      if (c[i] == '\0')
        break;
    }
  } while (i < 3 || i > 5);
  c[i] = '\0';
  printf("你输入的字符串为：%s", c);
}

