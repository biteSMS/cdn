#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define N 80
int main() {
  char str[N];
  do {
    printf("请输入一个字符串(长度为[3..5]个字符)：");
    gets(str);
    printf("\n");
  } while (strlen(str) < 3 || strlen(str) > 5);
  printf("你输入的字符串为：%s", str);
}

