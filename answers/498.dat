#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main() {
  char a[10];
  do {
    printf("请输入一个字符串(长度为[3..5]个字符)：\n");
    gets(a);
    int i = strlen(a);
    if (i >= 3 && i <= 5) {
      printf("你输入的字符串为：%s", a);
      break;
    }
  } while (1);
  return 0;
}
