#include <stdio.h>
struct date_rec {
  int day;
  int month;
  int year;
};
int main() {
  printf("请输入当前日期（年 月 日）：");
  struct date_rec a;
  struct date_rec b;
  scanf("%d%d%d", &a.year, &a.month, &a.day);
  printf("请输入当前日期（年 月 日）：");
  scanf("%d%d%d", &b.year, &b.month, &b.day);
  printf("当前日期：%d年%d月%d日！\n", a.year, a.month, a.day);
  printf("当前日期：%d年%d月%d日！\n", b.year, b.month, b.day);
  if (a.year < b.year) {
    printf("第一个日期早于第二个日期！");
  } else if (a.year > b.year) {
    printf("第一个日期晚于第二个日期！");
  } else {
    if (a.month < b.month)
      printf("第一个日期早于第二个日期！");
    else if (a.month > b.month)
      printf("第一个日期晚于第二个日期！");
    else {
      if (a.day < b.day)
        printf("第一个日期早于第二个日期！");
      else if (a.day > b.day)
        printf("第一个日期晚于第二个日期！");
      else
        printf("两个日期相同！");
    }
  }
  return 0;
}

