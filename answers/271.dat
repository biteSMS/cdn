#include <stdio.h>
int IsLeapYear(int y) {
  if (!(y % 400))
    return 1;
  else if ((!(y % 4)) && (y % 100))
    return 1;
  return 0;
}
int main() {
  int year;
  printf("Input year:\n");
  scanf("%d", &year);
  if (IsLeapYear(year))
    printf("Yes");
  else
    printf("No");
  return 0;
}

