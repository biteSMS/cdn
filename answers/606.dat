#include <stdio.h>
#include <stdlib.h>

#include <string.h>
int find(char *a) {
  int flag = -1;
  int i;
  char b[7][10] = {"Sunday",   "Monday", "Tuesday", "Wednesday",
                   "Thursday", "Friday", "Saturday"};
  for (i = 0; i < 7; i++) {
    if (strcmp(a, b[i]) == 0) {
      flag = i;
      break;
    }
  }
  return flag;
}
int main() {
  char a[20];
  int m;
  printf("Please enter a string:\n");
  scanf("%s", a);

  m = find(a);
  if (0 == m)
    m += 7;
  if (m == -1)
    printf("Not found!\n");
  else
    printf("%s is %d\n", a, m);

  return 0;
}

