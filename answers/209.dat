#include <stdio.h>
int main() {
  float a, b, c, area, t;
  printf("Input a,b,c:");
  scanf("%f,%f,%f", &a, &b, &c);
  if (a + b > c && a + c > b && b + c > a) {

    t = (a + b + c) / 2;
    area = sqrt(t * (t - a) * (t - b) * (t - c));
    printf("area=%.2f\n", area);
  } else {
    printf("area=-1.#J");
  }

  return 0;
}

