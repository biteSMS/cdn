#include <math.h>
#include <stdio.h>
int main() {
  float a, b, c;
  float s, area;
  printf("Input a,b,c:");
  scanf("%f,%f,%f", &a, &b, &c);
  s = 1.0 / 2.0 * (a + b + c);
  area = sqrt(s * (s - a) * (s - b) * (s - c));
  printf("area=%.2f\n", area);
  return 0;
}

