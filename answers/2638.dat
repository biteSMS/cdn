    printf("t=%f\tLower weight!\n", t);
  } else if (t <= 25 && t >= 18) {
    printf("t=%f\tStandard weight!\n", t);
  } else if (t > 25 && t < 27) {
    printf("t=%f\tHigher weight!\n", t);
  } else {
    printf("t=%f\tToo fat!\n", t);
  }
  return 0;
}


