/**
写一个函数days，实现主函数将年、月、日（结构体类型）传递给days函数，days函数计算该年该月该日是该年的第几天并传回主函数输出。
输入提示："请输入日期（年，月，日）\n"
输入格式："%d,%d,%d"
输出格式："\n%d月%d日是%d年的第%d天。"
*/
#include <stdio.h>

int days(int year, int month, int day, int Days[2][12]);
int main() {
  int Days[2][12] = {{31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31},
                     {31, 29, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31}};

  int year;
  int month;
  int day;
  int sumday = 0;

  printf("请输入日期（年，月，日）\n");
  scanf("%d,%d,%d", &year, &month, &day);
  sumday = days(year, month, day, Days);

  printf("\n%d月%d日是%d年的第%d天。\n", month, day, year, sumday);
  return 0;
}
int days(int year, int month, int day, int Days[2][12]) {
  int i, j;
  int sum = 0;

  for (i = j = 0; i < month - 1; i++) {
    if (year % 400 == 0 || (year % 4 == 0 && year % 100 != 0))
      j++;
    sum += Days[0][i];
  }
  sum += day;
  return sum;
}

