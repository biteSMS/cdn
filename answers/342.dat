#include <stdio.h>
int main() {
  int n, s = 0, i, p = 0;
  float a;
  for (i = 0; i < 10; i++) {
    printf("Input integer:");
    scanf("%d", &n);
    if (n > 0) {
      p++;
      s += n;
    }
  }
  a = (float)s / p;
  printf("Plus number:%d,average value:%.2f", p, a);
  return 0;
}

