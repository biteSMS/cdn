#include <stdio.h>
int main() {
  int a[10];
  int i, sum = 0;
  for (i = 0; i < 10; i++) {
    printf("Input integer:");
    scanf("%d", &a[i]);
  }
  int flag = 0;
  for (i = 0; i < 10; i++) {
    if (a[i] >= 0) {
      sum = sum + a[i];
      flag++;
    }
  }
  printf("Plus number:%d,average value:%.2f", flag - 1,
         (float)sum / (flag - 1));
  if (flag == 0)
    printf("Plus number:0,average value: 0");
  return 0;
}


