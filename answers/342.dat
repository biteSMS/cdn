#include <stdio.h>
#include <stdlib.h>

int main() {
  int num[10], i, num1 = 0, sum = 0;
  for (i = 0; i < 10; i++) {
    printf("Input integer:");
    scanf("%d", &num[i]);
  }
  for (i = 0; i < 10; i++) {
    if (num[i] > 0) {
      sum += num[i];
      num1++;
    }
  }
  printf("Plus number:%d,average value:%.2f", num1, (float)sum / num1);
  return 0;
}

