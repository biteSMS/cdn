#include <stdio.h>

void Squeeze(char s[], char c);

main() {
  char a[80], c;

  gets(a);
  c = getchar();
  Squeeze(a, c);
  printf("%s\n", a);
}

void Squeeze(char s[80], char c) {
  int i, j = 0;

  for (i = 0; s[i] != '\0'; i++) {
    if (s[i] != c) {
      s[j] = s[i];
      j++;
    }
  }
  s[j] = '\0';
}

