#include <stdio.h>
int main() {
  int upper, step;
  float c; // 1
  int f = (-40);
  upper = 110; // 1
  step = 10;   // 1

  while (f <= upper) // 2
  {
    c = 5.0 / 9 * (f - 32);       // 1
    printf("%4d\t%6.1f\n", f, c); // 1
    f = f + step;                 // 1
  }

  return 0;
}

