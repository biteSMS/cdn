#include <stdio.h>
#include <stdlib.h>
int main() {
  float x, y;
  printf("Please input x:\n");
  scanf("%f", &x);
  if (x < 1) {
    y = x;
  } else if (1 <= x && x < 10) {
    y = -1 / x - 1;
  } else {
    y = 5 * x - 11;
  }
  printf("y=%.2f\n", y);
  return 0;
}
