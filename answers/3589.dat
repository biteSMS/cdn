int main() {
  int a;
  printf("Please enter the number:\n");
  scanf("%d", &a);
  if (a >= 1000 & a <= 9999)
    printf("%d: 1000-9999\n", a);
  else if (a <= 999 & a >= 100)
    printf("%d: 100-999\n", a);
  else if (a < 100 & a >= 10)
    printf("%d: 10-99\n", a);
  else if (a >= 0 & a < 10)
    printf("%d: 0-9\n", a);
  else
    printf("error!\n");
  return 0;
}


