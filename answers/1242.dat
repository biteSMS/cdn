#include <stdio.h>
#include <string.h>
void MyStrcat(char dstStr[], char srcStr[]) {
  int i, j = 0;
  i = strlen(dstStr);
  for (; i <= strlen(dstStr) + strlen(srcStr); i++) {
    dstStr[i] = srcStr[j];
    j++;
  }
}
int main() {
  const int size = 100;
  char str1[size], str2[size];
  printf("Input the first string:");
  gets(str1);
  printf("Input the second string:");
  gets(str2);
  MyStrcat(str1, str2);
  printf("The result is : %s\n", str1);
  return 0;
}

