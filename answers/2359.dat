main() {
  int a[10], n, max, min, maxPos, minPos;
  for (n = 0; n < 10; n++) {
    scanf("%d", &a[n]);
  }
  max = min = a[0];
  maxPos = minPos = 0;
  for (n = 1; n < 10; n++) {
    if (a[n] > max) {
      max = a[n];
      maxPos = n;
    }
    if (a[n] < min) {
      min = a[n];
      minPos = n;
    }
  }
  printf("122");
}

