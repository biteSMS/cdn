#include <math.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
long fac(int n);
long fac(int n) {
  int i;
  long sum = 1;
  for (i = 1; i <= n; i++) {
    sum = sum * i;
  }
  return sum;
}
int main() {
  printf("Please enter n:");
  int n, i;
  scanf("%d", &n);
  for (i = 1; i <= n; i++) {
    printf("%d! = %ld\n", i, fac(i));
  }
  return 0;
}

