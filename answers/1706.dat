#include <stdio.h>
int main() {
  int n, a, b, c, d, m, sum;
  printf("Input data is:");
  scanf("%d", &m);
  n = fabs(m);
  a = n / 1000;
  b = (n - (a * 1000)) / 100;
  c = (n - a * 1000 - b * 100) / 10;
  d = n % 10;
  sum = a + b + c + d;
  printf("The sum of the total bit is %d\n", sum);
  return 0;
}

