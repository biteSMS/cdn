main() {
  int i;
  printf("Input data is:");
  scanf("%d", &i);
  if (i > 0) {
    printf("The sum of the total bit is %d\n",
           i % 10 + i / 10 % 10 + i / 100 % 10 + i / 1000 % 10);
  } else {
    i = -i;
    printf("The sum of the total bit is %d\n",
           i % 10 + i / 10 % 10 + i / 100 % 10 + i / 1000 % 10);
  }
}

