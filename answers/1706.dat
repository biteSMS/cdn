#include <stdio.h>
int main() {
  int i, x, s = 0;
  int a[4];
  printf("Input data is:");
  scanf("%d", &x);
  if (x < 0)
    x = -x;
  for (i = 0; i < 4; i++) {
    a[i] = x % 10;
    x /= 10;
  }
  for (i = 0; i < 4; i++) {
    s += a[i];
  }
  printf("The sum of the total bit is %d\n", s);
}

