#include <math.h>
#include <stdio.h>
int main() {
  int x, s = 0;
  printf("Input data is:");
  scanf("%d", &x);
  x = fabs(x);
  for (; x > 0;) {
    s += x % 10;
    x /= 10;
  }
  printf("The sum of the total bit is %d\n", s);
  return 0;
}

