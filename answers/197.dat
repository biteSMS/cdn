#include <stdio.h>
int main() {
  float t, w, h;
  printf("Please enter h,w:\n");
  scanf("%f,%f", &h, &w);
  t = w / (h * h);
  if (t < 18) {
    printf("t=%f\tLower weight!\n", t);
  } else if (t < 25 && t >= 18) {
    printf("t=%f\tStandard weight!\n", t);
  } else if (t < 27 && t >= 25) {
    printf("t=%f\tHigher weight!\n", t);
  } else if (t >= 27) {
    printf("t=%f\tToo fat!\n", t);
  }
}

