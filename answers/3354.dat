#include <stdio.h>

int main() {
  char sex;
  int age, flag = 0;
  printf("please input sex:\n");
  sex = getchar();
  printf("please input age:\n");
  scanf(" %d", &age);

  switch (sex) {
  case 'M':
    if (age >= 60)
      flag = 1;
  case 'F':
    if (age >= 55)
      flag = 1;
  }
  if (flag == 1)
    printf("retired\n");
  else
    printf("not retired\n");
  return 0;
}

