#include <stdio.h>

int main() {
  int age;
  char sex;
  printf("please input sex:\n");
  scanf("%c", &sex);
  printf("please input age:\n");
  scanf("%d", &age);
  if (sex == 'M') {
    if (age >= 60)
      printf("retired");
    else
      printf("not retired");
  } else {
    if (age >= 55)
      printf("retired");
    else
      printf("not retired");
  }
}

