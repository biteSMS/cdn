#include <stdio.h>
long fact(int n);
int main() {
  int n;
  long result = 0;
  printf("Input n:");
  scanf("%d", &n);
  result = fact(n);
  printf("%d != %ld", n, result);
}
long fact(int n) {
  if (n < 0)
    printf("n<0,data error!\n");
  else {
    if (1 == n)
      return 1;
    else
      return n * fact(n - 1);
  }
}

