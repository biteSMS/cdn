#include <stdio.h>
int Search(int a[], int n, int x) {
  int i = 0, count = 0;
  do {
    if (x == a[i]) {
      count++;
    }
    i++;
  } while (count == 0 && i < n);
  if (count == 0) {
    return -1;
  } else {
    return i - 1;
  }
}
int main() {
  const int size = 10;
  int a[size], x, r, i;

  printf("Input 10 numbers:\n");
  for (i = 0; i < size; i++) {
    scanf("%d", &a[i]);
  }

  printf("Input x:\n");
  scanf("%d", &x);

  r = Search(a, size, x);

  if (r == -1) {
    printf("Not found!\n");
  } else {
    printf("Subscript of x is %d\n", r);
  }
}

