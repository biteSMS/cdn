#include <stdio.h>
int Search(int a[], int n, int x);
int main() {
  int x, a[10], i, n = 10;
  printf("Input 10 numbers:\n");
  for (i = 0; i < 10; i++) {
    scanf("%d", &a[i]);
  }
  printf("Input x:\n");
  scanf("%d", &x);
  if (-1 == Search(a, n, x))
    printf("Not found!\n");
  else
    printf("Subscript of x is %d\n", Search(a, n, x));
  return 0;
}
int Search(int a[], int n, int x) {
  int i;
  for (i = 0; i < n; i++) {
    if (x == a[i])
      return i;
  }
  return -1;
}

