#include <stdio.h>

int gcd(int a, int b) {
  while (b != 0) {
    int temp = a % b;
    a = b;
    b = temp;
  }

  return a;
}

int main() {
  printf("Input a,b:");
  int a = 0, b = 0;
  if ((scanf("%d,%d", &a, &b) == 2) && a > 0 && b > 0) {
    printf("%d\n", gcd(a, b));
  } else {
    printf("Input Error!\n");
  }
}

