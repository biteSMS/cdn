#include <stdio.h>

int MaxCommonFactor(int a, int b);

int main() {
  int a, b, x;
  printf("Input a,b:");
  scanf("%d,%d", &a, &b);
  x = MaxCommonFactor(a, b);
  if (x < 0)
    printf("Input Error!\n");
  printf("%d\n", x);
  return 0;
}

int MaxCommonFactor(int a, int b) {
  int remainder;
  remainder = a % b;
  while (remainder) {
    a = b;
    b = remainder;
    remainder = a % b;
  }
  return b;
}

