int MaxCommonFactor(int a, int b) {
  int c;
  if (a <= 0 || b <= 0)
    return -1;
  else if (a == b)
    return a;
  else if (a > b) {
    c = (a - b) % b;
    while (c != 0) {
      a = b;
      b = c;
      c = a % b;
    }
    return b;
  } else {
    c = (b - a) % b;
    while (c != 0) {
      a = b;
      b = c;
      c = a % b;
    }
    return b;
  }
}

