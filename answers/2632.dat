#include <stdio.h>
#include <stdlib.h>

int main() {
  int n;
  printf("Input n:\n");
  scanf("%d", &n);
  if (n % 2 == 0)
    printf("This is an even number.");
  else
    printf("This is an odd number.");
  return 0;
}

