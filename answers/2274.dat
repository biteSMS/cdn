#include <stdio.h>
char stap(char string[]);
int main() {
  char string[100];
  char s;
  printf("input a string:");
  scanf("%s", string);
  s = stap(string);
  printf("string=%s\n", string);
  return 0;
}
char stap(char string[]) {
  int i = 0;
  for (i = 0; string[i] != NULL; i++) {
    if (string[i] == 'a')
      string[i] = '*';
  }
  return string;
}

