#include <stdio.h>
#include <stdlib.h>

int main() {
  const int size = 50;
  int i;
  printf("input a string:");
  char s[size];
  scanf("%s", s);
  for (i = 0; s[i] != '\0'; i++) {
    if (s[i] == 'a')
      s[i] = '*';
  }
  printf("string=%s\n", s);

  return 0;
}

