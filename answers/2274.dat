#include <string.h>
void replace(char *p);
main() {
  char s[20];
  printf("input a string:");
  gets(s);
  replace(s);
  printf("string=%s\n", s);
}
void replace(char *p) {
  int i;
  for (i = 0; i < strlen(p); i++) {
    if (p[i] == 'a') {
      p[i] = '*';
    }
  }
}

