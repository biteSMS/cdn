#include <stdio.h>

int main() {

  int year, month, day;
  int date[12];
  int i, j;
  int sum = 0;
  scanf("%d%d%d", &year, &month, &day);
  for (i = 0; i < 12; i++) {
    if (i == 0 || i == 2 || i == 4 || i == 6 || i == 7 || i == 9 || i == 11)
      date[i] = 31;
    else if (i == 1) {
      if (year % 4 == 0 && year % 100 != 0 || year % 400 == 0)
        date[i] = 29;
      else
        date[i] = 28;
    } else
      date[i] = 30;
  }
  for (j = 0; j < 12; j++) {
    if (month > j + 1) {
      sum += date[j];
    }
    if (month == j + 1) {
      if (date[j] >= day)
        sum += day;
    }
  }
  printf("%d", sum);
  return 0;
}

