#include <stdio.h>
#include <stdlib.h>

#include <math.h>
double funt(double x) { return pow(x, 2) - 5 * x + 4; }

int main() {
  double x;
  printf("Input x: ");
  scanf("%lf", &x);

  printf("x =\t%5.2f\n", x);
  printf("y1=\t%5.2f\n", funt(x));
  printf("y2=\t%5.2f\n", funt(x + 15));
  printf("y3=\t%5.2f\n", funt(sin(x)));

  return 0;
}

