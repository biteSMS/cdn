/*以下程序的功能是计算十个数据的平均值。找出其中的错误并改正之。*/
#include <stdio.h>
void main(void) {
  int i, sum = 0;
  float avg;
  int sc[10], *p = sc;
  for (i = 0; i < 10; i++) {
    scanf("%d", p);
    sum += *p;
    p++;
  }
  avg = (float)sum / 10;
  printf("avg=%f\n", avg);
}

