#include <stdio.h>
#define N 3
#define M 5
int main() {
  int score[M][N], num[M], sum[M], averge[M], a[M];
  int i, j, b;
  for (i = 0; i < M; i++) {
    printf("Enter No. and score as: MT EN PH\n");
    b = 0;
    scanf("%d", &num[i]);
    for (j = 0; j < N; j++) {
      scanf("%d", &score[i][j]);
      if (score[i][j] > 90)
        b++;
    }
    a[i] = b;
  }
  for (i = 0; i < M; i++) {
    sum[i] = 0;
    for (j = 0; j < N; j++)
      sum[i] = sum[i] + score[i][j];
  }
  for (i = 0; i < M; i++) {
    averge[i] = sum[i] / N;
  }
  printf("NO\tMT\tEN\tPH\tSUM\tV\t>90\n");
  printf("------------------------------------------------------\n");
  for (i = 0; i < M; i++) {
    printf("%d\t", num[i]);
    for (j = 0; j < N; j++) {
      printf("%d\t", score[i][j]);
    }
    printf("%d\t", sum[i]);
    printf("%d\t", averge[i]);
    if (a[i] == N)
      printf("Y");
    else
      printf("N");
    printf("\n");
  }
  return 0;
}

