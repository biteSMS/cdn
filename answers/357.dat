#include <stdio.h>
#include <string.h>
#define N 4
#define M 5
int main() {
  int i, j, flag;
  char c[M];
  int juzhen[M][N], sum[M], ave[M];
  for (i = 0; i < M; i++) {
    printf("Enter No. and score as: MT EN PH\n");
    for (j = 0; j < N; j++) {
      scanf("%d", &juzhen[i][j]);
    }
  }
  printf("NO\tMT\tEN\tPH\tSUM\tV\t>90\n");
  printf("------------------------------------------------------\n");
  for (i = 0; i < M; i++) {
    sum[i] = 0;
    ave[i] = 0;
    flag = 0;
    for (j = 1; j < N; j++) {
      sum[i] = sum[i] + juzhen[i][j];
      ave[i] = sum[i] / 3;
      if (juzhen[i][j] > 90)
        flag++;
    }
    if (flag == N - 1)
      c[i] = 'Y';
    else
      c[i] = 'N';
  }
  for (i = 0; i < M; i++) {
    printf("%d", i + 1);
    for (j = 1; j < N; j++)
      printf("\t%d", juzhen[i][j]);
    printf("\t%d\t%d\t%c\n", sum[i], ave[i], c[i]);
  }
  return 0;
}

