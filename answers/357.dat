#include <math.h>
#include <stdio.h>
#define N 5
typedef struct student {
  int NO;
  int MT;
  int EN;
  int PH;
  int SUM;
  int V;
  char over90;
} stu;
void main() {
  stu s[N];
  int i;
  for (i = 0; i < N; i++) {
    printf("Enter No. and score as: MT EN PH\n");
    scanf("%d %d %d %d", &(s[i].NO), &(s[i].MT), &(s[i].EN), &(s[i].PH));
    s[i].SUM = s[i].MT + s[i].EN + s[i].PH;
    s[i].V = s[i].SUM / 3;
    if (s[i].MT > 90 && s[i].EN > 90 && s[i].PH > 90)
      s[i].over90 = 'Y';
    else
      s[i].over90 = 'N';
  }
  printf("NO\tMT\tEN\tPH\tSUM\tV\t>90\n");
  printf("------------------------------------------------------\n");
  for (i = 0; i < N; i++) {
    printf("%d\t%d\t%d\t%d\t%d\t%d\t%c\n", s[i].NO, s[i].MT, s[i].EN, s[i].PH,
           s[i].SUM, s[i].V, s[i].over90);
  }
}


