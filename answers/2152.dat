#include <stdio.h>
#include <stdlib.h>
int number(int a, int b) {
  int cf, i;
  cf = a > b ? a : b;
  for (i = cf;; i++) {
    if (i % a == 0 && i % b == 0)
      break;
  }
  return i;
}
int main() {
  int a, b;
  printf("Input a,b:");
  scanf("%d,%d", &a, &b);
  printf("MinCommonMultiple = %d\n", number(a, b));
}

