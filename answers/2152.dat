int gcd(int a, int b) {
  int m = a > b ? b : a;
  int r;
  for (r = m; r > 0; r--)
    if (a % r == 0 && b % r == 0)
      break;
  return r;
}

int main() {
  int a, b, g;
  printf("Input a,b:");
  scanf("%d,%d", &a, &b);
  g = gcd(a, b);
  printf("MinCommonMultiple = %d\n", a / g * b);
}

