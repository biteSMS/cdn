#include <stdio.h>
#include <stdlib.h>

int main() {
  int a[3][4] = {1, 2, 3, 4, 9, 8, 7, 6, 10, -1, -4, 4};
  int i, j, max = 0;

  for (i = 0; i < 3; i++) {
    for (j = 0; j < 4; j++) {
      if (a[i][j] > max) {
        max = a[i][j];
      }
    }
  }
  printf("max=%d\n", max);
  return 0;
}

