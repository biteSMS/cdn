#include <stdio.h>
#include <stdlib.h>
int main() {
  int i, j;
  for (i = 1; i <= 4; i++) {
    for (j = 1; j <= 5 + i; j++) {
      if (j < i) {
        printf(" ");
      } else {
        printf("*");
      }
    }
    printf("\n");
  }
  return 0;
}

