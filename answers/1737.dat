#include <stdio.h>

int main() {
  int i, j;
  for (i = 1; i <= 4; i++) {
    switch (i) {
    case 2:
      printf(" ");
      break;
    case 3:
      printf("  ");
      break;
    case 4:
      printf("   ");
      break;
    default:
      break;
    }
    for (j = 1; j <= 6; j++) {
      printf("*");
    }

    printf("\n");
  }

  return 0;
}

