#include <stdio.h>
#include <string.h>
main() {
  char ch[20], c[20], temp;
  int i, j;

  printf("Input a string:\n");
  gets(c);
  strcpy(ch, c);
  for (i = 0; ch[i - 1] != '\0'; i++) {
    for (j = i + 1; ch[j] != '\0'; j++) {
      if (ch[i] < ch[j]) {
        temp = ch[i];
        ch[i] = ch[j];
        ch[j] = temp;
      }
    }
  }

  printf("The largest character of \"%s\" is \'%c\' ,The ASCII is %d.", c,
         ch[0], ch[0]);
}

