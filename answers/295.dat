#include <stdio.h>
char FindMaxChar(char *str) {
  char tmp = str[0];
  int i = 0;
  while (str[i] != '\0') {
    if (str[i] > tmp)
      tmp = str[i];
    i++;
  }
  return tmp;
}
int main() {
  char str[80];
  char max;
  printf("Input a string:\n");
  gets(str);
  max = FindMaxChar(str);
  printf("The largest character of \"%s\" is \'%c\' ,The ASCII is %d.", str,
         max, max);
  return 0;
}

