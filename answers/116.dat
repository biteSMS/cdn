#include <stdio.h>
#define ROW 2
#define COL 3
void main() {
  int a[ROW][COL], b[COL][ROW], c[ROW][ROW], i, j, k;
  printf("Input 2*3 matrix a:\n");
  for (i = 0; i < ROW; i++) {
    for (j = 0; j < COL; j++) {
      scanf("%d", &a[i][j]);
    }
  }
  printf("Input 3*2 matrix b:\n");
  for (i = 0; i < COL; i++) {
    for (j = 0; j < ROW; j++) {
      scanf("%d", &b[i][j]);
    }
  }
  for (i = 0; i < ROW; i++) {
    for (j = 0; j < ROW; j++) {
      c[i][j] = 0;
      for (k = 0; k < COL; k++) {
        c[i][j] = c[i][j] + a[i][k] * b[k][j];
      }
    }
  }
  printf("Results:\n");
  for (i = 0; i < ROW; i++) {
    for (j = 0; j < ROW; j++) {
      printf("%6d", c[i][j]);
    }
    printf("\n");
  }
}

