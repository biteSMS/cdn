#include <stdio.h>
#define N 10
void exchange(char *str) {
  int i, j, num = 0, max;
  char temp;
  int l;
  l = strlen(str);
  max = str[0];
  for (i = 1; i < l; i++) {
    if (str[i] > max) {
      max = str[i];
      num = i;
    }
  }
  for (j = num; j > 0; j--) {
    temp = str[j];
    str[j] = str[j - 1];
    str[j - 1] = temp;
  }
}
int main() {
  char str[N];
  gets(str);
  exchange(str);
  puts(str);
}

