#include <stdio.h>
#include <string.h>
#define N 30
int main() {
  char *s;
  char a[N], ch;
  s = a;
  int i, n, m = 0;

  gets(s);
  n = strlen(s);
  for (i = 1; i < n; i++) {
    if (a[i] > a[m]) {
      m = i;
    }
  }
  ch = s[m];
  for (i = m; m > 0; m--) {
    s[m] = s[m - 1];
  }
  s[0] = ch;
  puts(s);
  return 0;
}

