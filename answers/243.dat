#include <stdio.h>
#include <stdlib.h>

int Fail(int *a, int n) {
  int flag = 0;
  int i;

  for (i = 0; i < n; i++) {
    if (*(a + i) < 60)
      flag++;
  }

  return flag;
}
int main() {
  int a[40] = {};
  int n, i;
  printf("How many students?");
  scanf("%d", &n);

  for (i = 0; i < n; i++) {
    scanf("%d", a + i);
  }

  printf("Fail students = %d\n", Fail(a, n));
  return 0;
}

