#include <math.h>
#include <stdio.h>
void InputArray(int *p, int n) {
  int i;
  for (i = 0; i < n; i++) {
    scanf("%d", &p[i]);
  }
}
int FailStudents(int *p, int n) {
  int j, num = 0;
  for (j = 0; j < n; j++) {
    if (p[j] < 60) {
      num++;
    }
  }
  return num;
}
int main() {
  int *p = NULL;
  int n, num;

  printf("How many students?");
  scanf("%d", &n);
  p = (int *)malloc(n * sizeof(int));
  if (p == NULL) {
    printf("No enough memory!\n");
    exit(1);
  }
  InputArray(p, n);
  num = FailStudents(p, n);
  printf("Fail students = %d\n", num);
  return 0;
}

