#include <stdio.h>
int main() {
  int count = 0, score[40], n, i;
  printf("How many students?");
  scanf("%4d", &n);
  for (i = 0; i <= n; i++) {
    scanf("%d", &score[i]);
    if (score[i] < 60) {
      count++;
    }
  }
  printf("Fail students = %d\n", count);
  return 0;
}

