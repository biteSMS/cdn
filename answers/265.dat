#include <stdio.h>

int main() {
  long long y = 0, k = 0, n = 0;
  scanf("%d%d%d", &y, &k, &n);

  long long i = 0;
  int has_ans = 0;
  for (i = 1; i <= n; i++) {
    if ((i % k == 0) && i > y) {
      printf("%d ", i - y);
      has_ans = 1;
    }
  }
  if (!has_ans) {
    printf("-1");
  }

  return 0;
}

