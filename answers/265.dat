#include <stdio.h>
#include <stdlib.h>
int main() {
  int y, k, n, x, i = 0, flage = 0, l;
  scanf("%d %d %d", &y, &k, &n);
  int a[100];
  for (x = 1; x <= n - y; x++) {
    if ((x + y) % k == 0) {
      a[i] = x;
      i++;
      flage = 1;
      l = i;
    }
  }
  if (flage == 1) {
    for (i = 0; i < l; i++) {
      printf("%d ", a[i]);
    }
  } else {
    printf("-1\n");
  }
  return 0;
}

