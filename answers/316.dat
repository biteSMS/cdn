#include <stdio.h>
#include <stdlib.h>

int main() {
  int matrix_a[2][3], matrix_b[3][2], result[2][2] = {0};
  int i, j, count;
  printf("Input 2*3 matrix a:\n");
  for (i = 0; i < 2; i++) {
    for (j = 0; j < 3; j++) {
      scanf("%d", &matrix_a[i][j]);
    }
  }
  printf("Input 3*2 matrix b:\n");
  for (i = 0; i < 3; i++) {
    for (j = 0; j < 2; j++) {
      scanf("%d", &matrix_b[i][j]);
    }
  }
  for (i = 0; i < 2; i++) {
    for (count = 0; count < 2; count++) {
      for (j = 0; j < 3; j++) {
        result[i][count] =
            result[i][count] + matrix_a[i][j] * matrix_b[j][count];
      }
    }
  }
  printf("Results:\n");
  for (i = 0; i < 2; i++) {
    for (j = 0; j < 2; j++) {
      printf("%6d", result[i][j]);
    }
    printf("\n");
  }
  return 0;
}

