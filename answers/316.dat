#include <stdio.h>

int main() {
  int i, j, k;
  int B[3][2];
  int A[2][3];
  int C[2][2] = {0, 0, 0, 0};

  printf("Input 2*3 matrix a:\n");

  for (i = 0; i < 2; i++) {
    for (j = 0; j < 3; j++) {
      scanf("%d", &A[i][j]);
    }
  }
  printf("Input 3*2 matrix b:\n");
  for (i = 0; i < 3; i++) {
    for (j = 0; j < 2; j++) {
      scanf("%d", &B[i][j]);
    }
  }

  for (i = 0; i < 2; i++) {
    for (j = 0; j < 2; j++) {
      C[i][j] = 0;
      for (k = 0; k < 3; k++) {
        C[i][j] += A[i][k] * B[k][j];
      }
    }
  }
  printf("Results:\n");
  for (i = 0; i < 2; i++) {
    for (j = 0; j < 2; j++) {
      printf("%6d", C[i][j]);
    }
    printf("\n");
  }
  return 0;
}

