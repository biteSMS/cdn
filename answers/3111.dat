#include <stdio.h>
#include <string.h>
void Squeeze(char s[], char c) {
  int i, n = strlen(s), j;
  for (i = 0; s[i] != '\0'; i++) {
    if (s[i] == c) {
      for (j = i; s[j] != '\0'; j++) {
        s[j] = s[j + 1];
      }
      i--;
    }
  }
}
void main() {
  char s[80], c;
  printf("Input a string:\n");
  gets(s);
  printf("Input a character:\n");
  c = getchar();
  Squeeze(s, c);
  printf("Results:%s\n", s);
}

