#include <stdio.h>
#define N 80

void Squeeze(char s[], char c) {
  int i = 0, j;
  do {
    if (c == s[i]) {
      j = i;
      do {
        s[j] = s[j + 1];
        j++;
      } while (s[j] != '\0');
      i--;
    }
    i++;
  } while (s[i] != '\0');
  printf("Results:%s\n", s);
}
int main() {
  char s[N];
  char ch;

  printf("Input a string:\n");
  gets(s);
  printf("Input a character:\n");
  ch = getchar();

  Squeeze(s, ch);
  return 0;
}

