#include <stdio.h>
main() {
  int a, b, c, d = 0;
  for (a = 0; a <= 50; a++) {
    for (b = 0; b <= 50; b++) {
      if (10 * a + b * 5 + 50 - a - b == 100) {
        d += 1;
        c = 50 - a - b;
        printf("x = %d, y = %d, z = %d\n", a, b, c);
      }
    }
  }
  printf("count = %d\n", d);
}

