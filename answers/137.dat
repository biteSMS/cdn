#include <stdio.h>

void main() {
  int count = 0, i, j, k;
  for (i = 0; i <= 10; i++) {
    for (j = 0; j <= 20; j++) {
      for (k = 0; i * 10 + j * 5 + k <= 100; k++) {
        if (i * 10 + j * 5 + k == 100 && i + j + k == 50) {
          printf("x = %d, y = %d, z = %d\n", i, j, k);
          count++;
        }
      }
    }
  }
  printf("count = %d\n", count);
}

