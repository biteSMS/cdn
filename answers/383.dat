#include <stdio.h>
typedef struct {
  int year;
  int month;
  int day;
} days;
int main() {
  days a;
  days *p = &a;
  int sum = 0;
  int i = 1990;
  printf("Enter year/month/day:");
  scanf("%d%d%d", &p->year, &p->month, &p->day);
  while (p->year != i) {
    if ((p->year % 4 == 0 && p->year % 100 != 0) || p->year % 400 == 0)
      sum += 366;
    else
      sum += 365;
    i++;
  }
  for (i = 1; i <= p->month; i++) {
    if (i != p->month) {
      switch (i) {
      case 1:
        sum += 31;
        break;
      case 2:
        if ((0 == p->year % 4 && p->year % 100 != 0) || 0 == p->year % 400)
          sum += 29;
        else
          sum += 28;
        break;
      case 3:
        sum += 31;
        break;
      case 4:
        sum += 30;
        break;
      case 5:
        sum += 31;
        break;
      case 6:
        sum += 30;
        break;
      case 7:
        sum += 31;
        break;
      case 8:
        sum += 31;
        break;
      case 9:
        sum += 30;
        break;
      case 10:
        sum += 31;
        break;
      case 11:
        sum += 30;
        break;
      }
    } else
      sum += p->day;
  }
  0 == sum % 3 ? printf("He is fishing.\n") : printf("He is sleeping.\n");
  return 0;
}

