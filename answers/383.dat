#include <stdio.h>
struct time {
  int year;
  int month;
  int day;
};
void Input_time(struct time *final_time) {
  printf("Enter year/month/day:");
  scanf("%d%d%d", &final_time->year, &final_time->month, &final_time->day);
}
void Output(struct time *final_time, struct time *start_time) {
  int num, sum;
  int month1[12] = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31},
      month2[12] = {31, 29, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};
  int i, out;
  num = (final_time->year - start_time->year) % 4 + 1;
  switch (num) {
  case 1:
    for (i = 0, sum = 0; i < final_time->month - 1; i++) {
      sum = sum + month1[i];
    }
    sum = sum + final_time->day;
    out = sum % 5;
    if (out >= 1 && out <= 3) {
      printf("He is fishing.\n");
    } else if (out == 0 || out == 4) {
      printf("He is sleeping.\n");
    }
    break;
  case 2:
    for (i = 0, sum = 0; i < final_time->month - 1; i++) {
      sum = sum + month1[i];
    }
    sum = sum + final_time->day + 365;
    out = sum % 5;
    if (out >= 1 && out <= 3) {
      printf("He is fishing.\n");
    } else if (out == 0 || out == 4) {
      printf("He is sleeping.\n");
    }
    break;
  case 3:
    for (i = 0, sum = 0; i < final_time->month - 1; i++) {
      sum = sum + month2[i];
    }
    sum = sum + final_time->day + 365 + 366;
    out = sum % 5;
    if (out >= 1 && out <= 3) {
      printf("He is fishing.\n");
    } else if (out == 0 || out == 4) {
      printf("He is sleeping.\n");
    }
    break;
  case 4:
    for (i = 0, sum = 0; i < final_time->month - 1; i++) {
      sum = sum + month1[i];
    }
    sum = sum + final_time->day + 365 + 366 + 365;
    out = sum % 5;
    if (out >= 1 && out <= 3) {
      printf("He is fishing.\n");
    } else if (out == 0 || out == 4) {
      printf("He is sleeping.\n");
    }
    break;
  }
}
int main() {
  struct time start_time = {{1990}, {1}, {1}};
  struct time final_time;
  Input_time(&final_time);
  Output(&final_time, &start_time);
  return 0;
}

