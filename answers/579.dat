#include <stdio.h>
#include <stdlib.h>

int main() {
  int year, month, day, date = 0, i;
  int Day[2][12] = {{31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31},
                    {31, 29, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31}};
  printf("\nplease input year,month,day\n");
  scanf("%d,%d,%d", &year, &month, &day);

  if ((year % 4 == 0 && year % 100 != 0) || year % 400 == 0) {
    for (i = 0; i < month - 1; i++) {
      date += Day[1][i];
    }
    date += day;
    printf("It is the %dth day.", date);
  } else {
    for (i = 0; i < month - 1; i++) {
      date += Day[0][i];
    }
    date += day;
    printf("It is the %dth day.", date);
  }

  return 0;
}

