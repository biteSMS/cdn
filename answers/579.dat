#include <stdio.h>

void fun(void);
int Leapyear(int year);

int main(void) {
  fun();

  return 0;
}

int Leapyear(int year) {
  if (year % 400 == 0 || (year % 4 == 0 && year % 100 != 0))
    return 1;
  else
    return 0;
}

void fun(void) {
  int year, month, day;
  int Monthlib[12] = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};
  int i, count;

  printf("\nplease input year,month,day\n");
  scanf("%d,%d,%d", &year, &month, &day);

  if (Leapyear(year))
    Monthlib[1] = 29;

  count = day;
  for (i = 0; i < month - 1; ++i)
    count += Monthlib[i];

  printf("It is the %dth day.", count);

  return;
}

