#include <stdio.h>
int main() {
  int i = 1, j = 1, k = 1, count = 0;
  for (i = 1; i <= 4; i++) {
    for (j = 1; j <= 4; j++) {
      for (k = 1; k <= 4; k++) {
        if (i != j && j != k && i != k) {
          count = count + 1;
        }
      }
    }
  }
  printf("counter=%d\n", count);
  for (i = 1; i <= 4; i++) {
    for (j = 1; j <= 4; j++) {
      for (k = 1; k <= 4; k++) {
        if (i != j && j != k && i != k) {
          printf("%d%d%d ", i, j, k);
        }
      }
    }
  }
  return 0;
}

