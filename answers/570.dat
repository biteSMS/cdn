#include <stdio.h>
int main() {
  int counter = 0, i, j, k, sum[80] = {0};
  for (i = 1; i <= 4; i++)
    for (j = 1; j <= 4; j++)
      for (k = 1; k <= 4; k++)
        if (i != k && j != k && i != j) {
          sum[counter] = i * 100 + j * 10 + k;
          counter++;
        }
  printf("counter=%d\n", counter);
  for (i = 0; i < counter; i++) {
    if (i == 0)
      printf("%d", sum[i]);
    else
      printf(" %d", sum[i]);
  }
  return 0;
}

