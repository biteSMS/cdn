#include <math.h>
#include <stdio.h>
int IsPrime(int x);
void OutputPrimeFactor(int x);
int main() {
  int m;
  printf("Input m:");
  scanf("%d", &m);
  if (IsPrime(m)) {
    printf("It is a prime number\n");
  } else {
    printf("%d = ", m);
    OutputPrimeFactor(m);
  }
  return 0;
}
int IsPrime(int x) {
  int i, flag = 1;
  int squareRoot = (int)sqrt(x);
  if (x <= 1)
    flag = 0;
  for (i = 2; i <= squareRoot && flag; i++) {
    if (x % i == 0)
      flag = 0;
  }
  return flag;
}
void OutputPrimeFactor(int x) {
  int i;
  for (i = 2; i < x; i++) {
    if (x % i == 0) {
      printf("%d * ", i);
      OutputPrimeFactor(x / i);
      return;
    }
  }
  printf("%d", x);
}

