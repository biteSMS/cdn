#include <stdio.h>
int judge(int m) {
  int n;
  for (n = 2; n < m; n++) {
    if (m % n != 0)
      continue;
    else
      return su(m);
  }
  printf("It is a prime number");
}
int su(int m) {
  int n;
  printf("%d = ", m);
  for (n = 2; n <= m; n++) {
    while (m % n == 0) {
      if (m != n)
        printf("%d * ", n);
      else
        printf("%d ", n);
      m = m / n;
    }
  }
}
main() {
  int m, n, p;
  printf("Input m:");
  scanf("%d", &m);
  judge(m);
}


