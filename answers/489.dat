#include <stdio.h>
#include <stdlib.h>
int main() {
  int i, j, k;
  for (i = 0; i <= 50; i++) {
    for (j = 0; j <= 100 - i; j++) {
      k = 100 - i - j;
      if (5 * i + 3 * j + (float)k / 3 == 100) {
        printf("公鸡是%d只，母鸡是%d只，雏鸡是%d只.\n", i, j, k);
      }
    }
  }
  return 0;
}

