#include <stdio.h>
int main() {
  int a, b, c;
  for (a = 0; a <= 20; a++) {
    for (b = 0; b <= 33; b++) {
      for (c = 0; c <= 300; c++) {
        if (a + b + c == 100 && 5 * a + 3 * b + c / 3 == 100 && c % 3 == 0) {
          printf("公鸡是%d只，母鸡是%d只，雏鸡是%d只.\n", a, b, c);
        }
      }
    }
  }
}

