#include "stdio.h"
int gcd(int i, int j) {
  if (i < 1 || j < 1)
    return -1;
  if (i % j == 0)
    return j;
  else
    return gcd(j, i % j);
}
int main() {
  int a[11], i, max, min, ans;
  printf("Input 10 numbers:\n");
  for (i = 1; i <= 10; i++)
    scanf("%d", &a[i]);
  max = a[1];
  min = a[1];
  for (i = 2; i <= 10; i++) {
    if (a[i] > max)
      max = a[i];
    if (a[i] < min)
      min = a[i];
  }
  ans = gcd(max, min);
  printf("maxNum=%d\n", max);
  printf("minNum=%d\n", min);
  if (ans != -1)
    printf("%d", ans);

  return 0;
}

