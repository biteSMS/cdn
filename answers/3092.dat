  j = max;
  m = min;
  gongyueshu = j % m;

  while (gongyueshu != 0) {
    j = m;
    m = gongyueshu;
    gongyueshu = j % m;
  }
  printf("maxNum=%d\n", max);
  printf("minNum=%d\n", min);
