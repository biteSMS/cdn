#include <stdio.h>
int DBD(int m, int n) {
  if (m == n) {
    return m;
  } else {
    if (m > n) {
      return DBD(m - n, n);
    } else {
      return DBD(m, n - m);
    }
  }
}
int main() {
  int a[10], i, max = 0, min = 100000;
  printf("Input 10 numbers:\n");
  for (i = 0; i < 10; i++) {
    scanf("%d", &a[i]);
  }
  for (i = 0; i < 10; i++) {
    if (a[i] < min) {
      min = a[i];
    }
    if (a[i] > max) {
      max = a[i];
    }
  }
  if (min == 0) {
    printf("maxNum=%d\n", max);
    printf("minNum=%d\n", min);
  } else {
    printf("maxNum=%d\n", max);
    printf("minNum=%d\n", min);
    printf("%d", DBD(max, min));
  }
  return 0;
}

