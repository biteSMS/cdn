#include <stdio.h>
#define N 10
void remax(int a[]);
int main() {
  int a[N];
  int i, c;
  int max, min;
  printf("Input 10 numbers:\n");
  for (i = 0; i < N; i++) {
    scanf("%d", &a[i]);
  }
  remax(a);
  min = a[N - 1];
  max = a[0];
  if (a[N - 1] != 0) {
    c = max % min;
    while (c != 0) {
      max = min;
      min = c;
      c = max % min;
    }
    printf("maxNum=%d\n", a[0]);
    printf("minNum=%d\n", a[N - 1]);
    printf("%d", min);
  } else {
    printf("maxNum=%d\n", max);
    printf("minNum=%d\n", min);
  }
  return 0;
}
void remax(int a[]) {
  int i, j, k, temp;
  for (i = 0; i < N - 1; i++) {
    k = i;
    for (j = i + 1; j < N; j++) {
      if (a[k] < a[j])
        k = j;
    }
    if (k != i) {
      temp = a[k];
      a[k] = a[i];
      a[i] = temp;
    }
  }
}

