#include <stdio.h>
#define N 10

void FindNum(int a[N], int *max, int *min, int *maxpublic);
int FindPublic(int a, int b);
int main() {
  int a[N];
  int i = 0;
  int maxNum;
  int minNum;
  int publicmaxNum;

  printf("Input 10 numbers:\n");
  for (; i < N; i++) {
    scanf("%d", a + i);
  }
  FindNum(a, &maxNum, &minNum, &publicmaxNum);

  printf("maxNum=%d\n", maxNum);
  printf("minNum=%d\n", minNum);
  printf("%d\n", publicmaxNum);

  return 0;
}
void FindNum(int a[N], int *max, int *min, int *maxpublic) {
  int i;
  *max = *a;
  *min = *a;

  for (i = 0; i < N; i++) {
    if (a[i] > *max)
      *max = a[i];
    if (a[i] < *min)
      *min = a[i];
  }
  *maxpublic = FindPublic(*max, *min);
}
int FindPublic(int a, int b) {
  if (a > b)
    return FindPublic(a - b, b);
  else if (a < b)
    return FindPublic(a, b - a);
  else
    return a;
}

