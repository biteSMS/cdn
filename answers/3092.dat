  printf("Input 10 numbers:\n");
  for (i = 0; i < 10; i++) {
    scanf("%d", &str[i]);
  }
  max = str[0];
  min = str[0];
  for (i = 0; i < 10 - 1; i++) {
    if (max < str[i + 1]) {
      max = str[i + 1];
    }
    if (min > str[i + 1]) {
      min = str[i + 1];
    }
