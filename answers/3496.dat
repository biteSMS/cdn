#include <stdio.h>
char *mystrcat(char *dststr, char *srcstr) {
  char *pstr = dststr;
  while (*dststr != '\0') {
    dststr++;
  }
  for (; *srcstr != '\0'; dststr++, srcstr++) {
    *dststr = *srcstr;
  }
  *dststr = '\0';
  return pstr;
}
