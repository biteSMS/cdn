#include <stdio.h>
main() {
  int x, y, z, count = 0;
  for (x = 0; x <= 5; x++) {
    for (y = 0; y <= 10; y++) {
      z = 50 - x - y;
      if (x * 10 + y * 5 + z * 1 == 100) {
        printf("x = %d, y = %d, z = %d\n", x, y, z);
        count++;
      }
    }
  }
  printf("count = %d\n", count);
}

