#include <stdio.h>
int main() {
  int x, y, z, count = 1;
  for (x = 0; x < 10; x++) {
    for (y = 0; y < 20; y++) {
      for (z = 0; z < 100; z++) {
        if (x + z + y == 50 && 10 * x + 5 * y + z == 100) {
          printf("x = %d, y = %d, z = %d\n", x, y, z);
        }
      }
    }
  }
  printf("count = %d\n", count);
}

