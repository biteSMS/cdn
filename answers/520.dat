#include <stdio.h>
int main() {
  int n, i, sum;
  sum = 0;
  i = 0;
  printf("Input n:");
  scanf("%d", &n);
  do {
    i++;
    sum = sum + i;
  } while (i < n);
  printf("sum = %d\n", sum);
}

