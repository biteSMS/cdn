  scanf("%d", &n);
  getchar();
  printf("Input array a\n");
  for (i = 0; i < n; i++) {
    scanf("%d", &a[i]);
    getchar();
  }
  printf("Input array b\n");
  for (i = 0; i < n; i++) {
    scanf("%d", &b[i]);
    getchar();
  }
