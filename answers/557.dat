#include <stdio.h>
#define N 100
int main() {
  int a[N], b[N];
  int n, i;
  double sum = 0;
  printf("Input n\n");
  scanf("%d", &n);
  printf("Input array a\n");
  for (i = 0; i < n; i++) {
    scanf("%d", &a[i]);
  }
  printf("Input array b\n");
  for (i = 0; i < n; i++) {
    scanf("%d", &b[i]);
  }
  for (i = 0; i < n; i++) {
    sum += (a[i] * b[i]);
  }
  printf("\n");
  printf("sum=%lf", sum);
  return 0;
}

