#include <stdio.h>
#include <stdlib.h>
int MaxCommonFactor(int a, int b) {
  int r;
  if (a <= 0 || b <= 0)
    return -1;
  else {
    while (1) {
      r = a % b;
      if (r != 0) {
        a = b;
        b = r;
      } else
        return b;
    }
  }
}
int main() {
  int x, y;
  scanf("%d,%d", &x, &y);
  printf("%d", MaxCommonFactor(x, y));
}


