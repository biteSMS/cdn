int MaxCommonFactor(int a, int b);
int main() {
  int a, b;
  int flag = 0;
  scanf("%d,%d", &a, &b);
  flag = MaxCommonFactor(a, b);
  printf("%d", flag);
  return 0;
}
int MaxCommonFactor(int a, int b) {
  if (a < 0 || b < 0)
    return -1;
  else if (a == b)
    return a;
  else if (a > b)
    return MaxCommonFactor(a - b, b);
  else
    return MaxCommonFactor(a, b - a);
}

