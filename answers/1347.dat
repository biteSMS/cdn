#include <stdio.h>
#include <stdlib.h>
int MaxCommonFactor(int a, int b);
int main() {
  int a, b, result;
  scanf("%d,%d", &a, &b);
  result = MaxCommonFactor(a, b);
  printf("%d", result);
  return 0;
}
int MaxCommonFactor(int a, int b) {
  int r;
  if (a < 0 || b < 0)
    return -1;
  do {
    r = a % b;
    a = b;
    b = r;
  } while (r != 0);
  return a;
}

