int Gcd(int i, int j) {
  if (i <= 0 || j <= 0) {
    return -1;
  } else {
    if (i == j) {
      return i;
    } else if (i > j) {
      return Gcd(i - j, j);
    } else {
      return Gcd(i, j - i);
    }
  }
}

