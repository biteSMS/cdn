#define N 80
int Gcd(int a, int b) {
  int i, j = 0, m[N], max, temp;
  for (i = 1; i <= a; i++) {
    if (a % i == 0) {
      if (b % i == 0) {
        m[j] = i;
        j++;
      }
    }
  }
  max = m[0];
  for (i = 1; i < j; i++) {
    if (max < m[i]) {
      temp = max;
      max = m[i];
      m[i] = temp;
    }
  }
  if (a <= 0 || b <= 0)
    return -1;
  else
    return max;
}
