#include <string.h>
int fun(char a[]);
int main() {
  char a[80];
  printf("Input a string:\n");
  gets(a);
  if (fun(a)) {
    printf("This string is a plalindrome.\n");
  } else {
    printf("This string is not a plalindrome.\n");
  }
  return 0;
}
int fun(char a[]) {
  int i, j;
  for (i = 0, j = strlen(a); i < j; i++, j--) {
    if (a[i] != a[j - 1]) {
      return 0;
    }
  }
}

