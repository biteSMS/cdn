#include <stdio.h>
#include <string.h>
int main() {
  int a[10];
  int i, k = 0;
  for (i = 0; i < 10; i++) {
    scanf("%d", &a[i]);
  }
  for (i = 0; i < 10; i++) {
    if (a[i] % 2 == 1 && a[i] > 0)
      k++;
  }
  printf("the odd number is %d", k);
  return 0;
}

