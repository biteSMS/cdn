#include <stdio.h>
#include <stdlib.h>
int main() {
  int a[10], i, count = 0;
  for (i = 0; i < 10; i++) {
    scanf("%d", &a[i]);
  }
  for (i = 0; i < 10; i++) {
    if (a[i] % 2 != 0)
      count++;
  }
  printf("the odd number is %d", count);
  return 0;
}

