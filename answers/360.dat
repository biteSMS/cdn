#include <stdio.h>
#include <string.h>
#define MAX_LEN 10

void findDay(char day[7][MAX_LEN], char s[MAX_LEN], int *k);
int main() {
  char day[7][MAX_LEN] = {"sunday",  "monday", "tuesday", "wednesday",
                          "thurday", "friday", "saturday"};
  char inputday[MAX_LEN];
  int back = -1;

  printf("Enter a string:");
  scanf("%s", inputday);
  findDay(day, inputday, &back);
  back < 0 || back >= 7 ? printf("Error\n")
                        : printf("%s is %d\n", inputday, back);

  return 0;
}
void findDay(char day[7][MAX_LEN], char s[MAX_LEN], int *k) {
  int i;

  for (i = 0; i < 7; i++) {
    if (strcmp(day[i], s) == 0) {
      *k = i;
    }
  }
}

