#include <stdio.h>
#include <string.h>
#define N 7
#define MAX_LEN 10

int findDay(char day[N][MAX_LEN], char str[N]);
int main() {
  int back = 0;
  char day[N][MAX_LEN] = {"sunday",   "monday", "tuesday", "wednesday",
                          "thursday", "friday", "saturday"};
  char str[MAX_LEN];

  printf("Enter a string:");
  scanf("%s", str);
  back = findDay(day, str);

  back < 0 ? printf("Error\n") : printf("%s is %d\n", str, back);
  return 0;
}
int findDay(char day[N][MAX_LEN], char str[N]) {
  int i;

  for (i = 0; i < N; i++) {
    if (strcmp(day[i], str) == 0)
      return i;
  }
  return -1;
}

