#include <stdio.h>
#include <stdlib.h>
#define a 10
int main() {
  int s[a];
  int i, j, min = 0, max = 0;
  for (i = 0; i < a; i++) {
    scanf("%d", &s[i]);
  }
  for (i = 0; i < a; i++) {
    if (s[i] < s[min]) {
      min = i;
    }
  }
  for (j = 0; j < a; j++) {
    if (s[j] > s[max]) {
      max = j;
    }
  }
  printf("sum=%3d", s[min] + s[max]);
  return 0;
}

