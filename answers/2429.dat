#include <stdio.h>
int main() {
  int str[4][5];
  int i, j, k = 0;
  printf("请输入一个4乘5的矩阵：\n");
  for (i = 0; i < 4; i++) {
    for (j = 0; j < 5; j++) {
      printf("请输入a[%d][%d]：", i, j);
      scanf("%d", &str[i][j]);
    }
  }
  float small[20];
  int a[20], b[20];
  for (i = 0; i < 4; i++) {
    for (j = 0; j < 5; j++) {
      if (str[i][j] < 0) {
        small[k] = str[i][j];
        a[k] = i;
        b[k] = j;
        k++;
      }
    }
  }
  for (i = 0; i < k; i++) {
    printf("第%d行%d列的元素%.2f小于0\n", a[i], b[i], small[i]);
  }
}

