#include <stdio.h>
#define N 20
int main() {
  int a[4][5], i, j, b[N] = {0}, c[N] = {0}, e[N] = {0}, d = 0;
  printf("请输入一个4乘5的矩阵：\n");
  for (i = 0; i < 4; i++)
    for (j = 0; j < 5; j++) {
      printf("请输入a[%d][%d]：", i, j);
      scanf("%d", &a[i][j]);
      if (a[i][j] < 0) {
        b[d] = i;
        c[d] = j;
        e[d] = a[i][j];
        d++;
      }
    }
  for (i = 0; i < N; i++) {
    if (e[i] < 0)
      printf("第%d行%d列的元素%.2f小于0\n", b[i], c[i], (float)e[i]);
  }
}

