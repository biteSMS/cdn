#include <stdio.h>
#include <stdlib.h>

int main() {
  int i, j, m = 0;
  int hang[80], lie[80];
  double temp[80];
  double num[4][5];
  printf("请输入一个4乘5的矩阵：\n");
  for (i = 0; i < 4; i++) {
    for (j = 0; j < 5; j++) {
      printf("请输入a[%d][%d]：", i, j);
      scanf("%lf", &num[i][j]);
    }
  }
  for (i = 0; i < 4; i++) {
    for (j = 0; j < 5; j++) {
      if (num[i][j] < 0) {
        temp[m] = num[i][j];
        hang[m] = i;
        lie[m] = j;
        m++;
      }
    }
  }
  for (i = 0; i < m; i++) {
    printf("第%d行%d列的元素%.2f小于0\n", hang[i], lie[i], temp[i]);
  }
  return 0;
}


