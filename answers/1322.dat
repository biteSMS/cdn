#include <stdio.h>
int MinCommonMultiple(int a, int b);
main() {
  int a, b, x;
  printf("Input a,b:");
  scanf("%d,%d", &a, &b);
  x = MinCommonMultiple(a, b);
  printf("MinCommonMultiple = %d\n", x);
}
int MinCommonMultiple(int a, int b) {
  int i;
  int t = a < b ? a : b;
  for (i = 1; i < a * b; i++) {
    if ((i * t) % b == 0)
      return i * t;
  }
  return 0;
}


