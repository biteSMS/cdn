#include <stdio.h>
main() {
  int i = 0, findFlag = 1;
  char x[20];
  char *str[] = {"Pascal", "Basic",    "Fortran",
                 "Java",   "Visual C", "Visual Basic"};

  printf("Input string:\n");
  gets(x);

  while (i < 6 && 1 == findFlag) {
    if (strcmp(x, str[i]) == 0) {
      findFlag = 0;
      break;
    }
    i++;
  }
  if (findFlag == 0) {
    printf("%s\n", x);
  } else if (findFlag == 1) {
    printf("Not find!\n");
  }
}

