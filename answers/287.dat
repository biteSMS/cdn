#include <stdio.h>
#include <string.h>
int main() {
  int i = 0, findFlag = 1;
  char x[100];
  char *str[] = {"Pascal", "Basic",    "Fortran",
                 "Java",   "Visual C", "Visual Basic"};

  printf("Input string:\n");
  gets(x);

  while (i < 6 && findFlag) {
    if (0 == strcmp(x, str[i])) {
      findFlag = 0;
    }
    i++;
  }
  if (!findFlag) {
    printf("%s\n", x);
  } else {
    printf("Not find!\n");
  }
  return 0;
}

