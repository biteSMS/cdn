#include <stdio.h>
void exchange(int a[], int b[]) {
  int i;
  int temp;
  for (i = 0; i <= 4; i++) {
    temp = a[i];
    a[i] = b[i];
    b[i] = temp;
  }
}
main() {
  int a[5] = {1, 2, 3, 4, 5};
  int b[5] = {10, 20, 30, 40, 50};
  int i;
  exchange(a, b);
  for (i = 0; i <= 4; i++) {
    printf("a[%d]=%2d, ", i, a[i]);
  }
  printf("\n");
  for (i = 0; i <= 4; i++) {
    printf("b[%d]=%2d, ", i, b[i]);
  }
}

