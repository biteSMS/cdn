#include <stdio.h>
int main() {
  int Ii = 0, Itemp;
  int array_a[5] = {1, 2, 3, 4, 5};
  int array_b[5] = {10, 20, 30, 40, 50};
  int *a, *b;
  a = array_a;
  b = array_b;
  while (Ii < 5) {
    Itemp = *(a + Ii);
    *(a + Ii) = *(b + Ii);
    *(b + Ii) = Itemp;
    Ii++;
  }
  Ii = 0;
  while (Ii < 5) // 交换后的a
  {
    printf("a[%d]=%2d, ", Ii, array_a[Ii]);
    Ii++;
  }
  printf("\n");
  Ii = 0;
  while (Ii < 5) // 交换过的b
  {
    printf("b[%d]=%2d, ", Ii, array_b[Ii]);
    Ii++;
  }
}

