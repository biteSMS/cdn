#include <stdio.h>
void f(int a[], int b[]) {
  int t, i;
  for (i = 0; i < 5; i++) {
    t = a[i];
    a[i] = b[i];
    b[i] = t;
  }
}
int main() {
  int a[5] = {1, 2, 3, 4, 5};
  int b[5] = {10, 20, 30, 40, 50};
  int i;
  f(a, b);
  for (i = 0; i < 5; i++) {
    printf("a[%d]=%2d, ", i, a[i]);
  }
  printf("\n");
  for (i = 0; i < 5; i++)
    printf("b[%d]=%2d, ", i, b[i]);
  return 0;
}

