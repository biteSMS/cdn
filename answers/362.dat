#include <stdio.h>
void swap(int a[], int b[]);
int main() {
  int i;
  int a[5] = {1, 2, 3, 4, 5};
  int b[5] = {10, 20, 30, 40, 50};
  swap(a, b);
  for (i = 0; i < 5; i++) {
    printf("a[%d]=%2d, ", i, a[i]);
  }
  printf("\n");
  for (i = 0; i < 5; i++) {
    printf("b[%d]=%2d, ", i, b[i]);
  }
  return 0;
}
void swap(int a[], int b[]) {
  int i, t;
  for (i = 0, t = a[0]; i < 5; i++) {
    t = a[i];
    a[i] = b[i];
    b[i] = t;
  }
}

