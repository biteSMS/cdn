  int a[5] = {1, 2, 3, 4, 5};
  int b[5] = {10, 20, 30, 40, 50};
  change(a, b);
  return 0;
}
int change(int *a, int *b) {
  int x = 0, f;
  for (x; x < 5; x++) {
    f = *(a + x);
    *(a + x) = *(b + x);
    *(b + x) = f;
    printf("a[%d]=%2d, ", x, a[x]);
  }
  printf("\n");
  for (x = 0; x < 5; x++) {
    printf("b[%d]=%2d, ", x, b[x]);
  }
  return 0;
}

