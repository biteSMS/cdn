    printf("Enter No.%2d:", i + 1);
    scanf("%d", &a[i]);
  }
  for (i = 0; i < N - 1; i++) {
    p = i;
    for (j = i + 1; j < N; j++)
      if (a[p] > a[j])
        p = j;
    if (p != i) {
      t = a[p];
      a[p] = a[i];
      a[i] = t;
    }
  }
  for (i = 0; i < N; i++)
