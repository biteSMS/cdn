#include <stdio.h>
#include <string.h>
int main() {
  int i, num[5], j;
  for (i = 1; i < 6; i++) {
    printf("Enter No.%2d:", i);
    scanf("%d", &num[i - 1]);
  }
  for (i = 0; i < 5; i++) {
    for (j = i; j < 5; j++) {
      if (num[i] > num[j]) {
        int temp = num[i];
        num[i] = num[j];
        num[j] = temp;
      }
    }
  }
  for (i = 0; i < 5; i++)
    printf("%d", num[i]);
  return 0;
}

