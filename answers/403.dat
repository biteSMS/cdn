#include <stdio.h>
int main() {
  int i, n, k, j;
  int a[5];
  for (i = 0; i < 5; i++) {
    printf("Enter No.%2d:", i + 1);
    scanf("%d", &a[i]);
  }
  for (i = 0; i < 5 - 1; i++) {
    k = i;
    for (j = 1; j < 5; j++) {
      if (a[i] >= a[j]) {
        k = j;
      }
    }
    if (k != i) {
      n = a[i];
      a[i] = a[k];
      a[k] = n;
    }
  }
  for (i = 0; i < 5; i++) {
    printf("%d", a[i]);
  }
  return 0;
}
