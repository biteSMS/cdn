#include <stdio.h>
#include <stdlib.h>

int main() {
  const int N = 5;
  int i, j;
  int temp;
  int a[N];
  for (i = 1; i <= N; i++) {
    printf("Enter No.%2d:", i);
    scanf("%d", &a[i - 1]);
  }

  for (i = 0; i < N; i++) {
    for (j = 0; j < N - i - 1; j++) {
      if (a[j] > a[j + 1]) {
        temp = a[j];
        a[j] = a[j + 1];
        a[j + 1] = temp;
      }
    }
  }

  for (i = 0; i < N; i++) {
    printf("%d", *(a + i));
  }

  return 0;
}

