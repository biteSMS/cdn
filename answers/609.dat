#include <math.h>
#include <stdio.h>
int main() {
  int a, i, n;
  long sum = 0;
  printf("please input a and n\n");
  scanf("%d,%d", &a, &n);
  printf("a=%d,n=%d\n", a, n);
  for (i = 1; i <= n; i++) {
    sum += (pow(10, i) - 1) * a / 9;
  }
  printf("a+aa+...=%ld\n", sum + 2);
  return 0;
}

