#include <stdio.h>
int mul(int n);
int main() {
  int a, n, i, k;
  long sum = 0;
  printf("please input a and n\n");
  scanf("%d,%d", &a, &n);
  k = n;
  for (i = 1; i <= n; i++) {
    sum = sum + k * a * (mul(i));
    k--;
  }
  printf("a=%d,n=%d\n", a, n);
  printf("a+aa+...=%ld\n", sum);
  return 0;
}

int mul(int n) {
  int i, m = 1;
  for (i = 2; i <= n; i++) {
    m = m * 10;
  }
  return m;
}

