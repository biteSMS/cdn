#include <stdio.h>
int main() {
  char a;
  printf("Press a key and then press Enter:");
  scanf("%c\n", &a);
  if (65 <= a && a <= 91 || 97 <= a && a <= 122) {
    printf("It is an English character!\n");
  } else if (48 <= a && a <= 57) {
    printf("It is a digit character!\n");
  } else if (a == 32) {
    printf("It is a space character!\n");
  } else
    printf("It is other character!\n");
  return 0;
}


