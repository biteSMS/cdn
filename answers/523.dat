#include <stdlib.h>
int main() {
  int n, i;
  long sum = 1;
  printf("Please enter n:");
  scanf("%d", &n);
  for (i = 2; i <= n; i++)
    sum *= i;

  printf("%d! = %ld\n", n, sum);
}

