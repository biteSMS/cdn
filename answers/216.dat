#include <math.h>
#include <stdio.h>

int main() {
  printf("Input a,b,c:");
  float a, b, c;
  scanf("%f,%f,%f", &a, &b, &c);
  if (a + b > c && a + c > b && b + c > a) {
    float s, area;
    s = (a + b + c) / 2;
    area = sqrt(s * (s - a) * (s - b) * (s - c));
    printf("area=%.4f\n", area);
  } else
    printf("It is not a triangle\n");
}


