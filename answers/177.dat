#include <stdio.h>

int FindMax(int num[], int n) {
  int i, j = 0;
  for (i = 1; i < n; i++) {
    if (num[i] > num[j])
      j = i;
  }
  return num[j];
}

int main() {
  const int size = 10;
  int a[size], max, i;
  for (i = 0; i < size; i++) {
    scanf("%d", &a[i]);
  }
  max = FindMax(a, size);
  printf("max=%d\n", max);
  return 0;
}

