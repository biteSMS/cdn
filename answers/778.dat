#include <stdio.h>
#define N 0.8
int main() {
  int i = 0, num = 2;
  float aver, sum = 0;
  while (num <= 100) {
    i++;
    sum = sum + num * N;
    num = num * 2;
  }
  aver = sum / i;
  printf("%.2f\n", aver);
  return 0;
}

