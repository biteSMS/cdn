#include <stdio.h>
#include <stdlib.h>
int strlen(char *s) {
  int i = 0;
  while (*s != '\0') {
    i++;
    s++;
  }
  return i;
}

int main() {
  char str[100];
  int len;
  printf("Please enter a string:\n");
  gets(str);
  len = strlen(str);
  printf("The length of the string is:%u\n", len);
  return 0;
}

