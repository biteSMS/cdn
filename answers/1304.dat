#include <stdio.h>
int main() {
  int i, n = 20;
  double a = 2, b = 1, s = 0, t;
  for (i = 1; i <= n; i++) {
    s = s + a / b;
    t = a, a = a + b, b = t;
  }
  printf("总和=%9.6f\n", s);
  return 0;
}

