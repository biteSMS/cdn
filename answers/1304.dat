#include <stdio.h>
int main() {
  int a[20], b[20], i;
  double sum = 0;
  a[0] = 2, a[1] = 3;
  b[0] = 1;
  b[1] = 2;
  for (i = 2; i < 20; i++) {
    a[i] = a[i - 1] + b[i - 1];
    b[i] = b[i - 2] + b[i - 1];
  }
  for (i = 0; i < 20; i++) {
    sum = sum + (double)a[i] / b[i];
  }
  printf("总和=%9.6f\n", sum);
  return 0;
}

