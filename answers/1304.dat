#include <stdio.h>
int main() {
  float i, j = 2.0, sum = 0, k, l = 1;
  for (i = 1; i <= 20; i++) {
    sum += j / l;
    k = j;
    j += l;
    l = k;
  }
  printf("总和=%9.6f\n", sum);
}

