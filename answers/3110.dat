#include "stdio.h"
int main() {
  int i, j;
  for (i = 0; i <= 9; i++)
    printf("%7d", i);
  printf("\n");
  for (i = 0; i <= 9; i++) {
    printf("%d", i);
    for (j = 0; j <= 9; j++)
      printf("%7.3f", sqrt(i * 10 + j));
    printf("\n");
  }

  return 0;
}

