#include <stdio.h>
int Fibs(int n) {
  if (n == 1 || n == 2)
    return 1;
  else
    return Fibs(n - 1) + Fibs(n - 2);
}
int main() {
  int n;

  printf("Input n=?");
  scanf("%d", &n);

  printf("No. %d is %d\n", n, Fibs(n));
  return 0;
}

