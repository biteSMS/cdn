#include <stdio.h>
#include <stdlib.h>
int Fun(int n) {
  if (n == 0)
    return 0;
  if (n == 1)
    return 1;
  if (n >= 2) {
    return Fun(n - 1) + Fun(n - 2);
  }
}

int main() {
  int n;
  printf("Input n=?");
  scanf("%d", &n);
  printf("No. %d is %d\n", n, Fun(n));
}


