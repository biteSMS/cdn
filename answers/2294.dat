#include <stdio.h>
float Fun(int n);
int main() {
  int n;
  printf("please input a number:");
  scanf("%d", &n);
  printf("%10.6f\n", Fun(n));
  return 0;
}
float Fun(int n) {
  float i, j, t;
  float s = 1;
  for (i = 1; i <= n; i++) {
    t = 0;
    for (j = 1; j <= n; j++) {
      t = t + j;
    }
    s = s + 1 / t;
  }
  return s;
}

