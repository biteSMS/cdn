#include <stdio.h>
int main() {
  long a = 372, b = 246, a1, a2, a3, b1, b2, b3;
  a1 = a / 100;
  a2 = a / 10 % 10;
  a3 = a % 10;
  b1 = b / 100;
  b2 = b / 10 % 10;
  b3 = b % 10;
  if (a2 >= 5 && a2 <= 9 && b3 >= 5 && b3 <= 9 && a1 >= 0 && a1 <= 4 &&
      a3 >= 0 && a3 <= 4 && b1 >= 0 && b1 <= 4 && b2 >= 0 && b2 <= 4) {
    printf("\n   %ld\n", a);
    printf("*  %ld\n", b);
    printf("--------------\n");
    printf("  %ld\n %ld\n %ld\n", a * b3, a * b2, a * b1);
    printf("--------------\n");
    printf(" %ld\n", a * b);
  }
  return 0;
}

