#include <math.h>
#include <stdio.h>
int main() {
  long n;
  int i;
  __int64 x;
  __int64 b;
  while (scanf("%ld", &n) != EOF) {
    while (n--) {
      scanf("%I64d", &x);
      b = sqrt(x);
      for (i = 2; i * i <= b; i++) {
        if (b % i == 0)
          break;
      }
      if (i * i > b && b * b == x && x > 1) {
        printf("YES\n");
      } else {
        printf("NO\n");
      }
    }
  }
  return 0;
}


