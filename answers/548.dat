#include <stdio.h>

int length_string(char *s);

void main() {
  printf("Please enter a string:");
  char s1[50];
  gets(s1);
  int length;
  length = length_string(s1);
  printf("The length of the string is: %u\n", length);
}

int length_string(char *s) {
  int i = 0;
  while (*s != '\0') {
    s++;
    i++;
  }
  return i;
}

