#include <stdio.h>
#include <string.h>
void main() {
  char a[8][50], ch[50];

  int i, j;
  for (j = 0; j < 8; j++) {
    scanf("%s", a[j]);
    getchar();
  }
  for (i = 0; i < 8; i++) {
    for (j = i + 1; j < 8; j++) {
      if (strcmp(a[i], a[j]) > 0) {
        strcpy(ch, a[j]);
        strcpy(a[j], a[i]);
        strcpy(a[i], ch);
      }
    }
    printf("%s\n", a[i]);
  }
}


