#include <stdio.h>
int ReadScore(int score[]);
int FindMax(int score[], int n);
int main() {
  int b, c;
  int a[40];
  b = ReadScore(a);
  printf("Total students are %d\n", b);
  c = FindMax(a, b);
  printf("The highest score is %d\n", c);
  return 0;
}
int ReadScore(int score[]) {
  int i;
  for (i = 0;; i++) {
    printf("Input score:");
    scanf("%d", &score[i]);
    if (score[i] < 0)
      break;
  }
  return i;
}
int FindMax(int score[], int n) {
  int i;
  int max = score[0];
  for (i = 1; i < n; i++) {
    if (max < score[i])
      max = score[i];
  }

  return max;
}


