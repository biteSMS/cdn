#include <stdio.h>
#include <stdlib.h>

double cylinder(double r, double h) { return 3.1415926 * r * r * h; }
int main() {
  double r, h;
  printf("Enter radius and height: ");
  scanf("%lf%lf", &r, &h);

  printf("Volume = %.3f\n", cylinder(r, h));

  return 0;
}

