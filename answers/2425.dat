int main() {
  int h, m;
  for (h = 0; h < 13; h++) {
    for (m = 0; m < 60; m++) {
      printf("%d:", h);
      if (m >= 10) {
        printf("%d\t", m);
      } else {
        printf("0%d\t", m);
      }
    }
  }

  return 0;
}

