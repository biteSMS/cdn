#include <stdio.h>
int main() {
  int i = 0, j = 0;
  for (i = 0; i < 13; i++) {
    for (j = 0; j < 60; j++) {
      if (j < 10) {
        printf("%d:", i);
        printf("0%d\t", j);
      } else {
        printf("%d:", i);
        printf("%d\t", j);
      }
    }
  }
  return 0;
}

