#include <stdio.h>
void fact(int a[], int n) {
  int b[10];
  int i, j = 9;
  for (i = 0; i < n; i++) {
    b[i] = a[j];
    j = j - 1;
  }
  for (i = 0; i < n; i++) {
    printf("%5d", b[i]);
  }
}
int main() {
  int a[10];
  int i;
  printf("input 10 numbers:");
  for (i = 0; i < 10; i++) {
    scanf("%d", &a[i]);
  }
  fact(a, 10);
  return 0;
}

