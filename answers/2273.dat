#include "stdio.h"
#define M 10
int main() {
  int a[M];
  int i, j, temp;
  printf("input 10 numbers:");
  for (i = 0; i < M; i++) {
    scanf("%d", &a[i]);
  }

  for (i = 0, j = M - 1; i <= j; i++, j--) {
    temp = a[i];
    a[i] = a[j];
    a[j] = temp;
  }

  for (i = 0; i < M; i++) {
    printf("%5d", a[i]);
  }

  return 0;
}

