#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
  char a[18] = {"secret"};
  char b[18];

  int n;

  printf("Input Password:");
  scanf("%s", b);

  n = strcmp(a, b);
  if (0 == n)
    printf("Correct password! Welcome to the system...\n");
  if (n > 0)
    printf("Invalid password!user input<password\n");
  if (n < 0)
    printf("Invalid password!user input>password\n");

  return 0;
}

