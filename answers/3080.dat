#include <stdio.h>
#define N 100

int JudgePrime(int num);
int main() {
  long int c, d;
  int i;
  int temp[N];
  int k = 0;
  int back = 1;
  int total = 0;

  printf("please input c,d(c>2):\n");
  scanf("%ld,%ld", &c, &d);
  for (i = c + 1; i < d; i++) {
    back = JudgePrime(i);
    if (back != 0) {
      temp[k++] = i;
    }
  }
  for (i = 0; i < k; i++) {
    // printf("%d\n", temp[i]);
    if (temp[i + 1] - temp[i] == 2) {
      total++;
      printf("(%ld,%ld)\n", temp[i], temp[i + 1]);
    }
  }

  printf("total=%d\n", total);
  return 0;
}
int JudgePrime(int num) {
  int i;
  int flag = 0;

  for (i = 2; i < num && !flag; i++) {
    if (num % i == 0)
      flag = 1;
  }
  if (!flag)
    return num;
  return 0;
}

