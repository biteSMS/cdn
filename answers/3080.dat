#include <stdio.h>
int res(int num) {
  int i = 2, flag = 0; // flag=0为素数，默认为素数
  for (i; i < num; i++) {
    if (num % i == 0) {
      flag = 1;
      return flag;
    }
  }
  return flag;
}
int main() {
  long c, d;
  int num, total = 0;
  printf("please input c,d(c>2):\n");
  scanf("%ld,%ld", &c, &d);
  for (num = c; num <= d - 2; num++) {
    if (res(num) == 0 && res(num + 2) == 0) {
      printf("(%ld,%ld)\n", num, num + 2);
      total++;
    }
  }
  printf("total=%d\n", total);
}

