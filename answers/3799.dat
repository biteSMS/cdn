#include <stdio.h>
#define N 3
#define M 4
main() {
  int a[N][M], b[N][M], c[N][M];
  int i, j;
  printf("Input array a(%d*%d)\n", N, M);
  for (i = 0; i < N; i++)
    for (j = 0; j < M; j++)
      scanf("%d", &a[i][j]);
  printf("Input array b(%d*%d)\n", N, M);
  for (i = 0; i < N; i++)
    for (j = 0; j < M; j++)
      scanf("%d", &b[i][j]);
  for (i = 0; i < N; i++)
    for (j = 0; j < M; j++) {
      if (a[i][j] > b[i][j])
        c[i][j] = 1;
      else if (a[i][j] < b[i][j])
        c[i][j] = -1;
      else
        c[i][j] = 0;
    }
  printf("Output array a:\n");
  for (i = 0; i < N; i++) {
    for (j = 0; j < M; j++)
      printf("%4d", a[i][j]);
    printf("\n");
  }
  printf("Output array b:\n");
  for (i = 0; i < N; i++) {
    for (j = 0; j < M; j++)
      printf("%4d", b[i][j]);
    printf("\n");
  }
  printf("Output array c:\n");
  for (i = 0; i < N; i++) {
    for (j = 0; j < M; j++)
      printf("%4d", c[i][j]);
    printf("\n");
  }
}

