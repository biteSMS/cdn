#include <stdlib.h>
int main() {
  int a[3][4], b[3][4], c[3][4];
  int i, j;
  printf("Input array a(%d*%d)\n", 3, 4);
  for (i = 0; i < 3; i++) {
    for (j = 0; j < 4; j++) {
      scanf("%d", &a[i][j]);
    }
  }
  printf("Input array b(%d*%d)\n", 3, 4);
  for (i = 0; i < 3; i++) {
    for (j = 0; j < 4; j++) {
      scanf("%d", &b[i][j]);
    }
  }
  printf("Output array a:\n");
  for (i = 0; i < 3; i++) {
    for (j = 0; j < 4; j++) {
      printf("%4d", a[i][j]);
      if (j == 3)
        printf("\n");
    }
  }
  printf("Output array b:\n");
  for (i = 0; i < 3; i++) {
    for (j = 0; j < 4; j++) {
      printf("%4d", b[i][j]);
      if (j == 3)
        printf("\n");
    }
  }
  printf("Output array c:\n");
  for (i = 0; i < 3; i++) {
    for (j = 0; j < 4; j++) {
      if (a[i][j] == b[i][j])
        printf("%4d", 0);
      else if (a[i][j] > b[i][j])
        printf("%4d", 1);
      else
        printf("%4d", -1);
      if (j == 3)
        printf("\n");
    }
  }
  return 0;
}

