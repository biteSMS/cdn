#include <stdio.h>
#include <stdlib.h>
#define N 3

int ReadMatrixAndAverage(int matrix[][3]);
void PrintAboveAver(int matrix[][3], int average);

//输入矩阵并计算平均值
int ReadMatrixAndAverage(int matrix[][3]) {
  int i, j, sum = 0;
  for (i = 0; i < N; ++i) {
    for (j = 0; j < 3; ++j) {
      scanf("%d", &matrix[i][j]);
      sum += matrix[i][j];
    }
  }
  return sum / (3 * N);
}

void PrintAboveAver(int matrix[][3], int average) {
  int i, j;
  for (i = 0; i < N; ++i) {
    for (j = 0; j < 3; ++j) {
      if (matrix[i][j] >= average) {
        printf("%4d", matrix[i][j]);
      }
    }
  }
}

int main() {
  int matrix[N][3];
  int aver = 0;
  printf("please input 3*3 matrix\n");
  aver = ReadMatrixAndAverage(matrix);
  printf("aver = %d\n", aver);
  PrintAboveAver(matrix, aver);
  return 0;
}


