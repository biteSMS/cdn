#include <stdio.h>
#include <stdlib.h>
int main() {
  int n = 3;
  // printf("Input n:");
  // scanf("%d", &n);
  printf("please input 3*3 matrix\n");
  int **matrix = (int **)malloc(sizeof(int *) * n);
  int i = 0, j = 0;
  for (i = 0; i < n; i++) {
    matrix[i] = (int *)malloc(sizeof(int) * n);
  }
  for (i = 0; i < n; i++) {
    for (j = 0; j < n; j++) {
      scanf("%d", &matrix[i][j]);
    }
  }
  int aver = 0;
  for (i = 0; i < n; i++) {
    for (j = 0; j < n; j++) {
      aver += matrix[i][j];
    }
  }
  aver /= n * n;
  printf("aver = %d\n", aver);
  for (i = 0; i < n; i++) {
    for (j = 0; j < n; j++) {
      if (matrix[i][j] >= aver)
        printf("%4d", matrix[i][j]);
    }
  }
  for (i = 0; i < n; i++) {
    free(matrix[i]);
  }
  free(matrix);

  return 0;
}

