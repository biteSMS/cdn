#include <stdio.h>
#include <stdlib.h>

int main() {
  int student[40], count[10] = {0};
  int i, j, flag = 1;
  printf("Input the feedbacks of 40 students:\n");
  for (i = 0; i < 40; i++) {
    scanf("%d", student + i);
  }
  for (i = 0; i < 40; i++) {
    if (student[i] > 10 || student[i] < 1) {
      flag = 0;
      break;
    }
  }

  if (flag) {
    for (i = 0; i < 40; i++) {
      for (j = 0; j < 10; j++) {
        if (student[i] == j + 1)
          count[j]++;
      }
    }

    printf("Feedback\tCount\tHistogram\n");
    for (i = 0; i < 10; i++) {
      printf("%8d\t%5d\t", i + 1, count[i]);
      for (j = 0; j < count[i]; j++) {
        printf("*");
      }
      printf("\n");
    }
  } else {
    printf("input error!\n");
  }

  return 0;
}

