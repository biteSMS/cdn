#include <stdio.h>

int main() {
  printf("Input the feedbacks of 40 students:\n");
  int num, cnt[11] = {0}, i;
  for (i = 0; i < 40; i++) {
    scanf("%d", &num);
    if (num <= 0 || num > 10) {
      printf("input error!\n");
      continue;
    }
    cnt[num]++;
  }
  printf("Feedback\tCount\tHistogram\n");
  int j;
  for (i = 1; i <= 10; i++) {
    printf("%8d\t%5d\t", i, cnt[i]);
    for (j = 0; j < cnt[i]; j++) {
      printf("*");
    }
    printf("\n");
  }
  return 0;
}

