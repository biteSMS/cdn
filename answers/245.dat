#include <stdio.h>
#include <stdlib.h>

int main() {
  int i = 0, j = 0, flag = 1;
  int a[40] = {}, b[10] = {};
  printf("Input the feedbacks of 40 students:\n");
  for (i = 0; i < 40; i++) {
    scanf("%d", a + i);
    if (a[i] < 1 || a[i] > 10) {
      flag = 0;
      break;
    }
    b[a[i] - 1]++;
  }

  if (flag) {
    printf("Feedback\tCount\tHistogram\n");
    for (i = 0; i < 10; i++) {
      printf("%8d\t%5d\t", i + 1, b[i]);
      for (j = 0; j < b[i]; j++)
        printf("*");
      printf("\n");
    }
  }
  if (!flag)
    printf("input error!\n");

  return 0;
}

