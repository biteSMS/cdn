#include <stdio.h>
#include <stdlib.h>
int main() {
  char n;
  printf("input a capital letter:\n");
  n = getchar();
  if ('A' <= n && n <= 'Z') {
    printf("%c", n + 32);
  } else {
    printf("input error!");
  }
}

