int Compare(char *a, char *b) {
  int flag = 1, i, j, count_ = 0;
  for (i = 0; i < strlen(a); i++) {
    for (j = 0; j < strlen(b); j++) {
      if (a[i] == b[j]) {
        // flag = 1;
        count_++;
      }
    }
  }
  if (count_ >= 2)
