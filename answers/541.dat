#include <stdio.h>
void fact(int score[], int n) {
  int i, j, temp;
  for (i = 0; i < n - 1; i++) {
    for (j = 0; j < n; j++) {
      if (score[i] > score[j]) {
        temp = score[i];
        score[i] = score[j];
        score[j] = temp;
      }
    }
  }
  printf("The highest score is %d\n", score[0]);
}
int main() {
  int score[40];
  int i = 0;
  do {
    printf("Input score:");
    scanf("%d", &score[i]);
    i = i + 1;
  } while (score[i - 1] >= 0);
  printf("Total students are %d\n", i - 1);
  fact(score, i - 1);
  return 0;
}

