#include <stdio.h>
#include <stdlib.h>

int read(int *a) {
  int i = -1;
  do {
    i++;
    printf("Input score:");
    scanf("%d", a + i);
  } while (a[i] >= 0);

  return i;
}
int highest(int *a, int n) {
  int flag, i;

  flag = a[0];
  for (i = 1; i < n; i++) {
    if (a[i] > flag)
      flag = a[i];
  }

  return flag;
}
int main() {
  int a[40] = {}, n;

  n = read(a);

  printf("Total students are %d\n", n);
  printf("The highest score is %d\n", highest(a, n));

  return 0;
}

