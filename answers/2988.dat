#include <stdio.h>
main() {
  int i, j, k, n;
  printf("Input n:\n");
  scanf("%d", &n);
  char a[6][n];
  for (i = 0; i < n; i++) {
    for (j = 0; j < 6; j++) {
      a[i][j] = '*';
      printf("%c", a[i][j]);
    }
    printf("\n");
    for (k = 1; k <= i + 1; k++)
      printf("\40");
  }
  printf("\n");
}

