#include <stdio.h>

int main() {
  int i, n, count = 0;
  for (i = 1; i <= 10; i++) {
    scanf("%d", &n);
    if (n % 2 != 0)
      count++;
  }
  printf("Odd number is:%d\n", count);

  return 0;
}

