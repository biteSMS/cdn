#include <stdio.h>
#define n 80
void main() {
  int i, j, big = 0, small = 0, number = 0, kongge = 0, others = 0, flag = 0;
  char a[3][n + 1] = {0};
  char k = 0;
  for (i = 0; i < 3; i++) {
    printf("\n请输入第%d行：\n", i);
    gets(a[i]);
  }
  for (i = 0; i < 3; i++) {
    printf("%s\n", a[i]);
  }
  for (i = 0; i < 3; i++) {
    for (j = 0, flag = 0; j <= n; j++, flag = 0) {
      k = a[i][j];
      // printf("a%d%d:%c ",i,j,k);
      if (k == '\0') {
        j = n + 1;
        flag = 1;
      }
      //英文大写字母
      if (k > 64 && k < 91) {
        big++;
        flag = 1;
      }
      //小写字母
      if (k > 96 && k < 123) {
        small++;
        flag = 1;
      }
      //数字
      if (k > 47 && k < 58) {
        number++;
        flag = 1;
      }
      //空格
      if (k == ' ') {
        kongge++;
        flag = 1;
      }
      //其它
      if (!flag)
        others++;
    }
    // printf("\n");
  }
  printf("大写字母数：%d\n", big);
  printf("小写字母数：%d\n", small);
  printf("数字个数  ：%d\n", number);
  printf("空格个数  ：%d\n", kongge);
  printf("其它字符  ：%d\n", others);
}

