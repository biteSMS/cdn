#include <stdio.h> //7.10 有一篇文章，共有3行文字，每行有80个字符。要求分别统计出其中英文大写字母、小写字母、数字、空格以及其他字符的个数。
int main() {
  char c[3][80];
  int i, j, daxie = 0, xiaoxie = 0, shuzi = 0, kongge = 0, qita = 0;
  printf("\n请输入第%d行：\n", 0);
  gets(c[0]);

  printf("\n请输入第%d行：\n", 1);
  gets(c[1]);

  printf("\n请输入第%d行：\n", 2);
  gets(c[2]);

  for (i = 0; i < 3; i++) {
    for (j = 0; j < 80 && c[i][j] != '\0'; j++) {
      if (c[i][j] == ' ')
        kongge++;
      else if ((c[i][j] >= '0') && (c[i][j] <= '9'))
        shuzi++;
      else if ((c[i][j] >= 'A') && (c[i][j] <= 'Z'))
        daxie++;
      else if ((c[i][j] >= 'a') && (c[i][j] <= 'z'))
        xiaoxie++;
      else
        qita++;
    }
  }
  printf("%s\n", c[0]);
  printf("%s\n", c[1]);
  printf("%s\n", c[2]);

  printf("大写字母数：%d\n", daxie);
  printf("小写字母数：%d\n", xiaoxie);
  printf("数字个数  ：%d\n", shuzi);
  printf("空格个数  ：%d\n", kongge);
  printf("其它字符  ：%d\n", qita);
  return 0;
}

