  for (a = 1; a <= 20; a++) {
    for (b = 1; b <= 20; b++) {
      if (a > b) {
        max = a;
      } else {
        max = b;
      }
      for (c = max; c <= 20; c++) {
        if ((a * a + b * b == c * c) || (a * a + c * c == b * b) ||
