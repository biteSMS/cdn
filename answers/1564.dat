#include <stdio.h>
int main() {
  int a, b, c;
  for (a = 1; a < 21; a++)
    for (b = 1; b < 21; b++)
      for (c = 1; c < 21; c++) {
        if (a * a + b * b == c * c)
          printf("a=%d\tb=%d\tc=%d\n", a, b, c);
      }
  return 0;
}

