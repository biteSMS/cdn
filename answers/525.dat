#include <stdio.h>
#include <stdlib.h>
int f(int n) {
  long int x = 1;
  for (; n >= 1; n--) {
    x *= n;
  }
  return x;
}
int main() {
  int n, a;
  long int sum = 0;
  printf("Input n:");
  scanf("%d", &n);
  for (a = n; a >= 1; a--) {
    sum += f(a);
  }
  printf("1!+2!+…+%d! = %ld\n", n, sum);
}

