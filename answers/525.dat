#include <stdio.h>
int main() {
  int i, n, sum = 0, p = 1;
  printf("Input n:");
  scanf("%d", &n);
  for (i = 1; i <= n; i++) {
    p *= i;
    sum += p;
  }
  printf("1!+2!+…+%d! = %ld\n", n, sum);
  return 0;
}

