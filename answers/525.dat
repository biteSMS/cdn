#include <stdlib.h>

int main() {
  int n, i = 1;
  long sum = 0, p = 1;
  printf("Input n:");
  scanf("%d", &n);
  do {
    p *= i;
    i++;
    sum = sum + p;
  } while (i <= n && n >= 3 && n <= 10);
  printf("1!+2!+…+%d! = %ld\n", n, sum);
  return 0;
}

