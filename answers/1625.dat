#include <stdio.h>
float f1(float x) //返回函数的值
{
  float y;
  y = x * x + 1.0;
  return y;
}
float f2(float x) {
  float y;
  y = x / (1 + x * x);
  return y;
}
float Integrate(float (*f)(float), float a, float b) //计算函数的定积分
{
  float dx, y, sy, x;
  long n = 10000;
  dx = (b - a) / n;
  x = a;
  sy = 0.0;
  int i;
  for (i = 0; i <= n; i++, x += dx)
    sy += f(x);
  sy -= f(a) / 2.0;
  sy -= f(b) / 2.0;
  return sy * dx;
}
void main() {
  float s1, s2;
  s1 = Integrate(f1, 0, 1);
  s2 = Integrate(f2, 0, 3);
  printf("y1=%f\ny2=%f\n", s1, s2);
}

