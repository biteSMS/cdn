#include <stdio.h>
main() {
  long n, i;
  printf("Please input number:");
  scanf("%ld", &n);
  for (i = 999; i >= 100; i--) {
    if (n % i == 0) {
      printf("The max factor with 3 digits in %ld is: %d.\n", n, i);
      exit(0);
    }
  }
}

