#include <stdio.h>
int main() {
  int i, j = 0, n;
  printf("Please input number:");
  scanf("%ld", &n);
  for (i = 100; i <= 999; i++) {
    if (n % i == 0)
      j = i;
  }
  if (j != 0)
    printf("The max factor with 3 digits in %ld is: %d.\n", n, j);
  return 0;
}


