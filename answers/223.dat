#include <stdio.h>
main() {
  int n, x, y, i;
  printf("Please enter n:");
  scanf("%d", &n);
  for (i = 1; i <= n; i++) {
    x = i * i;
    printf("%d*%d = %d\n", i, i, x);
  }
  for (i = 1; i <= n; i++) {
    y = i * i * i;
    printf("%d*%d*%d = %d\n", i, i, i, y);
  }
}

