#include <stdio.h>
void main() {
  int year, flag;
  printf("Input year:");
  scanf("%d", &year);
  if (year % 4 == 0 && year % 100 != 0 || year % 400 == 0) {
    flag = 1;
  } else {
    flag = 0;
  }
  if (flag == 1) {
    printf("Yes!");
  } else {
    printf("No!");
  }
}

