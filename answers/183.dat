#include <stdio.h>
int FindMax(int num[], int n) {
  int max = num[0], i;
  for (i = 1; i < n; i++) {
    if (num[i] > max) {
      max = num[i];
    }
  }
  return max;
}
int main() {
  int a[10], i;
  for (i = 0; i < 10; i++) {
    scanf("%d", &a[i]);
  }
  printf("max=%d\n", FindMax(a, 10));
  return 0;
}

