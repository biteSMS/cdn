#include <stdio.h>
#define N 10
int *get(int *a) {
  int i;
  for (i = 0; i < N; i++) {
    scanf("%d", a + i);
  }
  return a;
}
int FindMax(int num[], int n) {
  int i, max = num[0];
  for (i = 1; i < n; i++) {
    if (max < num[i]) {
      max = num[i];
    }
  }
  return max;
}
int main() {
  int a[N];
  printf("max=%d\n", FindMax(get(a), N));
  return 0;
}

