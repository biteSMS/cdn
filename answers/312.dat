#include <stdio.h>
#include <string.h>
#define WEEKDAYS 7
#define MAX_STR_LEN 10

int main(int argc, char *cargv[]) {
  int i;
  int pos = 0;
  int findFlag = 0;
  char x[MAX_STR_LEN];
  char weekDay[WEEKDAYS][MAX_STR_LEN] = {"Sunday",    "Monday",   "Tuesday",
                                         "Wednesday", "Thursday", "Friday",
                                         "Saturday"};

  printf("Please enter a string:\n");
  gets(x);
  for (i = 0; i < WEEKDAYS && !findFlag; i++) {
    // printf("%s\n", weekDay[i]);           //一个程序的debug还可用printf来检查
    if (strcmp(x, weekDay[i]) ==
        0) // !!!特别注意：比较两个字符串是否相等时，要用strcmp()这个函数，或者自己自定义一个函数来比较
    {
      pos = i;
      findFlag = 1;
    }
  }
  if (findFlag) {
    printf("%s is %d\n", x, pos);
  } else {
    printf("Not found!\n");
  }

  return 0;
}

