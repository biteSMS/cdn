#include <stdio.h>

long getFact(int);

void main() {
  int i;
  long sum = 0;
  for (i = 1; i <= 10; i++) {
    sum += getFact(i);
  }
  printf("1!+2!+...+10! = %ld\n", sum);
}

long getFact(int num) {
  if (!num)
    return 1;
  else {
    int i;
    long fact = 1;
    for (i = 2; i <= num; i++) {
      fact *= i;
    }
    return fact;
  }
}

