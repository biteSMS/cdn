//编程计算: 1!+ 3!+ 5!+ ... + (2n - 1)!
//	****输入提示信息格式 : "Input n:\n"
//	****输入数据格式要求 : "%d"
//	****输出数据格式要求 : "sum=%ld\n"
//	注：请勿使用函数编写此题
#include <stdio.h>
#include <stdlib.h>
long long func(int x);
int main() {
  int n = 10;
  long long sum = 0;
  int i = 1;
  for (i = 1; i <= n; i++) {
    // printf("计算 ! %d\n", 2 * i - 1);
    sum += func(i);
  }
  printf("1!+2!+...+10! = %ld\n", sum);
  return 0;
}
long long func(int x) {
  static int base = 1;
  static long long pro = 1;
  for (; base <= x; base++) {
    pro *= base;
  }
  // printf("! %d = %ld\n", base - 1, pro);
  return pro;
}

