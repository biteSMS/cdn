#include <stdio.h>
void main() {
  int i, j, input = 0, a = 0, b = 0, temp = 0;
  //定义数组
  int ji[5] = {0}, ou[5] = {0};
  //输入
  printf("Input 10 numbers:\n");
  for (i = 0; i < 10; i++) {
    scanf("%d", &input);
    if (input % 2) //奇数
    {
      ji[a++] = input; // j:counter
    } else             //偶数
    {
      ou[b++] = input; // o:counter
    }
  }
  //冒泡排序
  for (i = 0; i < 5; i++) //第i趟
  {
    for (j = 0; j < 4 - i; j++) //第j个
    {
      if (ji[j] > ji[j + 1]) //前面的大于后面前面的 就交换
      {
        temp = ji[j];
        ji[j] = ji[j + 1];
        ji[j + 1] = temp;
      }
    }
  }
  for (i = 0; i < 5; i++) {
    for (j = 0; j < 4 - i; j++) //第j个
    {
      if (ou[j] > ou[j + 1]) //前面的大于后面前面的 就交换
      {
        temp = ou[j];
        ou[j] = ou[j + 1];
        ou[j + 1] = temp;
      }
    }
  }
  //输出奇数
  printf("Output: ");
  for (i = 0; i < 5; i++) {
    printf("%d,", ji[i]);
  }

  //输出偶数
  for (i = 0; i < 4; i++) {
    printf("%d,", ou[i]);
  }
  printf("%d\n", ou[i]);
}

