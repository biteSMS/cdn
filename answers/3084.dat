#include <math.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main() {
  printf("Input 10 numbers:\n");
  int a[10], b[10], c[10];
  int i, j = 0, k = 0;
  for (i = 0; i < 10; i++) {
    scanf("%d", &a[i]);
  }
  for (i = 0; i < 10; i++) {
    if (a[i] % 2 == 0) {
      b[j] = a[i];
      j++;
    } else {
      c[k] = a[i];
      k++;
    }
  }
  int t, g, h;
  for (g = 0; g < j - 1; g++) {
    for (h = g + 1; h < j; h++) {
      if (b[g] > b[h]) {
        t = b[g];
        b[g] = b[h];
        b[h] = t;
      }
    }
  }
  for (g = 0; g < k - 1; g++) {
    for (h = g + 1; h < k; h++) {
      if (c[g] > c[h]) {
        t = c[g];
        c[g] = c[h];
        c[h] = t;
      }
    }
  }
  int m = 0;
  printf("Output: ");
  for (i = k; i < k + j; i++) {
    c[i] = b[m];
    m++;
  }
  for (i = 0; i < 9; i++) {
    printf("%d,", c[i]);
  }
  printf("%d\n", c[i]);

  return 0;
}

