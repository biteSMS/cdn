#include <stdio.h>
const int maxn = 100;
void srt(int x[], int m) {
  int i, j;
  int temp;
  int minn;
  for (i = 0; i < m - 1; i++) {
    minn = i;
    for (j = i + 1; j < m; j++) {
      if (x[minn] > x[j]) {
        minn = j;
      }
    }
    if (i != minn) {
      temp = x[i];
      x[i] = x[minn];
      x[minn] = temp;
    }
  }
}
int main() {
  int a[maxn], b[maxn];
  int i;
  int x;
  int l, r;
  l = r = 0;
  printf("Input 10 numbers:\n");
  for (i = 0; i < 10; i++) {
    scanf("%d", &x);
    if (x % 2 == 0) {
      b[r++] = x;
    } else {
      a[l++] = x;
    }
  }
  srt(a, l);
  srt(b, r);
  printf("Output: ");
  for (i = 0; i < l; i++) {
    printf("%d,", a[i]);
  }
  for (i = 0; i < r - 1; i++) {
    printf("%d,", b[i]);
  }
  printf("%d\n", b[i]);
  return 0;
}

