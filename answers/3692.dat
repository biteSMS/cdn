#include <stdio.h>
int Func(int n);
int count(int n);
int main() {
  int n, s = 0, t, i, a;
  do {
    printf("Input n:");
    scanf("%d", &n);
  } while ((n <= 0 || n > 10) && printf("Input error!\n"));

  for (i = 1; i <= n; i++) {
    t = Func(i);
    s += t;
  }
  a = count(s);
  a = count(a);
  printf("%d", a);
}
int Func(int n) {
  int i, sum = 1;
  for (i = 1; i <= n; i++) {
    sum *= i;
  }
  return sum;
}
int count(int n) {
  int i = 0, t = 0, a = 0;
  while (n > 0) {
    i++;
    t = n % 10;
    a = a * 10 + t;
    n = n / 10;
    if (i >= 6) {
      return a;
    }
  }
  return a;
}

