#include <stdio.h>
int Func(int n);
int res(int n);
int main() {
  int n, result, temp;
  printf("Input n:");
  scanf("%d", &n);
  result = res(n);
  if (result < 100000) {
    printf("%d", result);
  } else {
    temp = result % 100000;
    printf("%d", temp);
  }
}
int res(int n) {
  int sum = 0;
  for (n; n > 0; n--) {
    sum += Func(n);
  }
  return sum;
}
int Func(int n) {
  int i, res = n;
  for (n; n > 1; n--) {
    res *= (n - 1);
  }
  return res;
}

