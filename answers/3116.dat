/**/

#include <stdio.h>
#include <stdlib.h>

void Chline(char ch, int column, int row);

int main(void) {
  char ch;
  int column, row;
  printf("input a char:\n");
  scanf("%c", &ch);
  printf("input column and row:\n");
  scanf("%d %d", &column, &row);
  Chline(ch, column, row);
}
void Chline(char ch, int column, int row) {
  int i, j;
  for (i = 0; i < row; ++i) {
    for (j = 0; j < column; ++j) {
      printf("%c", ch);
    }
    printf("\n");
  }
}

