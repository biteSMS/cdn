#include <stdio.h>
#define a 3.14159
main() {
  double r, s, v;
  printf("Input r:");
  scanf("%lf", &r);
  s = 4 * r * r * a;
  v = (4 * r * r * r * a) / 3;
  printf("surface = %lf\n", s);
  printf("volume = %lf\n", v);
}

