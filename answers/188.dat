#include <stdio.h>
#include <string.h>
void main() {
  char a[10][80], k[80];
  int i, j;
  for (i = 0; i < 10; i++) {
    gets(a[i]);
  }
  for (i = 0; i < 10; i++) {
    for (j = i + 1; j < 10; j++)
      if (strcmp(a[i], a[j]) > 0) {
        strcpy(k, a[j]);
        strcpy(a[j], a[i]);
        strcpy(a[i], k);
      }
  }
  for (i = 0; i < 10; i++) {
    puts(a[i]);
  }
}

