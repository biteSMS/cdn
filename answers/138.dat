#include <math.h>
#include <stdio.h>
#include <stdlib.h>
long Fact(int n);
int main() {
  int n, sum = 0, i;
  printf("Input n(n>0):");
  scanf("%d", &n);
  for (i = 1; i <= n; i++) {
    sum = sum + Fact(i);
  }
  printf("sum = %d\n", sum);
}
long Fact(int n) {
  if (n == 0 || n == 1) {
    return 1;
  } else
    return n * Fact(n - 1);
}

