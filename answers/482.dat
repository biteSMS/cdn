#include <stdio.h>

int is_runnian(int x) {
  if ((x % 4 == 0) && (x % 100 != 0) || (x % 400 == 0))
    return 1;
  else
    return 0;
}

int main() {
  printf("输入年份:");
  int x = 0;
  scanf("%d", &x);
  printf("%d%s\n", x, is_runnian(x) ? "是闰年!\n" : "不是闰年!\n");
  return 0;
}

