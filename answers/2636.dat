#include <stdio.h>
long f(int n) {
  if (n == 0)
    return 1;
  else
    return n * f(n - 1);
}
main() {
  int n, i;
  long sum = 0;
  printf("Input n:\n");
  scanf("%d", &n);
  for (i = 1; i <= 2 * n - 1; i += 2)
    sum += f(i);
  printf("%ld", sum);
}

