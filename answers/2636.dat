#include <stdio.h>
int N(int n) {
  int i, sum = 1;

  for (i = 1; i <= n; i++)
    sum *= i;
  return sum;
}
int main() {
  int i, n, sum = 0;
  printf("Input n:\n");
  scanf("%d", &n);
  for (i = 1; i <= 2 * n - 1; i = i + 2)
    sum += N(i);
  printf("%d\n", sum);
  return 0;
}

