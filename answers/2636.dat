#include <stdio.h>

int main() {
  int n, i;
  long sum = 0;
  printf("Input n:\n");
  scanf("%d", &n);
  for (i = 1; i <= 2 * n - 1; i += 2) {
    sum += func(i);
  }
  printf("%ld", sum);

  return 0;
}

int func(int n) {
  int i;
  for (i = n - 1; i > 0; i--) {
    n *= i;
  }

  return n;
}

