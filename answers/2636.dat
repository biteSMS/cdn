#include "math.h"
#include "stdio.h"
#include "stdlib.h"
int fun(int n) {
  int mult = 1;
  while (n >= 1) {
    mult = mult * n;
    n--;
  }
  return mult;
}
int main() {
  long int sum = 0;
  int i;
  int n;
  printf("Input n:\n");
  scanf("%d", &n);
  for (i = 1; i <= 2 * n - 1; i = i + 2) {
    sum = sum + fun(i);
  }
  printf("%ld", sum);
}


