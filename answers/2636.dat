#include <stdio.h>
long int Fun(int n);
int main(void) {
  int n, i;
  long int sum = 0;
  printf("Input n:\n");
  scanf("%d", &n);
  for (i = 1; i <= 2 * n - 1;) {
    sum += Fun(i);
    i += 2;
  }
  printf("%ld", sum);
  return 0;
}
long int Fun(int n) {
  long int result = 1;
  int i;
  for (i = 1; i <= n; i++) {
    result *= i;
  }
  return result;
}

