#include <stdio.h>
#include <stdlib.h>

int main() {
  float litres[] = {11.5, 11.21, 12.7, 12.6, 12.4};
  float miles[] = {471.5, 358.72, 495.3, 453.6, 421.6};
  int mpl[5];
  int *p = mpl;
  float *a = litres, *b = miles;
  int i;

  for (i = 0; i < 5; i++) {
    *(p + i) = *(b + i) / (*(a + i));
  }

  for (i = 0; i < 5; i++, p++) {
    printf("%d\t", *p);
  }
  return 0;
}


