#include <stdio.h>
int main() {
  int a, d, i, h, j, x;
  for (a = 1;; a++) {
    for (d = 1, h = 0, j = 1; h < 26 && j < 880; d++) {
      for (i = 1, x = a, h = 0, j = 1; i <= 4; i++, x += d) {
        h += x;
        j *= x;
      }
    }
    if (h == 26 && j == 880) {
      d--;
      for (x = a; x <= 62; x += d) {
        printf("%d,", x);
      }
      break;
    }
  }
  printf("......\n");
  return 0;
}

