#include <stdio.h>
int Length(char str[]) {
  int i, len = 0;
  for (i = 0; str[i] != '\0'; i++) {
    len++;
  }
  return len;
}
int main() {
  char str[500];
  printf("Enter string:");
  scanf("%s", str);
  printf("string length=%d\n", Length(str));
  return 0;
}

