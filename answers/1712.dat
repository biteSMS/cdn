#include <stdio.h>
main() {
  int i, j, a[2][3] = {{1, 4, 6}, {8, 10, 12}}, m;
  for (i = 0; i < 2; i++) {
    m = a[i][2];
    for (j = 2; j > 0; j--) {
      a[i][j] = a[i][j - 1];
    }
    a[i][0] = m;
  }
  for (i = 0; i < 2; i++) {
    for (j = 0; j < 3; j++) {
      printf("%6d", a[i][j]);
    }
    printf("\n");
  }
}

