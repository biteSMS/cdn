#include <stdio.h>

int main() {
  int a[2][4] = {{1, 4, 6}, {8, 10, 12}}, i, j, cmp[2];
  for (i = 0; i <= 1; i++) {
    cmp[i] = a[i][2];
    for (j = 1; j >= 0; j--) {
      a[i][j + 1] = a[i][j];
    }
  }
  for (i = 0; i <= 1; i++) {
    a[i][0] = cmp[i];
  }
  for (i = 0; i <= 1; i++) {
    for (j = 0; j < 3; j++) {
      printf("%6d", a[i][j]);
    }
    printf("\n");
  }
  return 0;
}

