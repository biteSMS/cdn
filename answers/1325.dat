#include <stdio.h>
void Change(int num[], int n, int m) {
  int i, j, temp;
  for (i = 0; i < m; i++) {
    temp = num[n - 1];
    for (j = n - 1; j > 0; j--) {
      num[j] = num[j - 1];
    }
    num[0] = temp;
  }
}
int main() {
  const int size = 40;
  int n, num[size], m, i;
  printf("共有多少个数？");
  scanf("%d", &n);
  printf("后移多少个？");
  scanf("%d", &m);
  printf("请以,号为间隔输入%d个数。\n", n);
  for (i = 0; i < n; i++) {
    scanf("%d,", &num[i]);
  }
  Change(num, n, m);
  printf("移动后顺序为：\n");
  for (i = 0; i < n - 1; i++) {
    printf("%d,", num[i]);
  }
  printf("%d", num[i]);
  return 0;
}

