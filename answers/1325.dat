#include <stdio.h>
#include <stdlib.h>

int main() {
  int n, m, i, j = 0;
  printf("共有多少个数？");
  scanf("%d", &n);
  int a[n], b[n];
  char ch[n];
  printf("后移多少个？");
  scanf("%d", &m);
  printf("请以,号为间隔输入%d个数。\n", n);
  for (i = 0; i < n; i++) {
    scanf("%d", &a[i]);
    scanf("%c", &ch[i]);
  }
  for (i = n - m; i < n; i++) {
    b[j] = a[i];
    j++;
  }
  for (i = 0; i <= m; i++) {
    b[j] = a[i];
    j++;
  }

  printf("移动后顺序为：\n");
  for (i = 0; i < n; i++) {
    printf("%d", b[i]);
    if (i != n - 1)
      printf(",");
  }
  return 0;
}

