#include <stdio.h>
#include <stdlib.h>
#define N 10
int findmax(int a[]);
int main() {
  int a[N], i;
  for (i = 0; i < N; i++) {
    scanf("%d", &a[i]);
  }
  printf("max=%d\n", findmax(a));
  return 0;
}
int findmax(int a[]) {
  int i = 0, j, k;
  k = i;
  for (j = i + 1; j < N; j++) {
    if (a[k] < a[j])
      k = j;
  }
  return a[k];
}

