int Max(int a[]) {
  int temp, i, j;
  for (i = 0; i < 9; i++) {
    for (j = i + 1; j < 10; j++) {
      if (a[i] < a[j]) {
        temp = a[i];
        a[i] = a[j];
        a[j] = temp;
      }
    }
  }
  return a[0];
}
main() {
  int i, a[10], max;
  for (i = 0; i < 10; i++)
    scanf("%d", &a[i]);

  max = Max(a);
  printf("max=%d\n", max);
}

