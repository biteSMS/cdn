#include <stdio.h>
#include <stdlib.h>

int Choose(int a[], int size) {
  int i, j;
  j = 0;
  for (i = 1; i < size; i++) {
    if (a[i] > a[j]) {
      j = i;
    }
  }
  return a[j];
}

int main() {
  const int size = 10;
  int a[10], i, max;
  for (i = 0; i < size; i++)
    scanf("%d", &a[i]);
  max = Choose(a, size);
  printf("max=%d\n", max);
  return 0;
}

