#include <stdio.h>
#include <string.h>
int main() {
  int i;
  char *result, *temp;
  result = (char *)malloc(82);
  temp = (char *)malloc(82);
  memset(result, NULL, 82);
  memset(temp, NULL, 82);
  printf("Input five countries' names:\n");
  gets(result);
  i = 0;
  while (i < 4) {
    memset(temp, NULL, 82);
    gets(temp);
