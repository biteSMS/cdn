#include <stdio.h>
#include <stdlib.h>
#include <string.h>
void sort(char str[5][80]);
int main() {
  char name[5][80];
  int i;
  printf("Input five countries' names:\n");
  for (i = 0; i < 5; i++) {
    gets(name[i]);
  }
  sort(name);
  printf("The minimum is:%s\n", name[0]);
  return 0;
}
/*函数功能：按字典排序*/
void sort(char str[5][80]) {
  int i, j;
  char temp[80];
  for (i = 0; i < 4; i++) {
    for (j = i + 1; j < 5; j++) {
      if (strcmp(str[j], str[i]) < 0) {
        strcpy(temp, str[i]);
        strcpy(str[i], str[j]);
        strcpy(str[j], temp);
      }
    }
  }
}

