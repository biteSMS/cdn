#include <stdio.h>
#include <stdlib.h>

void sort(int *a, int n) {
  int i, j;
  int temp;
  for (i = 0; i < n - 1; i++) {
    for (j = i + 1; j < n; j++) {
      if (a[i] > a[j]) {
        temp = a[i];
        a[i] = a[j];
        a[j] = temp;
      }
    }
  }
}
int main() {
  int a[80] = {};
  int n, i;
  printf("Input n:");
  scanf("%d", &n);
  printf("Input %d numbers:", n);
  for (i = 0; i < n; i++) {
    scanf("%d", a + i);
  }

  sort(a, n);

  printf("Sorting results:");
  for (i = 0; i < n; i++) {
    printf("%4d", a[i]);
  }

  return 0;
}

