#include <stdio.h>
#include <string.h>

int main() {
  int i, j, n, s[100], t;
  printf("Input n:");
  scanf("%d", &n);
  printf("Input %d numbers:", n);
  for (i = 0; i < n; i++) {
    scanf("%d", &s[i]);
  }
  for (i = 0; i < n - 1; i++) {
    for (j = 1; j < n - i; j++) {
      if (s[j] < s[j - 1]) {
        t = s[j];
        s[j] = s[j - 1];
        s[j - 1] = t;
      }
    }
  }
  printf("Sorting results:");
  for (i = 0; i < n; i++) {
    printf("%4d", s[i]);
  }
}


