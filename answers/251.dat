#include <stdio.h>
void DataSort(int a[], int n) {
  int temp;
  int i, j;
  for (i = 0; i < n - 1; i++) {
    for (j = 0; j < n - 1; j++) {
      if (a[j] > a[j + 1]) {
        temp = a[j];
        a[j] = a[j + 1];
        a[j + 1] = temp;
      }
    }
  }
}
int main() {
  int n, a[20];
  int i = 0;
  printf("Input n:");
  scanf("%d", &n);
  printf("Input %d numbers:", n);
  for (; i < n; i++)
    scanf("%d", &a[i]);
  DataSort(a, n);
  printf("Sorting results:");
  for (i = 0; i < n; i++) {
    printf("%4d", a[i]);
  }
  return 0;
}

