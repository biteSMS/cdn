#include <stdio.h>
#define N 400
void ReadScore(int score[], int n) {
  int i;
  for (i = 0; i < n; i++) {
    scanf("%d", &score[i]);
  }
}
void DataSort(int score[], int n) {
  int i, j, temp;
  for (i = 0; i < n - 1; i++) {
    for (j = i + 1; j < n; j++) {
      if (score[i] > score[j]) {
        temp = score[i];
        score[i] = score[j];
        score[j] = temp;
      }
    }
  }
}
int main() {
  int score[N], n, i;
  printf("Input n:");
  scanf("%d", &n);
  printf("Input %d numbers:", n);
  ReadScore(score, n);
  printf("Sorting results:");
  DataSort(score, n);
  for (i = 0; i < n; i++) {
    printf("%4d", score[i]);
  }
  return 0;
}

