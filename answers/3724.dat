#include <math.h>
#include <stdio.h>
int main() {
  float a, b, c, s;
  double area;
  printf("Input a,b,c:");
  scanf("%f,%f,%f", &a, &b, &c);
  if (a + b <= c || a + c <= b || b + c <= a)
    printf("Input error!");
  else {
    s = (a + b + c) / 2;
    area = sqrt(s * (s - a) * (s - b) * (s - c));
    printf("area=%.2f", area);
  }
  return 0;
}

