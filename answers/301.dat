#include <stdio.h>
#include <string.h>
void MyStrcat(char *dstStr, char *srcStr) {
  int a, b, i;
  a = strlen(dstStr);
  b = strlen(srcStr);
  for (i = 0; i < b; i++)
    dstStr[a + i] = srcStr[i];
  dstStr[a + b] = 0;
}
void main() {
  char dst[500];
  char src[200];
  printf("Input a string:\n");
  gets(dst);
  printf("Input another string:\n");
  gets(src);
  MyStrcat(dst, src);
  printf("Concatenate results:%s\n", dst);
}

