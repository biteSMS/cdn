main() {
  int w, h, W;
  float H, t;
  printf("Input weight, height:\n");
  scanf("%d,%d", &w, &h);
  H = (float)h / 100;
  W = 2 * w;
  t = w / (H * H);
  printf("weight=%d\n", W);
  printf("height=%.2f\n", H);
  printf("t=%.2f\n", t);
}

