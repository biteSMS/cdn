main() {
  printf("Input weight, height:\n");
  int w, h;
  float H, t;
  scanf("%d,%d", &w, &h);
  H = h / 100.0;
  printf("weight=%d\n", 2 * w);
  printf("height=%.2f\n", H);
  t = w / H / H * 1.0;
  printf("t=%.2f\n", t);
}

