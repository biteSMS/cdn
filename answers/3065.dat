#include <math.h>
#include <stdio.h>
#include <stdlib.h>
int main() {
  int w, h;
  float t, m;
  printf("Input weight, height:\n");
  scanf("%d,%d", &w, &h);
  m = h * 0.01;
  t = (float)w / (m * m);
  printf("weight=%d\n", 2 * w);
  printf("height=%.2f\n", (float)h / 100);
  printf("t=%.2f\n", t);
  return 0;
}

