#include <stdio.h>
#include <stdlib.h>

int main() {
  int i, j, a = 0, n;
  for (i = 1; i <= 5; i++) {
    for (j = 1; j <= 5; j++) {
      for (n = 1; n <= 5; n++) {
        if (i != j && i != n && j != n) {
          a++;
          printf("%d:%d,%d,%d\n", a, i, j, n);
        }
      }
    }
  }
  return 0;
}

