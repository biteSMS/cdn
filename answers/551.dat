#include <stdio.h>
#include <stdlib.h>

int main() {
  int a, b, c;
  int count = 0;

  for (a = 1; a <= 5; a++) // a，b，c，分别从一号书借到五号书
  {
    for (b = 1; b <= 5; b++) {
      for (c = 1; c <= 5; c++) {
        if (0 != (a - b) * (b - c) * (c - a)) {
          count++;
          printf("%d:%d,%d,%d\n", count, a, b, c);
        }
      }
    }
  }
  return 0;
}


