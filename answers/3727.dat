#include <stdio.h>
#include <stdlib.h>
int main() {
  int m, n, x, sum = 0;
  printf("Enter m, n:");
  scanf("%d,%d", &m, &n);
  for (x = m; x <= n; x++) {
    if (x % 3 != 0)
      sum += x;
  }
  printf("The number is %d\n", sum);
}

