#include <stdio.h>
int main() {
  int i, m, n, sum = 0;
  scanf("%d,%d", &m, &n);
  printf("Enter m, n:");
  for (i = m; i <= n; i++) {
    if (i % 3 == 0) {
      continue;
    } else {
      sum = sum + i;
    }
  }
  printf("The number is %d\n", sum);
  return 0;
}

