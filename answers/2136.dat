int main() {
  int a[10];
  int b[10];
  int i;

  for (i = 0; i < 10; i++) {
    scanf("%d", &a[i]);
  }
  b[0] = a[0];
  for (i = 1; i < 10; i++) {
    b[i] = a[i] * a[i - 1];
  }
  for (i = 1; i < 10; i++) {
    printf("%3d", b[i]);
  }

  return 0;
}

