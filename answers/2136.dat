#include <stdio.h>
#define size 10
int main() {
  int a[size], b[size - 1];
  int i;
  for (i = 0; i < size; i++)
    scanf("%d", &a[i]);
  for (i = 0; i < size - 1; i++)
    b[i] = a[i] * a[i + 1];
  for (i = 0; i < size - 1; i++)
    printf("%3d", b[i]);
  return 0;
}

