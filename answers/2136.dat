#include <stdio.h>

int main() {
  int a[10], b[10], i;

  for (i = 0; i < 10; i++) {
    scanf("%d", &a[i]);
  }
  for (i = 1; i < 10; i++) {
    b[i - 1] = a[i - 1] * a[i];
    printf("%3d", b[i - 1]);
  }
  return 0;
}

