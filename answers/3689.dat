#include <stdio.h>
int main() {
  int a[10];
  printf("Please input ten integers:\n");
  int i, sum = 0;
  for (i = 0; i < 10; i++) {
    scanf("%d", &a[i]);
    sum = sum + a[i];
  }
  float average;
  average = (float)sum / 10;
  printf("The mean value is %.2f\n", average);
  return 0;
}

