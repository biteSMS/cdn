#include <stdio.h>
int main() {
  int a[4] = {1, 2, 3, 4};
  int i, j, k;
  for (i = 0; i < 4; i++) {
    for (j = 0; j < 4; j++) {
      if (j == i)
        continue;
      for (k = 0; k < 4; k++) {
        if (k == i || k == j)
          continue;
        printf("%d%d%d\n", a[i], a[j], a[k]);
      }
    }
  }
  return 0;
}

