#include <stdio.h>

int main() {
  int sub1, sub2, sub3;
  int SUM = 0;
  float AVERAGE = 0;
  int REMAINDER = 0;

  printf("Enter three integer: ");
  scanf("%d%d%d", &sub1, &sub2, &sub3);
  SUM = sub1 + sub2 + sub3;
  AVERAGE = SUM / 3.0;
  REMAINDER = SUM % 3;

  printf("SUM = %4d\nAVERAGE = %.2f  REMAINDER = %3d\n", SUM, AVERAGE,
         REMAINDER);
  return 0;
}

