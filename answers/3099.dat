#include <stdio.h>
int main() {
  char a[30];
  gets(a);
  int sum = 0;
  int i = 0;
  int j = 1;
  while (i < 11) {
    if (a[i] != '-') {
      //   printf("%d %d\n", a[i]-'0', j);
      sum += (a[i] - '0') * j;
      j++;
    }
    i++;
  }
  int answer = sum % 11;
  if (answer == 10 && a[12] == 'X')
    printf("Right");
  else if (a[12] - '0' == answer)
    printf("Right");
  else {
    a[12] = answer + '0';
    printf("%s", a);
  }
  return 0;
}

