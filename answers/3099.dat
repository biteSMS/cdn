#include <stdio.h>
#include <string.h>
void main() {
  char a[50], c;
  int i, j = 1, n, nbiao, sum = 0;
  gets(a);
  n = strlen(a);
  c = a[n - 1];
  nbiao = (int)c - 48;
  for (i = 0; i < n - 1; i++) {
    if (a[i] >= '0' && a[i] <= '9') {
      sum = ((int)a[i] - 48) * j + sum;
      j++;
    } else
      continue;
  }
  if (nbiao == sum % 11)
    printf("Right");
  else {
    a[n - 1] = (char)((sum % 11) + 48);
    printf("%s", a);
  }
}

