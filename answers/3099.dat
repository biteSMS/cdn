#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define N 20
int judge(char *p) {
  int sum = 0, j = 1;
  char ch = 'X';
  int len = strlen(p), i;
  for (i = 0; i < len - 1; i++) {
    if (p[i] != '-') {
      sum = sum + (p[i] - '0') * j;
      j++;
    }
  }
  if (sum % 11 == (p[len - 1] - '0'))
    return 1;
  else {
    p[len - 1] = (sum % 11 == 10) ? ch : '0' + sum % 11;
    return 0;
  }
}
int main() {
  char book_number[N];
  gets(book_number);
  if (judge(book_number)) {
    printf("Right\n");
  } else {
    printf("%s", book_number);
  }
  return 0;
}

