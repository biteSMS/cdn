int main() {
  char a[40];
  gets(a);
  char *p = a;
  int sum = 0;
  int i;
  int j = 1;
  for (i = 0; i < 12; i++) {
    if (p[i] != '-') {
      sum += (p[i] - '0') * j;
      // printf("%d %d\n",p[i]-'0',j);
      j++;
    }
  }
  sum = sum % 11;
  char c = sum + '0';
  if (sum == 10) {
    c = 'X';
  }
  if (c == a[12]) {
    printf("Right");
  } else {
    a[12] = c;
    printf("%s", a);
  }
  return 0;
}


