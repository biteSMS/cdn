  ch = getchar();
  Squeeze(str, ch);

  printf("Results:%s\n", str);
  return 0;
}
void Squeeze(char *s, char c) {
  int i, j;
  for (i = 0; *(s + i) != '\0'; i++) {
    if (*(s + i) == c) {
      for (j = i; *(s + j) != '\0'; j++) {
        *(s + j) = *(s + j + 1);
      }
