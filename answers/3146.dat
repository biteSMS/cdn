#include <stdio.h>
#include <stdlib.h>
int main() {
  char l;
  printf("Please enter a character:\n");
  scanf("%c", &l);
  if (l >= '0' && l <= '9')
    printf("It is a number.");
  else if (l == '+' || l == '-' || l == '*' || l == '/')
    printf("It is an operator.");
  else
    printf("It is another character.");
  return 0;
}

