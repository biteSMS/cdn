#include <stdio.h>
void String(char *a, char *b);
int main() {
  char a[20], b[20];
  printf("Enter String:");
  scanf("%s", &a);
  String(a, b);
  printf("a=%s\nb=%s\n", a, b);
  return 0;
}
void String(char *a, char *b) {
  int i;
  for (i = 0; a[i] != '\0'; i++)
    b[i] = a[i];
  b[i] = '\0';
}

