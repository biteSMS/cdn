#include <stdio.h>
#include <string.h>
void fun(char *pa, char *pb) {
  do {
    *pb = *pa;
    pb++;
    pa++;
  } while (*pa != '\0');
  *pb = '\0';
}
int main() {
  const int n = 100;
  char a[n], b[n];
  printf("Enter String:");
  gets(a);
  fun(a, b);
  printf("a=%s\nb=%s\n", a, b);
  return 0;
}

