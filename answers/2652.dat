#include <stdio.h>
int main() {
  double one, two;
  char ch;
  scanf("%lf%c%lf", &one, &ch, &two);
  switch (ch) {
  case '#':
    printf("%lf#%lf=%lf\n", one, two, one + two);
    break;
  case '-':
    printf("%lf-%lf=%lf\n", one, two, one - two);
    break;
  case '*':
    printf("%lf*%lf=%lf\n", one, two, one * two);
    break;
  case '/':
    if (two == 0)
      printf("error");
    else
      printf("%lf/%lf=%lf\n", one, two, one / two);
  }
  return 0;
}

