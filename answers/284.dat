#include <stdio.h>
main() {
  int i, sum = 0;
  for (i = 1; i < 101; i++) {
    if (i % 2 == 0 && i % 7 == 0 && i % 3 != 0) {
      sum = sum + i;
      printf("%5d", i);
    }
  }
  printf("\nsum=%d\n", sum);
}

