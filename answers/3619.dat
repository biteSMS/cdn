int IsPrime(unsigned int x) {
  int i, j, sum = 0, flag;
  i = 2;
  printf("%d\n", i);
  sum = sum + i;

  for (i = 2; i <= x; i++) {
    for (j = 2; j < i; j++) {
      flag = 1;
      if (i % j == 0) {
        flag = 0;
        break;
      }
    }
    if (flag == 1) {
      printf("%d\n", i);
      sum = sum + i;
    }
  }
  printf("sum of prime numbers:%d", sum);
}

