#include <math.h>
#include <stdio.h>
#include <stdlib.h>
int IsPrime(unsigned int x) {
  int i, flag = 1;
  if (x == 2)
    return 1;
  else {
    for (i = 2; i < sqrt(x) + 1; i++) {
      if (x % i == 0) {
        flag = 0;
        break;
      }
    }
    if (flag)
      return 1;
    else
      return 0;
  }
}
int main() {
  int n, i, count = 0, flag;
  printf("Please input n(n>1):");
  scanf("%d", &n);
  for (i = 2; i < n; i++) {
    flag = IsPrime(i);
    if (flag) {
      printf("%d\n", i);
      count += i;
    }
  }
  printf("sum of prime numbers:%d", count);
  return 0;
}

