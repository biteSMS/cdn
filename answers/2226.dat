#include <stdio.h>
int main() {
  int n, i, j, k, sum = 0;
  printf("please input how many terms you want to compute?:\n");
  scanf("%d", &n);
  if (n < 1 || n >= 10)
    printf("Overflow!\n");
  else {
    for (i = 1; i <= n; i++) {
      k = 0;
      for (j = 1; j <= i; j++) {
        k = k * 10 + i;
      }
      sum += k;
    }
    printf("sum=%ld\n", sum);
  }
  return 0;
}


