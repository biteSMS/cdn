#include <stdio.h>
int main() {
  int i, a[10], sum = 0;
  printf("input 10 numbers:");
  for (i = 0; i < 10; i++) {
    scanf("%d", &a[i]);
    if (i % 2 == 0)
      sum += a[i];
  }
  printf("sum=%d\n", sum);
  return 0;
}

