#include <math.h>
#include <stdio.h>
#include <stdlib.h>
int fun(int m);
int main() {
  int n, result;
  for (n = 200; n <= 300; n++) {
    result = fun(n);
    if (result == 1)
      printf("%d\n", n);
  }
  return 0;
}

int fun(int m) {
  int i, k;
  k = (int)sqrt(m);
  for (i = 2; i <= k; i++) {
    if (m % i == 0)
      break;
  }
  if (i > k)
    return 1;
  else
    return 0;
}


