#include <stdio.h>
int IsPrime(int x);
int main(void) {
  int i;
  for (i = 200; i < 300; i++) {
    if (IsPrime(i) == 1)
      printf("%d\n", i);
  }
  return 0;
}
int IsPrime(int x) {
  int i, flag = 1;
  for (i = 2; i < x && flag == 1; i++) {
    if (x % i == 0)
      flag = 0;
  }
  return flag;
}

