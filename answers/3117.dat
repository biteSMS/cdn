}

int zws(int a, int b, int c) {
  int n[3], i, j, temp;
  n[0] = a;
  n[1] = b;
  n[2] = c;
  for (i = 0; i < 2; i++) {
    for (j = i + 1; j < 3; j++) {
      if (n[i] > n[j]) {
        temp = n[i];
        n[i] = n[j];
        n[j] = temp;
      }
    }
  }
  return n[1];
}

