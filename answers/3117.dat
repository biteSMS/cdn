#include <stdio.h>
#include <stdlib.h>
int middle(int a, int b, int c);
int main() {
  int a, b, c, result;
  scanf("%d%d%d", &a, &b, &c);
  result = middle(a, b, c);
  printf("The result is %d\n", result);
  return 0;
}
int middle(int a, int b, int c) {
  if (a < b) {
    if (b < c) {
      return b;
    } else {
      return a < c ? c : a;
    }
  } else {
    if (c > a) {
      return a;
    } else {
      return c > b ? c : b;
    }
  }
}

