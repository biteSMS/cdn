#include <stdio.h>
int mid(int a, int b, int c) {
  if (a > b && a > c) {
    if (b > c) {
      return b;
    } else
      return c;
  } else if (a > b && a < c || a < b && a > c) {
    return a;
  } else if (b > a && b > c) {
    if (a < c) {
      return c;
    } else
      return a;
  }
}
int main() {
  int a, b, c;
  scanf("%d%d%d", &a, &b, &c);

  printf("The result is %d\n", mid(a, b, c));
  return 0;
}

