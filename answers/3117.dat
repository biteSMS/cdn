#include <stdio.h>
int Find(int a, int b, int c);
main() {
  int a, b, c;
  scanf("%d%d%d", &a, &b, &c);
  a = Find(a, b, c);
  printf("The result is %d\n", a);
}
int Find(int a, int b, int c) {
  if (a > b) {
    if (b > c)
      return b;
    if (c > b && c < a)
      return c;
    else
      return a;
  } else {
    if (a > c)
      return a;
    if (c > a & c < b)
      return c;
    else
      return b;
  }
}

