#include <stdlib.h>

int Gcd(int a, int b) {
  int min, flag = 1;

  if (a >= b) {
    min = b;
  } else {
    min = a;
  }

  for (; min >= 2 && flag == 1; min--) {
    if ((a % min) == 0 && (b % min) == 0) {
      flag = 0;
    }
  }

  return min + 1;
}

