#include <stdio.h>
#include <stdlib.h>
int Gcd(int a, int b) {
  int tmp;
  if (a < b) {
    tmp = a;
    a = b;
    b = tmp;
  }
  while (b != 0) {
    tmp = a % b;
    a = b;
    b = tmp;
  }

  return a;
}
int main() {
  int a, b, n;
  printf("Input a,b:\n");
  scanf("%d,%d", &a, &b);
  if (a <= 0 || b <= 0)
    printf("Input number should be positive!\n");
  else {
    n = Gcd(a, b);
    printf("Greatest Common Divisor of %d and %d is %d\n", a, b, n);
  }
  return 0;
}

