#include <stdio.h>
#include <stdlib.h>
int main() {
  float t, w, h;
  printf("Please enter h,w:\n");
  scanf("%f,%f", &h, &w);
  t = w / (h * h);
  if (t < 18)
    printf("Lower weight!\n");
  else if (18 <= t && t < 25)
    printf("Standard weight!\n");
  else if (25 <= t && t < 27)
    printf("Higher weight!\n");
  else
    printf("Too fat!\n");
  return 0;
}

