#include <math.h>
#include <stdio.h>
int JC(n) {
  int i;
  int s = 1;
  for (i = 1; i <= n; i++) {
    s *= i;
  }
  return s;
}
int main() {
  int i;
  for (i = 1; i <= 10; i++) {
    printf("  %2d!=%ld", i, JC(i));
    if (i % 5 == 0)
      printf("\n");
  }
}

