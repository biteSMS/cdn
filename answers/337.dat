#include <stdio.h>
#include <stdlib.h>
long jiecheng(int n);
int main() {
  int i;
  for (i = 1; i < 6; i++) {
    printf("  %2d!=%ld", i, jiecheng(i));
  }
  printf("\n");
  for (i = 6; i < 11; i++) {
    printf("  %2d!=%ld", i, jiecheng(i));
  }

  return 0;
}

long jiecheng(int n) {
  if (n == 1) {
    return n;
  }
  if (n > 1) {
    return (n * jiecheng(n - 1));
  }
}

