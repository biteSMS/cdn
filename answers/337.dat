#include <stdio.h>
int f(int n) {
  if (n == 0)
    return 1;
  else
    return n * f(n - 1);
}
main() {
  int i;
  for (i = 1; i <= 10; i++) {
    printf("  %2d!=%ld", i, f(i));
    if (i % 5 == 0 && i < 10)
      printf("\n");
  }
}

