#include <math.h>
#include <stdio.h>
#include <stdlib.h>

int work(int n) {
  int i, sum = 1;
  for (i = 1; i <= n; i++) {
    sum *= i;
  }
  return sum;
}
int main() {
  int i;
  for (i = 1; i <= 10; i++) {
    printf("  %2d!=%ld", i, work(i));
    if (i == 5)
      printf("\n");
  }
  return 0;
}

